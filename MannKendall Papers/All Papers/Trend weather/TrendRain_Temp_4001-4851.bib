@article{ WOS:000925829100001,
Author = {Seebocus, Reena Hansa and Lollchund, Michel Roddy and Bessafi, Miloud},
Title = {Statistical and trend analyses of long-term rainfall data: a case study
   for Mauritius},
Journal = {INTERNATIONAL JOURNAL OF HYDROLOGY SCIENCE AND TECHNOLOGY},
Year = {2020},
Volume = {10},
Number = {5},
Pages = {413-428},
Abstract = {This paper focuses on the analysis of monthly rainfall data for the
   period 1950-2016 for Mauritius using statistical techniques and
   decomposition method. For the statistical analysis, the data are fitted
   to commonly used probability distribution function for which parameters
   are estimated using the method of maximum likelihood estimation (MLE).
   The Anderson-Darling (A-D), Kolmogorov-Smirnov (K-S) and chi-square
   (C-S) tests are then employed to determine which PDF best fits the data.
   The results obtained indicate that the log-normal, GEV and inverse
   Gaussian PDFs best fit the rainfall data at less than 5\% significance
   level. The ensemble empirical mode decomposition (EEMD) method is
   employed to study the trends in the data. Results obtained are in terms
   of intrinsic mode functions (IMFs) and the trendline. The analysis
   reveals that there is a general linear decrease of 1 mm/year in the
   amount of rainfall.},
