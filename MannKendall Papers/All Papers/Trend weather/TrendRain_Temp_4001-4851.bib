@article{ WOS:000229464400005,
Author = {Pfister, L and Drogue, G and Poirier, C and Hoffmann, L},
Title = {Spatial variability of trends in hydrological extremes induced by
   orographically enhanced rainfall events due to westerly atmospheric
   circulations},
Journal = {WATER SCIENCE AND TECHNOLOGY},
Year = {2005},
Volume = {51},
