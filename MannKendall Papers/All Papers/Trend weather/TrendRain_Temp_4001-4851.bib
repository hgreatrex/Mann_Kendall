Title = {The spatial and temporal structure of rainfall trends in the Valencia
   region (eastern of Spain) over the second half of the 20<SUP>th</SUP>
   century},
Booktitle = {DETECTING AND MODELLING REGIONAL CLIMATE CHANGE},
