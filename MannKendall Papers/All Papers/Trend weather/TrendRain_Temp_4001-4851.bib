@article{ WOS:000696433500001,
Author = {Lana, Xavier and Carmen Casas-Castillo, M. and Rodriguez-Sola, Raul and
   Serra, Carina and Dolors Martinez, M. and Kirchner, Ricard},
Title = {Rainfall regime trends at annual and monthly scales in Catalonia (NE
   Spain) and indications of CO<sub>2</sub> emissions effects},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2021},
Volume = {146},
Number = {3-4},
Pages = {981-996},
Month = {NOV},
Abstract = {The pluviometric regime in the Western Mediterranean and concretely in
   Catalonia (NE Spain) is characterised by irregular amounts at monthly
   and annual scales, sometimes with copious short episodes causing floods
   and, conversely, sometimes with long dry spells exceeding 1 month
   length, depending on the chosen threshold level to define the dry
   episode. Taking advantage of a dense network of rain gauges, most of
   them with records length of 50-60 years and some others exceeding 85
   years, the evolution of these monthly and annual amounts is quantified
   by means of their time trends, statistical significance and several
   irregularity parameters. In agreement with the evolution of the CO2
   emissions into the atmosphere and the increasing concentration, in parts
   per million (ppm), of this greenhouse gas, different time trends at
   annual scale have been detected up to approximately years 1960-1970 in
   comparison with the interval 1960-1970 to nowadays. Consequently,
   besides the greenhouse effects on the temperature regime, the influence
   on the pluviometric regime could not be negligible.},
