@article{ WOS:000229960700008,
Author = {Chernyak, SIM and Rice, CP and Quintal, RT and Begnoche, LJ and Hickey,
   JP and Vinyard, BT},
Title = {Time trends (1983-1999) for organochlorines and polybrominated diphenyl
