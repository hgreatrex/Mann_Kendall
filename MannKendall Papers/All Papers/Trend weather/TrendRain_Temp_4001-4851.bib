@article{ WOS:000682519600059,
Author = {Korkmaz, Meral},
Title = {TREND ANALYSIS OF THE EFFECT OF RAIN ON SOLAR ENERGY PRODUCTION IN A
   SUSTAINABLE FUTURE},
Journal = {FRESENIUS ENVIRONMENTAL BULLETIN},
Year = {2021},
Volume = {30},
Number = {8},
Pages = {10158-10166},
Abstract = {The greatest need of the modern world is energy. From production to
   consumption, energy has become essential in all areas. As fossil-derived
   energy production systems cause global warming and climate change,
   interest in renewable energy sources is increasing. Despite the
   pandemic, solar production has shown promising growth throughout the
   world. Turkey has been encouraging investments in renewable energy
   sources, especially solar energy, for the last 10 years. Feasibility
   studies before the installation of solar power plants are crucial for
   investors. As a result of feasibility studies, the cost of the
   investment and the depreciation period can be calculated. In this study,
   feasibility studies of the 474 kWp power plant in Samsun province were
   carried out with PVsyst program. Significant differences were found
   between the software results and the actual results of the solar plant
   built in Samsun province. Production losses between actual data and
   numerical data are thought to be related to meteorological data. There
   are numerous studies in the literature about PV software. However.
   current studies have not examined the effect of meteorological data of
   the region on the difference in production between PV software results
   and solar power plant production values. In this study, the Sen method
   was used to determine the precipitation trend by taking meteorological
   data from Samsun province. With this method, rainfall in Samsun province
   was seen to be on an upward trajectory. For this reason, in regions
   where there is a lot of precipitation; feasibility studies of solar
   power plants are falling by approximately 9 percent.},
