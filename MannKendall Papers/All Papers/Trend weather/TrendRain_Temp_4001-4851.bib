@inproceedings{ WOS:000249049900008,
Author = {Vivekanandan, N.},
Editor = {Jaiswal, RS and Uma, S},
Title = {Analysis of trend in rainfall using non parametric statistical methods},
Booktitle = {International Symposium on Rainfall Rate and Radio Wave Propagation
   (ISRR `07)},
Series = {AIP CONFERENCE PROCEEDINGS},
Year = {2007},
Volume = {923},
Pages = {101-113},
Note = {International Symposium on Rainfall Rate and Radio Wave Propagation,
   Salem, INDIA, JAN 29-30, 2007},
Organization = {Council Sci \& Ind Res; Indian Space Res Org; Tamilnadu State Council
   Sci \& Technol},
Abstract = {Identification of trend or persistence in the rainfall series is
   essential to present the hydrological information in the condensed form
   for decision-making in water resources planning of any region. This
   paper exemplifies a study involving non-parametric statistical method of
   Mann-Kendall test for identification of existence of trends in annual
   hourly maximum, monthly and annual, and seasonal rainfall series for
   Kanyakumari and Thiruvananthapuram regions. Sen's estimator of slope
   method was used to compute the change per unit time in a time series
   having linear trend. The paper presents the downward trend in the annual
   rainfall series of the regions covered under study. The paper also
   presents the upward trend in winter and monsoon seasons of rainfall for
   Kanyakumari and Thiruvananthapuram regions.},
