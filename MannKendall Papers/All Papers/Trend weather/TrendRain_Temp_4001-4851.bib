@article{ WOS:000578514600001,
Author = {Gan, Ruyu and Hu, Kaiming and Huang, Gang},
Title = {The role of internal variability in multi-decadal trends of summer
   rainfall over East Asia-Northwest Pacific},
Journal = {CLIMATE DYNAMICS},
Year = {2021},
Volume = {56},
