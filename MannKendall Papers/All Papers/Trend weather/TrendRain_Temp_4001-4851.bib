@article{ WOS:000265664800004,
Author = {Larsen, S. H. and Nicholls, N.},
Title = {Southern Australian rainfall and the subtropical ridge: Variations,
   interrelationships, and trends},
Journal = {GEOPHYSICAL RESEARCH LETTERS},
Year = {2009},
Volume = {36},
Month = {APR 29},
Abstract = {Monthly values of the latitude and intensity (mean maximum pressure) of
   the subtropical ridge (STR), averaged across Australian longitudes
   (105-155 degrees E), have been related to variations and trends in
   rainfall over southern Australia (south of 30 degrees S), for 1958-2005.
   There is an abrupt shift of latitude of the subtropical ridge over
   Australia at the onset of winter. In the cooler part of the year the
   intensity of the STR is more closely related to rainfall variations (and
   correlations between latitude and rainfall are a by-product of the
   relationship of intensity with both latitude and rainfall), whereas in
   summer the latitude of the STR is more closely related to rainfall. The
   decline in southern Australian rainfall in recent decades (which has
   occurred principally in autumn) appears related to a trend towards a
   more intense STR, rather than a trend in the latitude of the STR.
   Citation: Larsen, S. H., and N. Nicholls ( 2009), Southern Australian
   rainfall and the subtropical ridge: Variations, interrelationships, and
   trends, Geophys. Res. Lett., 36, L08708, doi: 10.1029/2009GL037786.},
