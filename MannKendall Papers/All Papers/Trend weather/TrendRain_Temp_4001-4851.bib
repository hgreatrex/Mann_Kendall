@article{ WOS:000655883700001,
Author = {Singh, R. N. and Sah, Sonam and Das, Bappa and Potekar, Sunil and
   Chaudhary, Amresh and Pathak, H.},
Title = {Innovative trend analysis of spatio-temporal variations of rainfall in
   India during 1901-2019},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2021},
Volume = {145},
Number = {1-2},
Pages = {821-838},
Month = {JUL},
Abstract = {Spatio-temporal trends of rainfall during the last 119 years (1901 to
   2019) in different meteorological sub-divisions of India were analyzed
   using gridded rainfall data. Innovative trend analysis (ITA) was
   performed for detecting the trends in seasonal and annual rainfall. The
   results of long-term trends and their magnitudes obtained from the ITA
   method were compared with traditional Mann-Kendall (M-K), modified
   Mann-Kendall (mM-K), and linear regression analysis (LRA) methods.
   Significant trends in seasonal and annual rainfall were detected in
   almost all the sub-divisions of India. The monsoon and annual rainfall
   showed increasing trends in most sub-divisions of the peninsular and
   northwest India, whereas decreasing trends were observed in the central
   northeast part of the country. Winter rainfall showed decreasing trends
   in most of the sub-divisions of the country. Results of M-K/mM-K and LRA
   concurred with the trends detected by ITA. However, the ITA is more
   sensitive in detecting hidden trends missed out by the traditional
   M-K/mM-K and LRA tests. This study will provide scientific reference for
   assessment and proactive mitigation of climate change impacts on water
   resources to manage the risk of climate variability.},
