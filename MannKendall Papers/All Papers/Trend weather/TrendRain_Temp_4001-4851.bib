@article{ WOS:000210067400010,
Author = {Romero, Vanessa and Noronha Marcuzzo, Francisco Fernando and Dias
   Cardoso, Murilo Raphael},
Title = {TREND IN THE NUMBER OF DAYS WITH RAIN IN THE STATE OF GOIAS AND ITS
   RELATIONSHIP WITH EXTREME OCEAN NINO INDEX},
