@article{ WOS:000174246000009,
Author = {Vaes, G and Willems, P and Berlamont, J},
Title = {100 years of Belgian rainfall: are there trends?},
Journal = {WATER SCIENCE AND TECHNOLOGY},
Year = {2002},
Volume = {45},
