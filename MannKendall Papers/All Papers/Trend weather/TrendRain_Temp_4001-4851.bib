@article{ WOS:A1985AAV5500009,
Author = {BAYLISS, P},
Title = {THE POPULATION-DYNAMICS OF RED AND WESTERN GREY KANGAROOS IN ARID
   NEW-SOUTH-WALES, AUSTRALIA .1. POPULATION TRENDS AND RAINFALL},
Journal = {JOURNAL OF ANIMAL ECOLOGY},
Year = {1985},
Volume = {54},
Number = {1},
Pages = {111-125},
Abstract = {Red and western gray kangaroo populations were monitored in Kinchega
   National Park and some adjoining sheep stations on a quarterly basis
   between 1973 and 1981. Kangaroos inside the Park were constrained while
   those outside were not. The years 1973-1976 experienced exceptionally
   high rainfall providing kangaroos with a superabundance of food. The
   preceding years were marked by the late 1960''s drought and low kangaroo
   densities, and the following years by average rainfall and high kangaroo
   densities. The kangaroo populations exhibited 2 general trends in
   relation to the exceptional and average rainfall years: a rapid rise and
   an asymptote. The average exponential rates of increase (.hivin.r)
   between 1973 and 1977 were 0.36 per annum {[}p.a.] and 0.27 p.a.,
   respectively, for red and western gray kangaroos inside the Park; and
   0.44 p.a. for both species outside. However, these rates were not
   significantly different. Since 1978 all populations had a zero average
   rate of increase and the high densities were maintained. There was no
   significant difference between the population dynamics of the kangaroos
   inside the park and on the adjoining sheep stations, regardless of
   densities being 2.5 times higher on the park. Competition for food by
   sheep may have suppressed kangaroo densities outside the park. The
   exponential rate of increase (r p.a.) of all populations was positively
   correlated with lagged rainfall. Rainfall and rate of increase of both
   species inside the park  exhibited regular trends on time. The
   regularity was strong for the western gray kangaroo but weak for the red
   kangaroo.},
