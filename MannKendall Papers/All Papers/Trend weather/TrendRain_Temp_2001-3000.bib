@article{ WOS:000313926400042,
Author = {Tahas, S. -V. and Dimen, L. and Ristoiu, D. and Cosma, C. and Muntean,
   L.},
Title = {STATISTICAL ANALYSIS OF GLOBAL SOLAR RADIATION AND TEMPERATURE TRENDS AT
   CLUJ-NAPOCA, ROMANIA (1921-2009)},
Journal = {JOURNAL OF ENVIRONMENTAL PROTECTION AND ECOLOGY},
Year = {2012},
Volume = {13},
Number = {4},
Pages = {2404-2415},
Abstract = {Different statistical procedures like t-test, regression models, the
   Runs test and cumulative curve of the radiation and precipitation
   standardised anomaly were used to find the most appropriate expression
   of the annual mean global solar radiation (GSR) and temperature
   interannual evolution for the period 1921 to 2009. In order to sustain
   the results from Cluj-Napoca, we also used data of 2 neighbouring
   stations, namely Bistrita and Sibiu. The parametric analysis with
   regression models shows significant increasing trends, the quadratic
   trend model providing a better fit than the linear and exponential
   models. According to the quadratic model, the GSR increased at all 3
   stations, while the temperature increased at Cluj-Napoca and Bistria and
   decreased at Sibiu. It is the most probably that this increasing in GSR
   and increasing/decreasing of temperature is rather the ascensional or
   descensional parts of some natural micro-oscillations due to the natural
   climate variability or solar variations.},
