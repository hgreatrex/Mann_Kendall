@article{ WOS:000255063900010,
Author = {Singh, Pratap and Kumar, Vijay and Thomas, T. and Arora, Manohar},
Title = {Basin-wide assessment of temperature trends in northwest and central
   India},
Journal = {HYDROLOGICAL SCIENCES JOURNAL},
Year = {2008},
Volume = {53},
Number = {2},
Pages = {421-433},
Month = {APR},
Abstract = {Estimates of trends of climatic changes at basin and state scales are
   required for developing adaptation strategies related to planning,
   development and management of water resources. In the present study,
   seasonal and annual trends of changes in maximum temperature (T-max),
   minimum temperature (T-min), mean temperature (T-mean), temperature
   range (T-range), highest maximum temperature (H-max) and lowest minimum
   temperature (L-min) have been examined at the basin scale. The longest
   available records over the last century, for 43 stations covering nine
   river basins in northwest and central India, were used in the analysis.
   Of the nine river basins studied, seven showed a warming trend, whereas
   two showed a cooling trend. The Narmada and Sabarmati river basins
   experienced the maximum warming and cooling, respectively. The majority
   of basins in the study area show increasing trend in T-range, H-max and
   L-min. Seasonal analysis of different variables shows that the greatest
   changes in T-max and T-mean were observed in the post-monsoon season,
   while T-min experienced the greatest change in the monsoon season. This
   analysis provides scenarios of temperature changes which may be used for
   sensitivity analysis of water availability for different basins, and
   accordingly in planning and implementation of adaptation strategies.},
