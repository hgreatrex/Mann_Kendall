@article{ WOS:001079103700001,
Author = {Mokarram, Marzieh and Taripanah, Farideh and Pham, Tam Minh},
Title = {Investigating the effect of surface urban heat island on the trend of
   temperature changes},
Journal = {ADVANCES IN SPACE RESEARCH},
