@inproceedings{ WOS:000225602000022,
Author = {Ho, CR and Kuo, NJ and Lin, CY},
Editor = {Bostater, CR and Santoleri, R},
Title = {Sea surface temperature trends in the waters adjacent to Taiwan from
   1982 to 2003},
Booktitle = {REMOTE SENSING OF THE OCEAN AND SEA ICE 2004},
