@article{ WOS:A1989AG18500002,
Author = {WEIR, B and DISNEY, L and GRACE, M and ROBERTS, P},
Title = {DAILY TRENDS IN WHITE BLOOD-CELL COUNT AND TEMPERATURE AFTER
   SUBARACHNOID HEMORRHAGE FROM ANEURYSM},
Journal = {NEUROSURGERY},
Year = {1989},
Volume = {25},
Number = {2},
Pages = {161-165},
Month = {AUG},
Abstract = {A retrospective analysis of the cases of 173 patients operated on for
   aneurysms and admitted to a neurosurgical service early after
   subarachnoid hemorrhage was conducted with respect to white blood cell
   (WBC) count and highest daily temperature. Daily trends for the
   development of clinically vasospasm (VSP) as well as mortality during
   the hospitilization were analyzed. An admission WBC count greater than
   15 .times. 109/l was associated with 55\% mortality as opposed to 25\%
   mortality for those with a lower WBC count. The mortality of those with
   a temperature greater than 37.5.degree.C on Day 0 was 60\%, compared
   with 35\% for those with a lower temperature. A WBC count greater than
   15 .times. 109/l on Day 0 was associated with a VSP rate of 40\%, while
   patients with lower temperature had a VSP rate of 30\%. By Day 6, the
   patients with temperatures > 37.5.degree.C had a VSP rate of 60\%,
   double that of the VSP rate of those with temperatures < 37.degree.C.
   WBC count was apparently more closely linked to the chance  of dying
   than the chance of developing VSP. The development of fever after a few
   days is related to both increased mortality and increased chance of
   developing VSP.},
