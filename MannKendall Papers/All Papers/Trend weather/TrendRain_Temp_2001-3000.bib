@article{ WOS:000914124000001,
Author = {Sage, Adrianna M. and Bossert, Julia L. and Brunstein, Kristin A. and
   Farrow, Abigail F. and Perregrino, Ali N. and Clark-Price, Stuart C.},
Title = {Retrospective investigation of decreasing body temperature trends during
   anaesthesia in dogs},
Journal = {VETERINARY RECORD},
Year = {2023},
Volume = {193},
Number = {4},
Month = {AUG 19},
Abstract = {BackgroundHypothermia during the perianaesthetic period may lead to an
   increased risk of morbidity in veterinary patients. However, the
   timeline of the decrease in body temperature during general anaesthesia
   has been minimally investigated. MethodsAnaesthetic records of 1097
   client-owned dogs were examined. Change in body temperature (Delta BT =
   baseline temperature - time point temperature) was plotted over time for
   all dogs. The slope of Delta BT was calculated for each 15-minute
   interval, and the magnitude of the largest Delta BT and the time point
   at which the largest Delta BT occurred were determined for each record.
   ResultsA rapid decline in Delta BT occurred from 0 to 15 minutes, a
   slower decline occurred from 15 to 60 minutes and a plateau occurred
   from 60 to 240 minutes. The largest Delta BT occurred at 75 (15-240)
   minutes from baseline, with a maximum Delta BT of -2.06 degrees C (-0.06
   degrees C to -8.72 degrees C). LimitationsThis is a retrospective study.
   As such, there were missing data points and potential confounding
   factors could not be controlled for. ConclusionsAnaesthetised dogs
   exhibited a distinct pattern of decrease in body temperature, with the
   most rapid reduction occurring within the first 15 minutes. The effects
   of procedures and anaesthetic agents on the timeline and pattern of heat
   loss warrant further investigation.},
