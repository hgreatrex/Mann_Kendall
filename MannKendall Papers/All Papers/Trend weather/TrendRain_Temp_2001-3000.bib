@article{ WOS:000082374600008,
Author = {Angell, JK},
Title = {Variation with height and latitude of radiosonde temperature trends in
   North America, 1975-94},
Journal = {JOURNAL OF CLIMATE},
Year = {1999},
Volume = {12},
Number = {8, 2},
Pages = {2551-2561},
Month = {AUG},
Abstract = {Based on the 120-station North American radiosonde network, temperature
   trends for 100-50-mb (low stratosphere), 300-100-mb (tropopause), and
   850-300-mb (troposphere) layers, and Earth's surface, are evaluated for
   six 10 degrees latitude bands extending from 20 degrees-30 degrees N to
   70 degrees-80 degrees N for the 20-yr interval 1975-94. Confidence
   estimates are indicated by two standard errors of the least squares
   regression. In the average for the six latitude bands, the 100-50-mb
   annual temperature trend is -0.5 degrees C decade(-1) and the 850-300-mb
   trend is 0.2 degrees C decade(-1). In spring at 70 degrees-80 degrees N,
   the 100-50-mb and 300-100-mb layers cool by almost 2 degrees C
   decade(-1). The 300-100-mb layer cools by 0.7 degrees C decade(-1)
   relative to the 850-300-mb layer at 70 degrees-80 degrees N, but the two
   layers have the same warming trend at 20 degrees-30 degrees N,
   indicating the transition from the 300-100-mb layer being mostly in the
   stratosphere in polar regions to mostly in the troposphere in the
   northern subtropics. The surface warms much more than the troposphere at
   70 degrees-80 degrees N (showing that surface temperature trends are not
   representative of tropospheric trends in polar regions) and slightly
   more at 20 degrees-30 degrees N, bur surface warming is less than
   tropospheric warming in the 40 degrees-70 degrees N belt. Ar the surface
   at the radiosonde sites the 1200 UTC (morning) temperature cools
   relative to the 0000 UTC (evening) temperature by 0.05 degrees C per
   decade on average, but the 850-300-mb temperature trends at 0000 UTC and
   1200 UTC are essentially the same. The 0.7 degrees C decade(-1) cooling
   of low stratosphere relative to troposphere increases to 0.9 degrees C
   decade(-1) when adjustment is made for the stratospheric warming and
   tropospheric cooling following El Chichon and Pinatubo eruptions. The
   temperature trends obtained from 11 North American radiosonde stations
   in a 63-station global network agree well with the trends based on the
   entire 120-station network, and the latter are fairly representative of
   zonally averaged trends based on the 63-station network and microwave
   sounding unit data, Comparison with Canadian ozonesonde data shows that.
   in the low stratosphere and high troposphere during 1975-94, a decrease
   in temperature of 1 degrees C decade(-1) was associated with a decrease
   in ozone of about 10\% decade(-1).},
