@article{ WOS:A1987G250500009,
Author = {KHALIL, MAK and RASMUSSEN, RA},
Title = {THE RESIDENCE TIME OF TRICHLOROFLUOROMETHANE IN POLYURETHANE FOAMS -
   VARIABILITY, TRENDS AND EFFECTS OF AMBIENT-TEMPERATURE},
Journal = {CHEMOSPHERE},
Year = {1987},
Volume = {16},
Number = {4},
Pages = {759-775},
Abstract = {Closed cell polyurethane foams now constitute the major use of F-11
   which is a relatively recent occurrence. The residence time of F-11 in
   various types of polyurethane foams is extremely long, probably more
   than a hundred years. Because of the long residence time, polyurethane
   foams constitute a significant and growing reservoir of F-11 from which
   the fluorocarbon will leak out into the atmosphere at increasing rates
   for a very long time. The temporal behavior of the emission rate of F-11
   is  characterized by slow, relatively constant emission rates once the
   effects of cutting the foam have subsided. Bruising or breaking the
   polyurethane foams can release large amounts of F-11. There is
   considerable variability in the averge emission rates between foams made
   by different manufacturers (.apprx. 400\%) but very little variability
   in average emission rates from samples of the same larger piece of foam
   (.ltoreq. 20\%). Consequently the residence time of F-11 is different
   for foams from different  manufacturers, but in all cases the residence
   time is very long. Emission rates increase rapidly with increasing
   temperature at an average rate of about 5\% per degree F in the
   temperature range between 70.degree. F and 80.degree. F. In actural
   applications the temperature may be an important determinant of the
   residence time of F-11 in polyurethane foams.},
