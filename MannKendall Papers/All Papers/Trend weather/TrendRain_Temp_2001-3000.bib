@article{ WOS:000446601200056,
Author = {Chen, Ning and Li, Binyan and Gui, Weihua and Xu, Shujian and Wang,
   Jiexi and Dai, Jiayang},
Title = {Research on Temperature Field Change Trend of the Sintering Process for
   Lithium-ion Battery Cathode Materials},
Journal = {IFAC PAPERSONLINE},
