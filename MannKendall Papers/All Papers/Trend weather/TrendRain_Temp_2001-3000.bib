@article{ WOS:000182219500006,
Author = {Rimbu, N and Lohmann, G and Kim, JH and Arz, HW and Schneider, R},
Title = {Arctic/North Atlantic Oscillation signature in Holocene sea surface
   temperature trends as obtained from alkenone data},
Journal = {GEOPHYSICAL RESEARCH LETTERS},
Year = {2003},
Volume = {30},
Number = {6},
Month = {MAR 19},
Abstract = {{[}1] The variability in alkenone-derived sea surface temperatures
   (SSTs) in the North Atlantic realm shows that a continuous SST decrease
   in the northeast Atlantic from the early to the late Holocene was
   accompanied by a persistent warming over the western subtropical
   Atlantic, the eastern Mediterranean Sea and the northern Red Sea. Based
   on the analysis of the instrumental data and of atmospheric general
   circulation model experiments, we show that this variation in SSTs
   during the Holocene can be attributed to a continuous weakening of a
   Northern Hemisphere atmospheric circulation pattern similar to that of
   the Arctic/North Atlantic Oscillation.},
