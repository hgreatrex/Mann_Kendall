@article{ WOS:000304696300014,
Author = {Shu, Qi and Qiao, Fangli and Song, Zhenya and Wang, Chunzai},
Title = {Sea ice trends in the Antarctic and their relationship to surface air
   temperature during 1979-2009},
Journal = {CLIMATE DYNAMICS},
