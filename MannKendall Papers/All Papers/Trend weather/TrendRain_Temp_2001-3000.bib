@article{ WOS:000379417300023,
Author = {Holmen, Silje Eriksen and Hall, Chris M. and Tsutsumi, Masaki},
Title = {Neutral atmosphere temperature trends and variability at 90aEuro-km, 70
   A°N, 19 A°E, 2003-2014},
Journal = {ATMOSPHERIC CHEMISTRY AND PHYSICS},
Year = {2016},
Volume = {16},
Number = {12},
Pages = {7853-7866},
Abstract = {Neutral temperatures at 90aEuro-km height above Tromso, Norway, have
   been determined using ambipolar diffusion coefficients calculated from
   meteor echo fading times using the Nippon/Norway Tromso Meteor Radar
   (NTMR). Daily temperature averages have been calculated from November
   2003 to October 2014 and calibrated against temperature measurements
   from the Microwave Limb Sounder (MLS) on board Aura. Large-scale
   periodic oscillations ranging from similar to aEuro-9 days to a year
   were found in the data using Lomb-Scargle periodogram analysis, and
   these components were used to seasonally detrend the daily temperature
   values before assessing trends. Harmonic oscillations found are
   associated with the large-scale circulation in the middle atmosphere
   together with planetary and gravity wave activity. The overall
   temperature change from 2003 to 2014 is -2.2aEuro-KaEuro-+/-
   aEuro-1.0aEuro-KaEuro-decade(-1), while in summer (May-June-July) and
   winter (November-December-January) the change is -0.3 KaEuro-+/-
   aEuro-3.1aEuro-KaEuro-decade(-1) and -11.6aEuro-KaEuro-+/-
   aEuro-4.1aEuro-KaEuro-decade(-1), respectively. The temperature record
   is at this point too short for incorporating a response to solar
   variability in the trend. How well suited a meteor radar is for
   estimating neutral temperatures at 90aEuro-km using meteor trail echoes
   is discussed, and physical explanations behind a cooling trend are
   proposed.},
