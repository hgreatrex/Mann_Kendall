@article{ WOS:000502929200001,
Author = {Gong, Hainan and Wang, Lin and Chen, Wen and Wu, Renguang},
Title = {Time-Varying Contribution of Internal Dynamics to Wintertime Land
   Temperature Trends Over the Northern Hemisphere},
Journal = {GEOPHYSICAL RESEARCH LETTERS},
