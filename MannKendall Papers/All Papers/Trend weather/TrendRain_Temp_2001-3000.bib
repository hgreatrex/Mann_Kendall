@article{ WOS:000615500300001,
Author = {Barmpakos, Dimitris and Kaltsas, Grigoris},
Title = {A Review on Humidity, Temperature and Strain Printed Sensors-Current
   Trends and Future Perspectives},
Journal = {SENSORS},
Year = {2021},
Volume = {21},
Number = {3},
Month = {FEB},
Abstract = {Printing technologies have been attracting increasing interest in the
   manufacture of electronic devices and sensors. They offer a unique set
   of advantages such as additive material deposition and low to no
   material waste, digitally-controlled design and printing, elimination of
   multiple steps for device manufacturing, wide material compatibility and
   large scale production to name but a few. Some of the most popular and
   interesting sensors are relative humidity, temperature and strain
   sensors. In that regard, this review analyzes the utilization and
   involvement of printing technologies for full or partial sensor
   manufacturing; production methods, material selection, sensing
   mechanisms and performance comparison are presented for each category,
   while grouping of sensor sub-categories is performed in all applicable
   cases. A key aim of this review is to provide a reference for sensor
   designers regarding all the aforementioned parameters, by highlighting
   strengths and weaknesses for different approaches in printed humidity,
   temperature and strain sensor manufacturing with printing technologies.},
