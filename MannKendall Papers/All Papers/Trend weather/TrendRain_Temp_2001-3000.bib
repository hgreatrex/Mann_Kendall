@article{ WOS:000248887600004,
Author = {Lockwood, Mike and Froehlich, Claus},
Title = {Recent oppositely directed trends in solar climate forcings and the
   global mean surface air temperature},
Journal = {PROCEEDINGS OF THE ROYAL SOCIETY A-MATHEMATICAL PHYSICAL AND ENGINEERING
   SCIENCES},
Year = {2007},
Volume = {463},
Number = {2086},
Pages = {2447-2460},
Month = {OCT 8},
Abstract = {There is considerable evidence for solar influence on the Earth's
   pre-industrial climate and the Sun may well have been a factor in
   post-industrial climate change in the first half of the last century.
   Here we show that over the past 20 years, all the trends in the Sun that
   could have had an influence on the Earth's climate have been in the
   opposite direction to that required to explain the observed rise in
   global mean temperatures.},
