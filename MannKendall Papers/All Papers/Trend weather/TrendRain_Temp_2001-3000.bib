@article{ WOS:000358762400009,
Author = {Quinton, Page C. and Percival, Ian G. and Zhen, Yong-Yi and Macleod,
   Kenneth G.},
Title = {Ordovician temperature trends: constraints from δ<SUP>18</SUP>O analysis
   of conodonts from New South Wales, Australia},
Journal = {STRATIGRAPHY},
Year = {2015},
Volume = {12},
Number = {3-4, S},
Pages = {62-67},
Abstract = {The argument that temperature change and biological change during the
   Ordovician are correlated and, perhaps, causally related has been
   advanced by measurements of delta O-18(phos) values on conodont apatite,
   a phase more resistant to diagenetic alteration than carbonates.
   However, the available conodont delta O-18(phos) records are
   discontinuous and are biased towards North American samples. To test the
   generality of global patterns and to expand the geographical range of
   studied regions, we document delta O-18(phos) values from conodont
   apatite as well as delta C-13 and delta O-18(calcite) values from bulk
   carbonate from New South Wales, Australia. New results span most of the
   Ordovician and include the first Late Ordovician phosphate delta O-18
   values from the Australian continent. The data from New South Wales show
   an similar to 1. 5 parts per thousand(VSMOW) increase in delta
   O-18(phos) values during the Early and Middle Ordovician. This pattern
   matches previously documented trends from Laurentia and central
   Australia, but values in New South Wales are consistently similar to 2.
   5 parts per thousand(VSMOW) lower than those from other regions. We
   attribute these low delta O-18(phos) values to local paleoceanographic
   effects on the seawater delta O-18 value.},
