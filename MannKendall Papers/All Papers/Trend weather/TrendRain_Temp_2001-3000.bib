@article{ WOS:000215961100010,
Author = {Beigt, Debora and Cintia Piccolo, M.},
Title = {Temperature trends in a tide flat at Bahia Blanca estuary (2000-2007)},
Journal = {INVESTIGACIONES GEOGRAFICAS-SPAIN},
Year = {2009},
Number = {48},
Pages = {253-271},
Abstract = {Temperature trends in a tidal flat of the Bahia Blanca estuary
   (Argentina) were analyzed during the period 2000 - 2007. Air, water and
   sediment temperatures were recorded every 10 minutes by a thermistor
   chain. Minimum temperatures showed an increasing trend, reaching 0,8
   degrees C/year (absolute minimum water temperature in winter) and 0,7
   degrees C/year (average minimum air temperature in spring). Average
   maximum summer temperatures at the air and sediment showed a negative
   trend of - 0.5C/year. Spectral analysis of the estuarine temperatures
   evidenced an interannual periodicity (2 - 6 year) at the air, water and
   sediment, showing a variability pattern associated to the typical
   periodicities of the ENSO (2 - 7 years). Correlation analysis among
   thermal anomalies and SOI suggested that air, water and sediment
   temperatures in the estuary are substantially influenced by remote
   forcing. The occurrence of El Nino events was coincident with higher
   than average air and water temperatures and vice versa during La Nina
   events.},
