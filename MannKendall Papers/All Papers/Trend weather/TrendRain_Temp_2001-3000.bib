@article{ WOS:000403819400002,
Author = {Urban, Grzegorz and Tomczynski, Karol},
Title = {AIR TEMPERATURE TRENDS AT MOUNT SNIEZKA (POLISH SUDETES) AND SOLAR
   ACTIVITY, 1881-2012},
Journal = {ACTA GEOGRAPHICA SLOVENICA-GEOGRAFSKI ZBORNIK},
Year = {2017},
Volume = {57},
Number = {2},
Pages = {33-44},
Abstract = {This article discusses air temperature variability at Mount Sniezka in
   the Sudetes from 1881 to 2012. It analyzes the relationship between
   changing trends in mean annual air temperature (T-avg) and solar
   activity, expressed by the mean annual Wolf number. The characteristic
   feature of changes in annual mean extremes (T-max, T-min) and T-avg at
   Mount Sniezka is an upward trend. The increase of T-min (0.148 degrees
   C/10 years) has been twice as fast as that for T-max (0.069 degrees C/10
   years). A strong correlation (almost 1.0) was found between the mean
   annual Wolf number for twenty-two-year cycles of magnetic changes in the
   Sun and 1988. During the 1989-2012 cycle, there was a strong increase in
   T-avg and, at the same time, a decrease in the mean annual Wolf number.},
