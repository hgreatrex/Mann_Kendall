Keywords = {temperature trends; stratopause; satellite; radiative forcing},
Keywords-Plus = {WATER-VAPOR; SOLAR-CYCLE; LOWER THERMOSPHERE; OZONE; STRATOSPHERE;
   VARIABILITY; MESOSPHERE; SIMULATION; SATELLITE; TRANSPORT},
Research-Areas = {Meteorology \& Atmospheric Sciences},
