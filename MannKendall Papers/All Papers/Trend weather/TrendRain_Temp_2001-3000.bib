@article{ WOS:000295371500017,
Author = {Mallik, Chinmay and Lal, Shyam},
Title = {Changing long-term trends in tropospheric temperature over two
   megacities in the Indo-Gangetic Plain},
Journal = {CURRENT SCIENCE},
Year = {2011},
Volume = {101},
Number = {5},
Pages = {637-644},
Month = {SEP 10},
Abstract = {The radiosonde data over two megacities in the Indo-Gangetic Plain, viz.
   Delhi and Kolkata are examined to study the long-term trends in air
   temperature at various pressure levels between 850 and 150 hPa during
   1973-2008. The maximum increase in temperature occurs at 700 hPa over
   both the stations during this period. The warming trends are seen until
   400 hPa, whereas cooling trends are found to occur above this altitude.
   Overall, the annual long-term trends vary between +0.3 and -0.2 degrees
   C/decade for the altitude range studied. Long-term trends in different
   seasons reveal highest warming during winter until 300 hPa over Delhi.
   Over Kolkata, the highest warming occurs during monsoon at heights above
   700 hPa. These trends are found to be the lowest during post-monsoon.
   The important point that has emerged from this study is that there has
   been an accelerated rise in temperature during 1973-2008 when compared
   with a previous study which reported negative trends during an earlier
   period, i.e. 1958-1985.},
