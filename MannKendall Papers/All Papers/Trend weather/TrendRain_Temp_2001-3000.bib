@article{ WOS:000254970800004,
Author = {Sun Xian and Lin Zhenshan and Cheng Xiaoxia and Jiang Chuangye},
Title = {Regional features of the temperature trend in China based on Empirical
   Mode Decomposition},
Journal = {JOURNAL OF GEOGRAPHICAL SCIENCES},
Year = {2008},
Volume = {18},
Number = {2},
Pages = {166-176},
Month = {MAY},
Abstract = {By the Empirical Mode Decomposition method, we analyzed the observed
   monthly average temperature in more than 700 stations from 1951-2001
   over China. Simultaneously, the temperature variability of each station
   is calculated by this method, and classification chart of long term
   trend and temperature variability distributing chart of China are
   obtained, supported by GIS, 1 kmx1 km resolution. The results show that:
   in recent 50 years, the temperature has increased by more than 0.4
   degrees C/10a in most parts of northern China, while in Southwest China
   and the middle and lower Yangtze Valley, the increase is not
   significant. The areas with a negative temperature change rate are
   distributed sporadically in Southwest China. Meanwhile, the temperature
   data from 1881 to 2001 in nine study regions in China are also analyzed,
   indicating that in the past 100 years, the temperature has been
   increasing all the way in Northeast China, North China, South China,
   Northwest China and Xinjiang and declining in Southwest China. An
   inverse `V-shaped' trend is also found in Central China. But in Tibet
   the change is less significant.},
