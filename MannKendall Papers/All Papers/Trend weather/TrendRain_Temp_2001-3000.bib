@article{ WOS:A1996VC20900013,
Author = {DeGaetano, AT},
Title = {Recent trends in maximum and minimum temperature threshold exeedences in
   the northeastern United States},
Journal = {JOURNAL OF CLIMATE},
Year = {1996},
Volume = {9},
Number = {7},
Pages = {1646-1660},
Month = {JUL},
Abstract = {Changes in the annual number of daily maximum and minimum temperature
   threshold exceedences between 1951 and 1993 are assessed at a network of
   22 primarily rural sites in the northeastern United States. After
   adjusting the annual time series for changes in observation time and
   eliminating stations at which nonhomogeneous time series were detected,
   annual rime series of the number of days with maximum temperatures
   greater than or equal to 35.0 degrees, 32.2 degrees, and 29.4 degrees C
   (95 degrees, 90 degrees, and 85 degrees F); maximum temperatures less
   than or equal to 0.0 degrees -6.7 degrees, and -12.2 degrees C (32
   degrees, 20 degrees, and 10 degrees F): minimum temperatures greater
   than or equal to 23.8 degrees, 21.1 degrees, and 18.3 degrees C (75
   degrees, 70 degrees, and 65 degrees F); and minimum temperatures less
   than or equal to -12.2 degrees, -15.0 degrees, and -17.8 degrees C (10
   degrees, 5 degrees, and 0 degrees F) are statistically analyzed.
   Overall, a statistically significant trend toward fewer cold minimum
   temperature threshold exceedences is detected across the region over the
   period from 1959 to 1993. Al stations show a decreasing trend in days
   with minimum temperatures less than or equal to -15.0 degrees C over
   this time period, with significant (alpha = 0.1.) trends present at
   nearly 50\% of the sites. Similarly, significant increases in the
   exceedence of warm minimum temperature thresholds are evident at more
   stations than can be expected by chance. A significant number of trends
   toward fewer warm maximum temperature threshold exceedences is also
   detected over the 1951-1993 time period. However. these trends are most
   likely due to changes in the attributes of the individual stations. Only
   three northern stations show significant increases in cold maximum
   temperature threshold exceedences.},
