@article{ WOS:000256818400008,
Author = {Huang, Yilong and Wang, Yanglin and Zhang, Liping},
Title = {Long-term trend of chemical composition of wet atmospheric precipitation
   during 1986-2006 at Shenzhen City, China},
Journal = {ATMOSPHERIC ENVIRONMENT},
Year = {2008},
Volume = {42},
Number = {16},
Pages = {3740-3750},
Month = {MAY},
Abstract = {In order to understand the long-term trend of chemical composition of
   wet atmospheric precipitation, the precipitation samples were collected
   for the time period from 1986 to 2006 at Shenzhen City, China. All the
   rainwater samples were analyzed for pH, conductivity, Cl-, F-, SO42-,
   NO3-, NH4+, Na+, K+, Ca2+ and Mg2+ concentration. Average pH during
   1986-2006 was 5.02, which is considerably higher compared to those
   reported in many other cities of the typical acid precipitation regions
