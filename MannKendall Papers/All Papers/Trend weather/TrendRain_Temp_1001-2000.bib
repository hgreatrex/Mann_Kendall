@article{ WOS:000502089700007,
Author = {Worku, Tesfa and Khare, Deepak and Tripathi, S. K.},
Title = {Spatiotemporal trend analysis of rainfall and temperature, and its
   implications for crop production},
Journal = {JOURNAL OF WATER AND CLIMATE CHANGE},
Year = {2019},
Volume = {10},
Number = {4},
Pages = {799-817},
Month = {DEC},
Abstract = {Global warming is a significant global environmental problem in the 21st
   century. The problem is high in developing countries, particularly
   sub-Saharan countries in which the majority of the population live on
   rainfed agriculture. The present study aimed to undertake spatiotemporal
   analysis of seasonal and annual rainfall and temperature and its
   implications. The MK test, Sen's slope and precipitation concentration
   index (PCI) were applied. Finally, Pearson correlation analysis between
   climatic variables and crop production was analysed. The Mann-Kendall
   test results showed that the annual and seasonal rainfall trend was
   highly variable. The minimum and maximum temperatures have increased by
   0.8 and 1.1 degrees C/year, respectively. Based on PCI results, rainfall
   during the summer and spring seasons is moderately distributed as
   compared to annual and winter season rainfall. Based on these
   observations, the rainfall pattern and distribution of the area could be
   classified as irregular and erratic distribution. Results of correlation
   analysis between monthly and seasonal rainfall with crop production were
   insufficient to conclude the impact of rainfall and temperature on crop
   production. In view of this, the incidence of food shortage is a common
   occurrence. Therefore, depending on the historical trend of rainfall
   variability and prolonged temperature increase, appropriate coping and
   adaptation strategies need to be encouraged.},
