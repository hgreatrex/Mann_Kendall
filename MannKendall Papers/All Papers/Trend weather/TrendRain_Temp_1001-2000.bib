@article{ WOS:000232394800003,
Author = {Kothawale, DR and Kumar, KR},
Title = {On the recent changes in surface temperature trends over India},
Journal = {GEOPHYSICAL RESEARCH LETTERS},
Year = {2005},
Volume = {32},
Number = {18},
Month = {SEP 28},
Abstract = {Marked differences from global trends in terms of diurnal asymmetry of
   temperature trends were reported earlier for India, indicating that the
   warming over India was solely contributed by maximum temperatures. We
   report substantial recent changes in the nature of trends, using updated
   data sets up to 2003, with special focus on the last three decades.
   While all-India mean annual temperature has shown significant warming
   trend of 0.05 degrees C/10yr during the period 1901-2003, the recent
   period 1971-2003 has seen a relatively accelerated warming of 0.22
   degrees C/10yr, which is largely due to unprecedented warming during the
   last decade. Further, in a major shift, the recent period is marked by
   rising temperatures during the monsoon season, resulting in a weakened
   seasonal asymmetry of temperature trends reported earlier. The recent
   accelerated warming over India is manifest equally in daytime and
   nighttime temperatures.},
