@article{ WOS:000416197800019,
Author = {Popov, Tatjana and Gnjato, Slobodan and Trbic, Goran and Ivanisevic,
   Marko},
Title = {RECENT TRENDS IN EXTREME TEMPERATURE INDICES IN BOSNIA AND HERZEGOVINA},
Journal = {CARPATHIAN JOURNAL OF EARTH AND ENVIRONMENTAL SCIENCES},
