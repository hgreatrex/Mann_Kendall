@article{ WOS:000501449200001,
Author = {Me-ead, Cherdchai and McNeil, Rhysa},
Title = {Pattern and Trend of Night Land Surface Temperature in Africa},
Journal = {SCIENTIFIC REPORTS},
Year = {2019},
Volume = {9},
Month = {DEC 4},
Abstract = {This study aims to identify patterns and trends of the night land
   surface temperature over eight day period from 2000 to 2014 in Africa
   using statistical analysis. Data were obtained from the United States
   National Aeronautics and Space Administration satellite, comprising 99
   locations of 5 degrees by 5 degrees latitude and longitude grid-boxes
   between latitudes 35 degrees north and south of the equator and
   longitudes 20 degrees west to 50 degrees east. First, the variation in
   the night surface temperatures was removed. Then, the trend of
   seasonally adjusted night temperatures was estimated using linear
   regression. The correlations between adjoining regions were considered
   by using factor analysis to classify the temperatures into four regions.
   Cubic spline models were fitted to the data within these regions to
   investigate patterns of the temperatures. The result showed that
   temperatures in most regions of Africa increased. The temperatures
   decreased was observed in southern Africa and parts of central and
   eastern Africa.},
