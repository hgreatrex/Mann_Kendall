@article{ WOS:000321261600059,
Author = {Mitchell, D. M. and Thorne, P. W. and Stott, P. A. and Gray, L. J.},
Title = {Revisiting the controversial issue of tropical tropospheric temperature
   trends},
Journal = {GEOPHYSICAL RESEARCH LETTERS},
