@article{ WOS:000361731600012,
Author = {Mondal, Arun and Khare, Deepak and Kundu, Sananda},
Title = {Spatial and temporal analysis of rainfall and temperature trend of India},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
