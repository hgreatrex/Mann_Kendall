@article{ WOS:000691092100013,
Author = {Ceribasi, Gokmen and Ceyhunlu, Ahmet Iyad},
Title = {Analysis of total monthly precipitation of Susurluk Basin in Turkey
   using innovative polygon trend analysis method},
Journal = {JOURNAL OF WATER AND CLIMATE CHANGE},
Year = {2021},
Volume = {12},
Number = {5},
Pages = {1532-1543},
Month = {AUG},
Abstract = {The effects of climate change caused by global warming can be seen in
   changes of climate variables such as precipitation, humidity, and
   temperatures. These effects of global climate change can be interpreted
   as a result of the examination of meteorological parameters. One of the
   most effective methods to investigate these effects is trend analysis.
   The Innovative Polygon Trend Analysis (IPTA) method is a trend analysis
   method that has emerged in recent years. The distinctive features of
   this method compared with other trend methods are that it depends on
   time series and can compare data series among themselves. Therefore, in
   this study, the IPTA method was applied to total monthly precipitation
   data of Susurluk Basin, one of Turkey's important basins. Data from ten
   precipitation observation stations in Susurluk Basin were used. Data
   were provided by the General Directorate of State Meteorology Affairs.
   The length of this data series was 12 years (2006-2017). As a result of
   the study, since there is no regular polygon in IPTA graphics of each
   station, it is seen that precipitation data varies by years. While this
   change is seen increasingly at some stations, it is seen decreasingly at
   other stations.},
