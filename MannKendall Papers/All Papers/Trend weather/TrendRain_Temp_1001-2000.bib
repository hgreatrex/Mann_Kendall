@article{ WOS:000275919800009,
Author = {Feller, Michael C.},
Title = {Trends in precipitation and streamwater chemistry in East Creek
   watershed in southwestern British Columbia, 1971-2008},
Journal = {JOURNAL OF LIMNOLOGY},
Year = {2010},
Volume = {69},
Number = {1},
Pages = {77-91},
Abstract = {Bulk precipitation and,stream water in a small, undisturbed. forested
   watershed in southwestern British Columbia were sampled regularly and
   analyzed for dissolved chemical constituents from 1971 to 2008.
   Concentrations and fluxes of most chemicals in precipitation and
   streamwater have exhibited considerable early variation. Temporal
   trends, when they lime occurred, hare rarely been consistent for. the
   entire 1972-2008 time period. Precipitation has exhibited a decline in
   electrical conductivity, a decline in NH4, inorganic-N and total-N
   concentrations and fluxes since the 1980s, an increase in a decline in
   acid H fluxes since similar to 1990. and a decrease in SO4
   concentrations and fluxes from 1980 until the late 1990s. Streamwater
   has exhibited an increase in NO2 concentrations' and fluxes until the
   late 1990s, an increase in pH and decrease in acid H fluxes since the
   early 1990s, a decrease in SO4 concentrations and fluxes from similar to
   1980 until similar to 2000, and increases in Na and Cl concentrations
   and fluxes until similar to 2000. Critical precipitation SO4 and
   inorganic-N loads have probably been exceeded for most years. East Ck.
   watershed has continuously experienced net inflows of all forms of N and
   acid H, and net outflows of dissolved Si, Na. Mg. and Ca. Net of
   inorganic forms of N and total-N have decreased since the early 1980s.
   Net acid H inflows have decreased since the early 1990s, while net Na
   and Cl outflows increased until similar to 2000. The contribution of
   nutrient cycling processes within the watershed to the changes is
   currently unknown.},
