@article{ WOS:A1983RU31000002,
Author = {HORVATH, L},
Title = {TREND OF THE NITRATE AND AMMONIUM CONTENT OF PRECIPITATION WATER IN
   HUNGARY FOR THE LAST 80 YEARS},
Journal = {TELLUS SERIES B-CHEMICAL AND PHYSICAL METEOROLOGY},
Year = {1983},
Volume = {35},
Number = {5},
Pages = {304-308},
Abstract = {The NO3- content of bulk precipitation samples in Hungary has increased
   about 7 times relative to its original value during the last 8 decades.
   At the same time, the concentration of NH4+ ion has decreased. The
   increase of NO3- is significant in all seasons, especially in spring
   months. The NH4+ concentration has decreased in fall and in winter, but
   has increased in spring and summer. The present spring concentration
   maxima of NO3- and NH4+ have developed since the turn of this century.
   The trends observed can be explained by the increased tropospheric
   concentration of air pollutants in this region.},
