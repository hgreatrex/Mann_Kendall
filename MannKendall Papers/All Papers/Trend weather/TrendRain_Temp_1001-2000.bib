@inproceedings{ WOS:000245813300009,
Author = {Brongersma, Sywert Hidde and Zhang, Wenqi},
Editor = {Russell, SW and Mills, ME and Osaki, A and Yoda, T},
Title = {Surface scattering revisited: Dimensional and low temperature trends},
Booktitle = {Advanced Metallization Conference 2006 (AMC 2006)},
Year = {2007},
Pages = {77-82},
Note = {Advanced Metallization Conference 2006, San Diego, CA, OCT 17-19, 2006},
Abstract = {Despite numerous room temperature experiments that indicate surface
   scattering is almost fully diffuse, low temperature resistivity data
   from narrow lines don't show the generally expected temperature
   dependent surface scattering contribution seen for large grained thin
   films. Here we show a detailed study of the trends expected for surface
   scattering using a numerical integration of the general equations. It
   shows that the surface scattering induce non-linear behavior requires
   the asymmetrical shape of a thin films cross-section in order to be
   manifest itself and is considerably reduced for square lines. This
   analysis also leads to a re-evaluation of using Matthiessen's rule for
   cross-section extraction, which has a significant effect on the
   separation of various scattering contributions. It leads to the
   conclusion that surface scattering is the main contributor to the
   resistivity increase and research should be refocused on how to reduce
   this contribution.},
