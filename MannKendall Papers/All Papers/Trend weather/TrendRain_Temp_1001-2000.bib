@article{ WOS:000380298800009,
Author = {Boodhoo, K. and Lollchund, M. R. and Dilmahamod, A. F.},
Title = {Trends analysis of precipitation data over the tropical South-West
   Indian Ocean (SWIO) basin using the Ensemble Empirical Mode
   Decomposition (EEMD) method},
Journal = {MAUSAM},
Year = {2016},
Volume = {67},
Number = {2},
Pages = {423-430},
Month = {APR},
Abstract = {In this paper, we propose the use of the Ensemble Empirical Mode
   Decomposition (EEMD) method in the analysis of trends in climate data.
   As compared to existing traditional methods, EEMD is simple, fast and
   reliable. It works by decomposing the time-series data into intrinsic
   mode functions until a residual component is obtained which represents
   the trend in the data. The dataset considered consists of satellite
   precipitation estimates (SPE) obtained from the Tropical Rainfall
   Measuring Mission (TRMM) for the tropical South-West Indian Ocean (SWIG)
   basin recorded during the periods January 1998 to December 2013. The
   SWIO basin spans from the latitudes 5 degrees S to 35 degrees S and the
   longitudes 30 degrees E to 70 degrees E and comprises of part of the
   east coast of Africa and some small island developing states (SIDS) such
   as Comoros, Madagascar, Mauritius and Reunion Island. The EEMD analysis
   is carried out for summer, winter and yearly time series of the SPE
   data. The results from the study are presented in terms of intrinsic
   mode functions (IMFs) and the trends. The analysis reveals that in
   summer, there is a tendency to have an increase in the amount of
   rainfall, whereas in winter, from 1998 to 2004 there has been an initial
   increase of 0.0022 mm/hr/year and from there onwards till 2013 a
   decrease of 0.00052 mm/hr/year was noted.},
