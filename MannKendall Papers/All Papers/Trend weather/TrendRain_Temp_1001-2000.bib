@article{ WOS:000344435300011,
Author = {Chen, Feng and Yuan, Yu-jiang and Wei, Wen-shou and Zhang, Tong-wen and
   Shang, Hua-ming and Zhang, Ruibo},
Title = {Precipitation reconstruction for the southern Altay Mountains (China)
   from tree rings of Siberian spruce, reveals recent wetting trend},
Journal = {DENDROCHRONOLOGIA},
Year = {2014},
Volume = {32},
Number = {3, SI},
Pages = {266-272},
Abstract = {We developed six tree-ring width chronologies of Siberian spruce (Picea
   obovata) from the low elevation forest of the southern Altay Mountains
   in northern Xinjiang, China. Although the six chronologies come from
   different sampling sites, significant correlations existed among the
   chronologies (r >= 0.477), and the first principal component (PC1)
   accounted for 72.2\% of total variance over their common period
   1825-2010. Correlation response analysis revealed that radial growth of
   Siberian spruce is mainly limited by a 12-month precipitation starting
   from July of the previous year to June of the current year. We therefore
   developed a July-June precipitation reconstruction spanning 1825-2009,
   which explained 65.5\% of the instrumental variance for the period
   1962-2009. The information of our precipitation reconstruction suggested
   that dry conditions existed for the periods 1829-1838, 1852-1855,
   1876-1888, 1898-1911, 1919-1923, 1932-1936, 1943-1955, 1963-1968,
