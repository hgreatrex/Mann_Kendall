@article{ WOS:001061639300001,
Author = {Redd, Mark E. and Guffey, Cassandra J. and Gustafson, Ethan L. and Hart,
   Elizabeth H. and Mcquade, Keturah S. and Giles, Neil F. and Knotts,
   Thomas A. and Wilding, W. Vincent},
Title = {Autoignition temperature trends for various chemical families},
Journal = {FUEL},
Year = {2024},
Volume = {355},
Month = {JAN 1},
Abstract = {Autoignition temperature (AIT) data in the literature are frequently
   disparate and have high uncertainties. Examining the trend of AIT for
   chemical families that have well-defined homologous series can allow
   evaluation for internal consistency and provide phenomenological insight
   into the mechanisms that produce the observed trends, allowing engineers
   to better predict AIT for increasingly exotic compounds. The trend for
   the normal alkanes is well-studied and serves as a limiting case for
   other chemical families. A comprehensive examination of AIT trends for
   several chemical families reveals differences from the normal alkane
   family trend. AIT values from experimental measurements are presented
   and used, with literature values, to establish trends for several
   single-functional group chemical families, as well as propose and
   discuss phenomenological explanations for observed differences. Broader
   implications for these trends are also discussed along with best
   practices for evaluating and measuring AIT.},
