@article{ WOS:000171588000028,
Author = {Maul, GA and Davis, AM and Simmons, JW},
Title = {Seawater temperature trends at USA tide gauge sites},
Journal = {GEOPHYSICAL RESEARCH LETTERS},
Year = {2001},
Volume = {28},
Number = {20},
Pages = {3935-3937},
Month = {OCT 15},
Abstract = {Seawater temperatures have been measured at United States tide gauges
   throughout most of the 20(th) century. All available records have been
   digitized, and the longest 14 have been analyzed by linear least-square
   regression. The largest positive trend is from Boston MA (+3.6 +/- 0.4
   degreesC per century), and the largest negative trend is at Charleston
   SC (-0.1 +/- 0.3 degreesC per century). No consistent latitudinal or
   east-coast vs. west-coast patterns are discernable, but air temperature
   trends are typically greater than seawater changes.},
