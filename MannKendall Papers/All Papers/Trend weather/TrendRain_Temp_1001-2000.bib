@article{ WOS:000385472400045,
Author = {Ziernicka-Wojtaszek, Agnieszka and Kruzel, Joanna},
Title = {The Diversification of Air Temperature Trends in Poland (1981-2010)},
Journal = {POLISH JOURNAL OF ENVIRONMENTAL STUDIES},
Year = {2016},
Volume = {25},
Number = {5},
Pages = {2205-2209},
Abstract = {The aim of this study is to answer questions such as: What is the pace
   of warming? How diversified is it in particular months? How is it
   spatially diversified in Poland in the 1981-2010 30-year period
   constituting the new, normal climate? The study was based on materials
   concerning monthly mean air temperature values from the above-mentioned
   period from 53 weather stations belonging to the network of the
   Institute of Meteorology and Water Management spread evenly across
   Poland. It was stated that warming in Poland in the 1981-2010 period,
   amounting to 0.33 degrees C/decade, resulted mainly from a spring and
   summer temperature increase of alpha = 0.05. Temperature increases
   occurred at the greatest pace in July (0.81) and April (0.73). The pace
   is slightly slower in June (0.57 degrees C), and August (0.48 degrees
   C/decade). A high trend for February (0.85 degrees C/decade) is not
   statistically significant. Trend values in October and December of 0.2
   degrees C/decade are negative but statistically insignificant. The
   regional differentiation of statistically significant temperature trends
   in Poland is as follows: in April the greatest increase in temperature
   is visible in mid-western Poland, with the lowest values in: Zielona
   Gora 0.96, Gorzow Wielkopolski 0.88, Bielsko Biala 0.85, and Poznan.
   0.84 degrees C/decade. In June, there is a decrease of trends, from the
   highest values in southwestern Poland to the lowest in northeastern
   Poland: Zielona Gora 0.79, Poznan. 0.74, Kalisz 0.61, Plock 0.57, and
   Ostroleka 0.36 degrees C/decade. In July there is a decrease of trend
   values from east to west: Suwalki 1.08, Warsaw 0.97, Poznan 0.89,
   Legnica 0.71, Zielona Gora 0.69, and Szczecin 0.57 degrees C/decade. In
   July and August a decrease in trend values can be observed from east to
   west: Zamoge 0.71, Warsaw 0.49, and Szczecin 0.27 degrees C/decade.},
