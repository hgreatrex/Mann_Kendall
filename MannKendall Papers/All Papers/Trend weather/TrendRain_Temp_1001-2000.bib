@article{ WOS:A1946XY75500001,
Author = {MANLEY, G},
Title = {TEMPERATURE TREND IN LANCASHIRE, 1753-1945},
Journal = {QUARTERLY JOURNAL OF THE ROYAL METEOROLOGICAL SOCIETY},
Year = {1946},
Volume = {72},
Number = {311},
Pages = {1-31},
Abstract = {A table of monthly mean temps. for the Lancashire plain from 1753 to the
   present day has been compiled by reduction from a large number of
   little-known early observations. It is believed that from 1781 onward
   the values are likely to be fairly reliable. The general trend of the
   mean temp. for each month agrees remarkably well with that shown to have
   occurred since about 1820 in Scandinavia. In general, there has been an
   upward trend of the mean monthly winter temperatures (e.g., the Jan.
   means have risen about 3 {[}degree]F since 1820), whereas there has been
   no appreciable change during the summer months. The overall rise in mean
   annual temp. since 1820 has been about 0.6{[}degree]F.},
