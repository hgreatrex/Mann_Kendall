@article{ WOS:001176004500001,
Author = {Gregory, Richard P.},
Title = {Risk premiums from temperature trends},
Journal = {INTERNATIONAL REVIEW OF ECONOMICS \& FINANCE},
Year = {2024},
Volume = {91},
Pages = {505-525},
Month = {MAR},
Abstract = {Previous estimates of temperature shock risk premiums in the stock
   market have been biased by the use of the Fama-MacBeth estimator, the
   use of monthly portfolios, and the neglect that temperature time-series
   can exhibit both short- and -long-term components. From the previous
   literature, we know that temperature shocks can increase equity premiums
   through a number of channels. I use the Jegadeesh et al. (2019) EIV
   estimation method and daily individual stock returns from the NYSE, ASE
   and NASDAQ from July 1, 1963 to Dec. 31, 2020, to correct for the
   estimation bias, and I divide the temperature series into long- and
   short -term components. I also control for productivity shocks,
   non-linearity and macroeconomic variables. I find that the US
   Short-Term, US Long-Term and the World Long-Term Temperature factors are
   significantly priced by the market at less than the 1\% level. Together,
   these three components are estimated to raise the cost of equity in the
   United States by 3.1\% per year, higher than previous estimates.},
