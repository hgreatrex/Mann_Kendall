@article{ WOS:000254165200001,
Author = {Holt, J. M. and Zhang, S. R.},
Title = {Long-term temperature trends in the ionosphere above Millstone Hill},
Journal = {GEOPHYSICAL RESEARCH LETTERS},
Year = {2008},
Volume = {35},
Number = {5},
Month = {MAR 13},
Abstract = {Increasing concentrations of greenhouse gases are expected on
   theoretical grounds to lead to a cooling of the upper atmosphere. Due to
   the close thermal coupling of the neutral and ionized components of the
   upper atmosphere the effects of this cooling are also expected to be
   seen in the ionospheric ion temperature. Long-term satellite
   observations of neutral density show a trend consistent with the
   expected cooling. Temperature data from the Millstone Hill incoherent
   scatter radar ( 46.2 degrees N, 288.5 degrees E) from 1978 to 2007 have
   been analyzed to provide a direct estimate of the temperature trend
   above the radar. The long-term trend in the directly measured ion
   temperature T-i at 375 km is found to be -4.7 K/ year with a 95\%
   confidence interval of -3.6 to -5.8 K. The estimated trend in the
