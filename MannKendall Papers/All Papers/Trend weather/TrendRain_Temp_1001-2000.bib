@article{ WOS:000564302900001,
Author = {Shao, Yawen and Wang, Quan J. and Schepen, Andrew and Ryu, Dongryeol},
Title = {Embedding trend into seasonal temperature forecasts through statistical
   calibration ofGCMoutputs},
