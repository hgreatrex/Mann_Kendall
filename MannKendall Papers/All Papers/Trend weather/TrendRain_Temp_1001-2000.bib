@article{ WOS:000419286600007,
Author = {Lobato-Sanchez, Rene and Angel Altamirano-del-Carmen, Miguel},
Title = {Detection of local temperature trends in Mexico},
Journal = {TECNOLOGIA Y CIENCIAS DEL AGUA},
Year = {2017},
Volume = {8},
Number = {6},
Pages = {101-116},
Month = {NOV-DEC},
Abstract = {Temperature records indicate that three of the four climatic stations
   evaluated in Mexico show warming over the years 1950 to 2013, as
   compared to the period 1961 to 1990 which was used as a baseline. After
   a complete analysis of climate records in Mexico, only 112 were
   determined to have met the quality and standards required for the
   present study. From those, a subset of 20 stations with a positive trend
   were randomly selected, which were representative of urban and rural
   areas distributed across Mexico. The mid 1970s was identified as the
   most recent period of intense warming, which is consistent with the mean
   global temperature indicated by the Intergovernmental Panel on Climate
   Change. A 10-year difference in the start of the warming period was
   found between rural and urban regions in Mexico. Warming and the
   difference between warm and cold periods are typically greater in urban
   areas than in rural areas.},
