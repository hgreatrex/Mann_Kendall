@article{ WOS:000593444800001,
Author = {Kelder, T. and Muller, M. and Slater, L. J. and Marjoribanks, T. I. and
   Wilby, R. L. and Prudhomme, C. and Bohlinger, P. and Ferranti, L. and
   Nipen, T.},
Title = {Using UNSEEN trends to detect decadal changes in 100-year precipitation
   extremes},
Journal = {NPJ CLIMATE AND ATMOSPHERIC SCIENCE},
Year = {2020},
Volume = {3},
Number = {1},
Month = {DEC 27},
Abstract = {Sample sizes of observed climate extremes are typically too small to
   reliably constrain return period estimates when there is non-stationary
   behaviour. To increase the historical record 100-fold, we apply the
   UNprecedented Simulated Extreme ENsemble (UNSEEN) approach, by pooling
   ensemble members and lead times from the ECMWF seasonal prediction
   system SEAS5. We fit the GEV distribution to the UNSEEN ensemble with a
   time covariate to facilitate detection of changes in 100-year
   precipitation values over a period of 35 years (1981-2015). Applying
   UNSEEN trends to 3-day precipitation extremes over Western Norway
   substantially reduces uncertainties compared to estimates based on the
   observed record and returns no significant linear trend over time. For
   Svalbard, UNSEEN trends suggests there is a significant rise in
   precipitation extremes, such that the 100-year event estimated in 1981
   occurs with a return period of around 40 years in 2015. We propose a
   suite of methods to evaluate UNSEEN and highlight paths for further
   developing UNSEEN trends to investigate non-stationarities in climate
   extremes.},
