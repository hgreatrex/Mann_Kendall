@article{ WOS:000566358600002,
Author = {Fagnant, Carlynn and Gori, Avantika and Sebastian, Antonia and Bedient,
   Philip B. and Ensor, Katherine B.},
Title = {Characterizing spatiotemporal trends in extreme precipitation in
   Southeast Texas},
