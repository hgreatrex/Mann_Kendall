@article{ WOS:000306347900027,
Author = {Yang, Guosheng and Ma, Lingling and Xu, Diandou and Liu, Liyan and Jia,
   Hongliang and Chen, Yang and Zhang, Yongbao and Chai, Zhifang},
Title = {Temporal trends of polychlorinated biphenyls in precipitation in
   Beijing, China},
