@inproceedings{ WOS:000078339600030,
Author = {De Luís, M and Raventós, J and Sánchez, JR and Gonzalez, JC and Cortina,
   J and García-Cano, MF},
Editor = {Lemmela, R and Helenius, N},
Title = {Event contribution to annual precipitation:: A trend analysis},
Booktitle = {PROCEEDINGS OF THE SECOND INTERNATIONAL CONFERENCE ON CLIMATE AND WATER,
   VOLS 1-3},
Year = {1998},
Pages = {290-300},
Note = {2nd International Conference on Climate and Water, ESPOO, FINLAND, AUG
   17-20, 1998},
Organization = {Helsinki Univ Technol; World Meteorol Org; UNESCO; European Commiss; Int
   Atom Energy Agcy; European Themat Network Educ \& Training Environm
   Water; Technol Water Resources; Int Assoc Hydrol Sci; Int Commiss Hydrol
   Rhine Basin; Austrian Natl Comm IHP; Acad Finland, Natl Comm Int Hydrol
   Programme; Finnish Minist Agr \& Forestry; Finnish Minist Environm;
   Finnish UNESCO Commiss; Finnish Geophys Soc; Interamer Inst Climate
   Change Res; Keski Uusimaa Joint Municipal Board Water Pollut Control;
   Kemijoki Oy; City Espoo; Finnair},
Abstract = {Climatic conditions in the Mediterranean area (i.e. long drought periods
   and rainfall concentration in a few storm events) favour soil erosion
   and land degradation. In this area, several studies show that more that
   80\% of soil losses are caused by 5 to 10\% of the rainfall events. The
   temporal and spatial analysis of these high erosivity rainfall events is
   a key factor to evaluate trends in erosion and desertification risk.
   We analysed spatial and temporal trends in the ten annual maximum
   rainfall days from 97 pluviometric stations in the Region of Valencia (E
   Spain) for the normal period 1961-1990.
   The main results are as follows:
   1) The proportion of annual rainfall falling during the day, with
   maximum rainfall increases in 37\% of the stations, and decreases in the
