@article{ WOS:000655799400001,
Author = {Shobeiri, Shahin and Sharafati, Ahmad and Neshat, Aminreza},
Title = {Evaluation of different gridded precipitation products in trend analysis
   of precipitation features over Iran},
Journal = {ACTA GEOPHYSICA},
Year = {2021},
Volume = {69},
Number = {3, SI},
Pages = {959-974},
Month = {JUN},
Abstract = {In recent years, gridded precipitation products have been widely used in
   hydrology studies and other fields of water sciences. This study
   evaluated the potential of several gridded precipitation products,
   including GPCC, TRMM, CRU, ERA-Interim, and ERA5, in trend analysis of
   precipitation depth and the number of rainy days in various regions of
   Iran. Moreover, the observational precipitation data of the daily time
   series were collected from 68 Iranian synoptic stations. The
   Mann-Kendall test was conducted to determine gridded and observed
   precipitation trends in the period of 1997 to 2017. The probability of
   detection (POD) and false alarm ratio (FAR) indices were utilized to
   compare gridded and observed precipitation trends. Results showed that
   the best consistency (POD: 52\% similar to 80\%, FAR: 60\% similar to
   88\%) was observed between the observed trends of the number of rainy
   days and those obtained by TRMM product over different regions of Iran.
   Moreover, ERA-Interim offered a better performance (POD: 50\% similar to
   100\%, FAR: 58\% similar to 72\%) in the trend analysis of precipitation
   depth in Iran. The consistency between observational and gridded
   precipitation trends has never been analyzed in Iran at this level;
   therefore, this is considered a unique analysis. Besides, the generated
   maps of precipitation products' performance provide a comprehensive view
   of better water resources management over different regions of Iran.},
