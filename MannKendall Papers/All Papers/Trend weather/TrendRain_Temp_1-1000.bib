@inproceedings{ WOS:000297612400001,
Author = {Spence, Christopher and Kokelj, Steven V. and Ehsanzadeh, Eghbal},
Editor = {Yang, D and Marsh, P and Gelfan, A},
Title = {Precipitation trends contribute to streamflow regime shifts in northern
   Canada},
Booktitle = {COLD REGIONS HYDROLOGY IN A CHANGING CLIMATE},
Series = {IAHS Publication},
Year = {2011},
Volume = {346},
Pages = {3+},
Note = {25th General Assembly of the International Union of Geodesy and
   Geophysics, Melbourne, AUSTRALIA, JUN 28-JUL 07, 2011},
Organization = {Int Commiss Water Resources Systems; Int Commiss Surface Water; Int
   Commiss Water Qual; Int Assoc Hydrol Sci; UNESCO-IHP; Forests NSW; USDA,
   Forest Serv; Chinese Acad Forestry; Weyerhaeuser CO; Colorado State Univ},
Abstract = {Autumn runoff events rivalling the size of the spring freshet peak as
   well as sustained winter streamflow have become more common in the
   northwestern Canadian Shield since the mid 1990s. Previous circumpolar
   and large regional-scale studies have implied these phenomena are due to
   increased water inputs from thawing permafrost. However, results from an
   investigation of the precipitation and temperature trends provide an
   alternate explanation for this region. A shift from a nival to a
   combined nival/pluvial streamflow regime, particularly in small
   watersheds, can be attributed to trends in the timing and state of
   autumn precipitation. Because these trends are subtle, careful
   consideration of hydrological processes, and the temporal and landscape
   context in which they operate, is important when attempting to explain
   the observed shifts in regional streamflow. It is important to correctly
   explain why streamflow regimes are changing because of close
   relationships with variations in ground thermal conditions and aquatic
   chemistry, which are of significance to society. These relationships are
   discussed.},
