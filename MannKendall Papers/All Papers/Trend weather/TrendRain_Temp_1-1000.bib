@article{ WOS:000182607200013,
Author = {Kunkel, KE},
Title = {North American trends in extreme precipitation},
Journal = {NATURAL HAZARDS},
Year = {2003},
Volume = {29},
Number = {2},
Pages = {291-305},
Month = {JUN},
Abstract = {An analysis of extreme precipitation events indicates that there has
   been a sizable increase in their frequency since the 1920s/1930s in the
   U. S. There has been no discernible trend in the frequency of the most
   extreme events in Canada, but the frequency of less extreme events has
   increased in some parts of Canada, notably in the Arctic. In the U. S.,
   frequencies in the late 1800s/early 1900s were about as high as in the
   1980s/1990s. This suggests that natural variability of the climate
   system could be the cause of the recent increase, although anthropogenic
   forcing due to increasing greenhouse gas concentrations cannot be
   discounted as another cause. It is likely that anthropogenic forcing
   will eventually cause global increases in extreme precipitation,
   primarily because of probable increases in atmospheric water vapor
   content and destabilization of the atmosphere. However, the location,
   timing, and magnitude of local and regional changes remain unknown
   because of uncertainties about future changes in the frequency/intensity
   of meteorological systems that cause extreme precipitation.},
Publisher = {SPRINGER},
Address = {ONE NEW YORK PLAZA, SUITE 4600, NEW YORK, NY, UNITED STATES},
Type = {Article},
Language = {English},
Affiliation = {Illinois State Water Survey, 2204 Griffith Dr, Champaign, IL 61820 USA.
   Illinois State Water Survey, Champaign, IL 61820 USA.},
DOI = {10.1023/A:1023694115864},
ISSN = {0921-030X},
EISSN = {1573-0840},
