@article{ WOS:000359011900011,
Author = {Wang, Yuan},
Title = {Air Pollution or Global Warming: Attribution of Extreme Precipitation
   Changes in Eastern China-Comments on ``Trends of Extreme Precipitation
   in Eastern China and Their Possible Causes{''}},
Journal = {ADVANCES IN ATMOSPHERIC SCIENCES},
Year = {2015},
Volume = {32},
Number = {10},
Pages = {1444-1446},
Month = {OCT},
Abstract = {The recent study ``Trends of Extreme Precipitation in Eastern China and
   Their Possible Causes{''} attributed the observed decrease/increase of
   light/heavy precipitation in eastern China to global warming rather than
   the regional aerosol effects. However, there exist compelling evidence
   from previous long-term observations and numerical modeling studies,
   suggesting that anthropogenic pollution is closely linked to the recent
   changes in precipitation intensity because of considerably modulated
   cloud physical properties by aerosols in eastern China. Clearly, a
   quantitative assessment of the aerosol and greenhouse effects on the
   regional scale is required to identify the primary cause for the extreme
   precipitation changes.},
