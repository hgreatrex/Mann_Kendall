@article{ WOS:001031288500002,
Author = {Young, Kathy L. L. and Brown, Laura C. C.},
Title = {Temperature and Precipitation Trends of the Shoulder Seasons at Polar
   Bear Pass (Nanuit Itillinga) - A Ramsar Wetland of Importance, Nunavut},
Journal = {WETLANDS},
Year = {2023},
Volume = {43},
Number = {6},
Month = {AUG},
Abstract = {Temperature and precipitation changes play critical roles in permafrost
   thaw, wetland water storage, plant growth and greenhouse gas emissions.
   This study evaluates the spring (AMJ) and fall (SON) - shoulder season
   temperature and precipitation conditions for Polar Bear Pass (PBP), a
   Ramsar wetland site of international importance, to assess significant
   changes since the early 1980s when a weather station was deployed here
   on a polar desert upland adjacent to the wetland. Temperature and
   precipitation records are compared to the nearby government weather
   station at Resolute Bay (RB). Fall temperature conditions at PBP and RB
   show significant warming, though spring temperature conditions do not.
   The North Atlantic Oscillation Index (NAO) accounts for most of the
   variability in air temperature at the sites. Surface temperatures are
   increasing in the spring at PBP but cannot yet be linked to an earlier
   loss of snow cover. There is an earlier trend to snow on validating
   recent High Arctic space-based snow cover studies. The identified trends
   during the shoulder seasons highlight the importance of year-round
   monitoring and the need for improved data collection techniques in
   important ecological environments such as High Arctic wetlands.},
