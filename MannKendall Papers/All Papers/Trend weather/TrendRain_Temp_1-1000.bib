@article{ WOS:000293610500001,
Author = {Haenggi, Pascal and Jetel, Marketa and Kuettel, Marcel and Wanner, Heinz
   and Weingartner, Rolf},
Title = {Weather type-related trend analysis of precipitation in Switzerland},
Journal = {HYDROLOGIE UND WASSERBEWIRTSCHAFTUNG},
Year = {2011},
Volume = {55},
Number = {3},
Pages = {140-154},
Month = {JUN},
Abstract = {In hydrology, research of changes in precipitation is of particular
   interest, because precipitation directly affects runoff and its
   spatial-temporal distribution. We analysed how changes in the frequency
   of occurrence (number of days per season or year) of the Alpine Weather
   Types (AWTs) by SCHUEPP (1968) may influence seasonal and annual
   precipitation amounts and the interannual variability over Switzerland.
   This analysis included a comparison of trends both in precipitation
   amounts and frequencies of the AWTs. Since 1901 they have shown in the
   northern part of the Swiss Alps a significant increase in the
   precipitation amounts in winter and in the whole year, while
   precipitation in spring became more variable there. The annual
   frequencies of AWTs shifted towards more convective and less advective
   ones. However, the increasing trends in the convective weather types
   during spring and summer may be the result of inhomogeneities in the
   time series. To explain the observed changes in precipitation amounts,
   the changes both in frequency and precipitation-productivity (average
   amount of precipitation per weather type) of the AWTs were used: The
   former mainly explain changes within the convective AWTs, the latter
   within the advective ones. The significant increase of variability in
   spring precipitation can be explained by increased variability of
   productivity within the AWTs. It was shown that changes both in the
   frequencies of the AWT and in their precipitation-productivity are
   important to explain the observed changes in precipitation amounts.
   However, none of them could be identified as the key factor in all
   seasons and regions. Moreover, the driving forces behind the observed
   changes in the AWTs still remain unclear.},
