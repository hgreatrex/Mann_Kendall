@article{ WOS:000718790800005,
Author = {El Kasri, Jada and Lahmili, Abdelaziz and Soussi, Halima and Jaouda,
   Imane and Bentaher, Maha},
Title = {Trend Analysis of Meteorological Variables: Rainfall and Temperature},
Journal = {CIVIL ENGINEERING JOURNAL-TEHRAN},
Year = {2021},
Volume = {7},
Number = {11},
Pages = {1868-1879},
Month = {NOV},
Abstract = {The Souss-Massa region in southwestern Morocco is characterized by a
   semi-arid climate with high variability in rainfall. Frequent droughts
   and flash flood events combined with overexploitation of water resources
   in recent decades have had a significant impact on the human security
   and the economy which is mainly based on agriculture, tourism and
   fishery. For better management of extreme events and water resources
   under changing climatic conditions, a study was carried out to quantify
   the seasonal and annual variability and trends in rainfall and
