@article{ WOS:000459226500021,
Author = {Ul Shafiq, Mifta and Rasool, Rehana and Ahmed, Pervez and Dimri, A. P.},
Title = {Temperature and Precipitation trends in Kashmir valley, North Western
   Himalayas},
