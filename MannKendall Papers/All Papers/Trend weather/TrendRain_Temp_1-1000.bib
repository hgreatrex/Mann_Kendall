@inproceedings{ WOS:000246040600072,
Author = {Miler, Antoni T. and Miler, Malgorzata},
Editor = {Piecuch, T},
Title = {Trends and periodicities of temperature and precipitation changes for
   Poznan between years 1848-2000},
Booktitle = {VII National Polish Scientific Conference on Complex and Detailed
   Problems of Environmental Engineering},
Series = {ZESZYTY NAUKOWE WYDZIALU BUDOWNICTWAI I INZYNIERII SRODOWISKA},
Year = {2005},
Number = {22},
Pages = {945-956},
Note = {7th National Polish Scientific Conference on Complex and Detailed
   Problems of Environmental Engineering, Koszalin, POLAND, 2005},
Abstract = {At present one of the most essential problems in contemporary
   climatology is the problem of climate warming. Whereas the natural
   factors of climate changes show periodic variations, the anthropogenic
   factor of climate changes is characterized with a constant tendency of
   variation, i.e. a linear trend. Short-term trends of temporal climate
   changes depend, therefore on: natural periodicals, the level of
   pollution in the atmosphere and a non-measurable factor connected with
   randomness. Narrowing consideration to the Wielkopolska region, with
   main, centrally situated agglomeration of Poznan, one should notice,
   that the problem of climatic changes has a special meaning in this
   region. It results from the fact, that water resources in the
   Wielkopolska region are very small (the smallest in relation to other
   regions in Poland).
   Therefore, the subject discussed in his paper, concerning estimation of
   climate changes in Poznan during last 150 years (time series of air
   temperature and precipitation), seems to be vital for cognitive reasons
   as well as utilitarian.
   In the present study harmonical analysis (based on Fourier's series) is
