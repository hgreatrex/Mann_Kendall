@article{ WOS:000484814100013,
Author = {Yucel, Ali and Atilgan, Atilgan and Oz, Hasan},
Title = {TREND ANALYSIS IN TEMPERATURE, PRECIPITATION AND HUMIDITY: THE CASE OF
