@article{ WOS:000547843200001,
Author = {Sezen, C. and Partal, T.},
Title = {Wavelet combined innovative trend analysis for precipitation data in the
   Euphrates-Tigris basin, Turkey},
Journal = {HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES},
Year = {2020},
Volume = {65},
Number = {11},
Pages = {1909-1927},
Month = {AUG 17},
Abstract = {In this study, annual and seasonal precipitation trend analysis was
   performed in the Euphrates-Tigris basin, Turkey, using innovative trend
   analysis (ITA) and discrete wavelet transformation. In this context, it
   was seen that there is a downward trend in winter, spring and annual
   precipitation, whereas precipitation has an increasing tendency in
   summer and autumn seasons, in the greater part of the basin. When annual
   and seasonal data were decomposed into wavelet components, the most
   significant trends were observed for high-periodic wavelet components,
   such as D3 (8-year), D4 (16-year) and D5 (32-year), where these
   components represent the periods of the precipitation data. Then, the
   relationship between North Atlantic Oscillation (NAO) and trend in
   precipitation was investigated. In this regard, it was found that there
   could be a significant relationship between the NAO and precipitation
   trends of the Euphrates-Tigris basin, especially in winter, based on the
   wavelet ITA.},
