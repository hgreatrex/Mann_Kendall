@article{ WOS:000349063900001,
Author = {Chinchorkar, S. S. and Sayyad, F. G. and Vaidya, V. B. and Pandye, Vyas},
Title = {Trend detection in annual maximum temperature and precipitation using
   the Mann Kendall test - A case study to assess climate change on An and
   of central Gujarat},
