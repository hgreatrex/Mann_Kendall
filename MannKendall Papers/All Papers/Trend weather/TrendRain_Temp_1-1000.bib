@article{ WOS:001257644700002,
Author = {Gu, Guojun and Adler, Robert F.},
Title = {Variability and trends in tropical precipitation intensity in
   observations and climate models},
Journal = {CLIMATE DYNAMICS},
Year = {2024},
Volume = {62},
Number = {8},
Pages = {7429-7443},
Month = {AUG},
Abstract = {Variations and changes in the tropical (30oN-30oS) precipitation amount
   and precipitation intensity during the satellite era (1979-present) are
   investigated by means of the satellite-based monthly precipitation
   product from the Global Precipitation Climatology Project (GPCP) version
   2.3. Precipitation spatial intensity distributions represented by
   monthly percentiles and the differences between high/low percentiles and
   mean precipitation are examined for the tropical domain and for seven
   tropical sectors with distinct climate characteristics. Variations and
   trends in these components are closely related to the changes in
   precipitation regimes including shifting, shrinking/enlarging of
   tropical and regional wet/dry zones and also to the changes in
   precipitation intensity. Increases in high percentiles and decreases in
   low percentiles appear over tropical land and ocean, despite intense
   interannual variability contributed primarily by the El Ni \&
   ntilde;o-Southern Oscillation (ENSO). Positive (negative) trends in the
   differences between wet (dry) percentiles and mean precipitation can be
   seen over tropical land and ocean as well, suggesting the broadening of
   precipitation spatial intensity distribution. Regional results generally
   support this conclusion except over tropical Africa and South Asia.
   These observational results are also generally evident in CMIP6
   historical and AMIP6 runs with regards to the signs of the changes,
   though with smaller magnitudes. Relative to tropical (land + ocean) mean
   precipitation, wet percentile trends are + 0.7, + 0.8 and + 2.5\%/decade
   and dry percentile trends are - 0.6, -0.7 and - 1.8\%/decade for CMIP6,
   AMIP6 and GPCP, respectively. The zonal-mean ITCZ intensity defined by
   GPCP, CMIP6 and AMIP6 precipitation increased during the period at a
   similar change rate to respective mean precipitation over tropical wet
   grid-cells, both stronger than the changes in tropical mean
   precipitation. These consistencies further corroborate the strengthening
   contrast between wet and dry in the tropics.},
