@article{ WOS:000757748300002,
Author = {Li, Jingnan and Amano, Yoshimasa and Machida, Motoi},
Title = {Temperature-dependent growth characteristics and dominance trends of the
   cyanobacterium Microcystis sp. and the diatom Cyclotella meneghiniana},
Journal = {HYDROBIOLOGIA},
Year = {2022},
Volume = {849},
Number = {7},
Pages = {1677-1688},
Month = {APR},
Abstract = {The temperature-related growth characteristics of Microcystis sp. and
   its competitor Cyclotella meneghiniana were examined in a series of
   monoculture experiments at six different temperatures. Then, the
   temperature-related term of each species was established for a
   previously developed interspecific competition model, which can not be
   applied in actual lakes due to the lack of temperature-related terms.
   Based on experimental results, the highest temperature-dependent growth
   rate for C. meneghiniana was calculated to be 0.64 day(-1) at 29 degrees
   C and that for Microcystis sp. was 0.65 day(-1) at 37 degrees C. The
   coefficient epsilon, which reflects the extent of the biovolume change
   affected by temperature, was 19.4 for C. meneghiniana and 8.7 for
   Microcystis sp., indicating that C. meneghiniana is more sensitive to
   the temperature than Microcystis sp. Moreover, cocultivation experiments
   at low temperature (20 degrees C) and high temperature (30 degrees C)
   were conducted to verify the influence of these temperature-dependent
   growth characteristics on the interspecific interaction. The biovolume
   in saturation and dominant species were portrayed in the two temperature
   groups to assess the effect of temperature on their interaction. This
   study also discussed the effect of temperature on Microcystis blooms
   formation by comparing experimental results with the observed situation
   in the field of Lake Tega.},
