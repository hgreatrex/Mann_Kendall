@article{ WOS:000797000300002,
Author = {Makhasana, Payal and Joshi, Geeta S.},
Title = {Trends in rainfall extremity and peak flood in Sabarmati River Basin,
   India},
Journal = {PHYSICS AND CHEMISTRY OF THE EARTH},
Year = {2022},
Volume = {126},
Month = {JUN},
Abstract = {It is important to recognize the trend of the extreme rainfall and its
   influence on peak flood event occurrences. This study analyzes the
   trends in the rainfall extremity and the trend in the peak river flood
   in the Sabarmati River basin of India and its sub-basins/sub-regions.
   Five indices have been considered for the assessment of extreme rainfall
   evaluation and trend detection. Results of the trend of each index is
   coherent with the trend of observed peak flood in Sabarmati River basin.
   The spatial distribution of trend of intense rainfall has been obtained
   by aggregating the spatial distribution of indices for maximum 1-day
   rainfall of the year (MR-1 day), and number/count of heavy rainfall days
   (Rheavy) in GIS in each sub-basin of the study area. It is concluded
   that the positive trend in the intense rainfall index govern the
   positive trend with a strong correlation in the peak river flood in the
   Sabarmati River sub-basins. The study will be helpful for infrastructure
   facilities and services such as storm water drain and water storage
   structure design and operation etc., and to prevent the damages due to
   extreme events in the river basin area.},
