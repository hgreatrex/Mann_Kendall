@article{ WOS:000759388800002,
Author = {Gao, Feng and Chen, Xiaoling and Yang, Wenfu and Wang, Wenwen and Shi,
   Lijiang and Zhang, Xiaolong and Liu, Yaomeng and Tian, Yaofei},
Title = {Statistical characteristics, trends, and variability of rainfall in
   Shanxi province, China, during the period 1957-2019},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2022},
Volume = {148},
Number = {3-4},
Pages = {955-966},
Month = {MAY},
Abstract = {Under the background of global warming, an analysis of the trend and
   variability of rainfall time series on various timescales is very
   important for understanding climate change on a regional scale. In this
   study, a trend analysis of rainfall time series on monthly, seasonal,
   and annual scales in Shanxi province, China, during the period 1957-2019
   was conducted using the Mann-Kendall test and Sen's slope estimator.
   Statistical characteristics and the distribution of rainfall were
   examined using commonly used statistical indicators and the
   precipitation concentration index (PCI). Three rainfall-based indices,
   standardized anomaly index (SAI), rainfall anomaly index (RAI), and
   standardized precipitation index (SPI), were used to investigate the
   rainfall variability and drought conditions in Shanxi province over the
   period 1957-2019. The results showed that a statistically significant
   decreasing trend occurred in March and July at significance levels of
   1\% and 10\%, respectively. Spring and autumn showed an upward trend,
   and summer and winter exhibited a downward trend. An insignificant
   decreasing trend was detected for the annual rainfall series. Summer
   experienced the largest decrease rate of rainfall (-0.808 mm/year). The
   climate of Shanxi province is gradually becoming dry. However, rainfall
   presented a recovery tendency after entering the twenty-first century.
   The findings in this study can provide useful information for
   agricultural production, water resource planning, and socioeconomic
   development.},
