@inproceedings{ WOS:000532808700041,
Author = {Martinez Morales, Laura Valentina and Currea Piratova, Karen Ximena and
   Rico Martinez, Monica Andrea},
Editor = {Martinez, MAR},
Title = {Real-time temperature and audio baby monitoring using IoT technologies},
Booktitle = {2019 CONGRESO INTERNACIONAL DE INNOVACION Y TENDENCIAS EN INGENIERIA
   (CONIITI )},
Year = {2019},
Note = {International Congress of Innovation and Trends in Engineering
   (CONIITI), Bogota, COLOMBIA, OCT 02-04, 2019},
Organization = {Univ Catolica Colombia, Vigilada Mineducac, Acreditac Instituc Alta
   Calidad; IEEE Reg 9; IEEE Colombia Sect Technol \& Engn Management
   Chapter; IEEE Technol \& Engn Management Soc; IEEE},
Abstract = {In babies, a very important factor, is the body temperature, since the
   increase in this factor indicates a defense mechanism that the body uses
   to try to combat diseases. Especially in the baby's sleep hours it is
   not very easy to see a change in temperature, because it frequently
   concurs with the parents' sleep time, therefore the babies are less
   supervised. It is important to mention that an increase in temperature
   in a short time can generate a febrile complication and cause Sudden
   Infant Death Syndrome (SIDS), a problem that affects approximately 533
   babies per year according from the AAP (American Academy of Pediatrics)
   {[}1]{[}2]. In this project an electronic device is developed that
   census the temperature in real time. At the same time, it sends the
   level of audio captured from babies between 0-12 months of age, to the
   end user in order to assist parents in monitoring the baby. The device
   is developed to be placed in the baby's bed. When the device detects an
   increase outside the normal range of body temperature, or a high level
   of noise, it transfers the data via WiFi and Bluetooth to a web platform
   and it sends an alarm via a mobile application to the parents indicating
   an alteration in the state of the baby.},
