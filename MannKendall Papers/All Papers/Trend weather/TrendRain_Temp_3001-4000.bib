@article{ WOS:000578319700001,
Author = {Mitchell, Dann M. and Eunice Lo, Y. T. and Seviour, William J. M. and
   Haimberger, Leopold and Polvani, Lorenzo M.},
Title = {The vertical profile of recent tropical temperature trends: Persistent
   model biases in the context of internal variability},
Journal = {ENVIRONMENTAL RESEARCH LETTERS},
Year = {2020},
Volume = {15},
Number = {10},
Month = {OCT},
Abstract = {Tropospheric and stratospheric tropical temperature trends in recent
   decades have been notoriously hard to simulate using climate models,
   particularly in the upper troposphere. Aside from the warming trend
   itself, this has broader implications, e.g. atmospheric circulation
   trends depend on latitudinal temperature gradients. In this study,
   tropical temperature trends in the CMIP6 models are examined, from 1979
   to 2014, and contrasted with trends from the RICH/RAOBCORE radiosondes,
   and the ERA5/5.1 reanalysis. As in earlier studies, we find considerable
   warming biases in the CMIP6 modeled trends, and we show that these
   biases are linked to biases in surface temperature. We also uncover
   previously undocumented biases in the lower-middle stratosphere: the
   CMIP6 models appear unable to capture the time evolution of
   stratospheric cooling, which is non-monotonic owing to the Montreal
   Protocol. Finally, using models with large ensembles, we show that their
   standard deviation in tropospheric temperature trends, which is due to
   internal variability alone, explains similar to 50\% (+/- 20\%) of that
   from the CMIP6 models.},
