@article{ WOS:000824299200001,
Author = {Ballarin, Andre S. and Anache, Jamil A. A. and Wendland, Edson},
Title = {Trends and abrupt changes in extreme rainfall events and their influence
   on design quantiles: a case study in Sao Paulo, Brazil},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2022},
Volume = {149},
Number = {3-4},
Pages = {1753-1767},
Month = {AUG},
Abstract = {The traditional approach to characterize extreme rainfall events is
   based on using past observed data to fit a probability distribution,
   which is used to associate a certain probability of occurrence to a
   rainfall magnitude, under a stationary assumption. Observed series,
   however, may exhibit trends and abrupt changes, resulting from the
   natural hydro climatic variability and the climate change context, which
   are not usually considered in the stationary modelling procedure and
   which may affect the characterization of extreme events. In this paper,
   it was evaluated the presence of these temporal changes in rainfall
   series and their impact to estimate design quantiles, using rainfall
   data of 317 stations, located in Sao Paulo State, Brazil. The results
   showed that extreme rainfall events are changing in the state.
   Low-return period events (up to 10 years) are intensifying. Moderate-
   and high-return period events, on the other hand, did not present a
   clear spatial pattern. Trends and abrupt changes may significantly
   influence the estimation of design quantiles. The presence of the former
   in rainfall series exhibited more influence on low-return period events,
   showing, however, no clear relationship with high-return period design
   quantiles. The presence of the latter seems to influence the
   characterization process, affecting estimated quantiles of all evaluated
   return periods. Such findings highlight the need to consider the
   presence of trends and abrupt changes and their influence on design
   quantiles for a more reliable characterization.},
