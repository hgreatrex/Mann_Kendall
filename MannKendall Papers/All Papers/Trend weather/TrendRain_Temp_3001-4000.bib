@article{ WOS:001181272000004,
Author = {Sharma, Kratika and Tiwari, Ritu and Wadhwani, Arun Kumar and
   Chaturvedi, Shobhit},
Title = {Spatiotemporal analysis of land surface temperature trends in Nashik,
   India: A 30-year study from 1992 to 2022},
Journal = {EARTH SCIENCE INFORMATICS},
Year = {2024},
Volume = {17},
Number = {3},
Pages = {2107-2128},
Month = {JUN},
Abstract = {Over the past thirty years, Nashik, India, witnessed rapid urbanization,
   elevating Land Surface Temperatures (LSTs). This study presents a
   comprehensive approach for analyzing the spatiotemporal dynamics of LST
   in Nashik by utilizing Thermal Infrared Radiation bands from LANDSAT
   datasets. Regional LST maps from mid-May in 1992, 2003, 2013, and 2022,
   produced using the single channel algorithm, analyze LST changes and
   distributions. Land Cover Maps are developed to analyse the LST changes
   across different Land Classes. Further, statistical relationships are
   established across LSTs and landscape indicators like Normalized
   Difference Built-Up Index (NDBI), Normalized Difference Bareness Index
   (NDBaI), Normalized Difference Vegetation Index (NDVI), and Normalized
   Difference Water Index (NDWI). A consistent increase in citywide LST is
   detected as the area coverage for hottest (40-45)degrees C LST band grew
   from 30.47 km2 in 1992 to 132.16 km2, 141.47 km2 and 174.85 km2 in 2003,
   2013 and 2022 respectively. In contrast the cooler (30-40)degrees C band
   coverages fell from 305.39 km2 in 1992 to 203.12 km2, 194.66 km2and
   144.82 km2 in 2003, 2013 and 2022 respectively. In 2022, an
   unprecedented 16.25 km2 area experienced extreme LST (45-50)degrees C, a
   trend unseen in prior decades. Noteworthy, Built-up areas and Barelands
   showed much higher (40-45)degrees C LSTs than vegetated areas and water
   bodies (30-40)degrees C. Statistical relations also affirm these
   patterns: Stronger LST corelations (rho = 0.66) to NDBI indicate higher
   temperatures in densely built-up areas. In comparison, negative LST
   correlations with NDVI (rho = -0.61) and NDWI (rho = -0.66) signify
   cooler zones with vegetation and water bodies. These findings emphasize
   water-rich areas with vegetation moderating temperatures, while built-up
   areas elevate heat. Thus, municipalities must prioritize awareness,
   green spaces, water conservation, cooling technologies, and zoning
   regulations to counteract this warming trend.},
