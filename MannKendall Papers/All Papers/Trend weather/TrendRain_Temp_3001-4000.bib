@article{ WOS:A1977DH72200003,
Author = {MADDOCK, L and SWANN, CL},
Title = {STATISTICAL-ANALYSIS OF SOME TRENDS IN SEA TEMPERATURE AND CLIMATE IN
   PLYMOUTH AREA IN LAST 70 YEARS},
Journal = {JOURNAL OF THE MARINE BIOLOGICAL ASSOCIATION OF THE UNITED KINGDOM},
Year = {1977},
Volume = {57},
Number = {2},
Pages = {317-338},
Abstract = {Records of more than 70 yr of offshore and inshore sea temperatures, air
   temperature, rainfall and `'westerly weather'' in the Plymouth area
   {[}England] were analyzed and correlated. The mean seasonal cycles are
   described and their amplitudes, phases and variability are compared.
   Possible mechanisms for the relationships are suggested. The changes
   from year to year were also analyzed, both on the basis of annual means
   or totals and individual values. The short-term variability is high but
   no dominant cyclic changes were found in the series of annual figures.
   However, most variables show trends over the period studied and although
   these are of relatively small amplitude they are sufficiently large to
