@article{ WOS:001302564600001,
Author = {Qiu, Weiteng and Collins, Matthew and Scaife, Adam A. and Santoso, Agus},
Title = {Tropical Pacific trends explain the discrepancy between observed and
   modelled rainfall change over the Americas},
Journal = {NPJ CLIMATE AND ATMOSPHERIC SCIENCE},
Year = {2024},
Volume = {7},
Number = {1},
Month = {AUG 29},
Abstract = {Understanding the causes for discrepancies between modelled and observed
   regional climate trends is important for improving present-day climate
   simulation and reducing uncertainties in future climate projections.
   Here, we analyse the performance of coupled climate models in
   reproducing regional precipitation trends during the satellite era. We
   find statistically significant observed drying in southwestern North
   America and wetting in the Amazon during the period 1979-2014.
   Historical climate model simulations do not capture these observed
   precipitation trends. We trace this discrepancy to the inability of
   coupled simulations to capture the observed Pacific trade wind
   intensification over this period. A linear adjustment of free running
   historical simulations, based on the observed strengthening of the
   Pacific trade winds and modeled ENSO teleconnections, explains the
   discrepancy in precipitation trends. Furthermore, both the Pacific trade
   wind trends and regional precipitation trends are reproduced in climate
   simulations with prescribed observed sea surface temperatures (SST),
   underscoring the role of the tropical Pacific SST patterns.},
