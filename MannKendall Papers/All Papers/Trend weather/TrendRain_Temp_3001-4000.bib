@article{ WOS:A1997XF40200010,
Author = {Lysenko, EV and Nelidova, GG and Prostova, AM},
Title = {Changes in the stratospheric and mesospheric thermal conditions during
   the last three decades .2. The evolution of a temperature trend},
Journal = {IZVESTIYA AKADEMII NAUK FIZIKA ATMOSFERY I OKEANA},
Year = {1997},
Volume = {33},
Number = {2},
Pages = {241-249},
Month = {MAR-APR},
Abstract = {Moving estimates of a 13-year temperature trend were derived in the
   height range of 25 to 75 km from monthly mean temperatures measured by
   rocket sounding over Hayes Island, Volgograd, Tumba, and Molodezhnaya
   stations during 1964-1994. A negative trend of upper and middle
   mesospheric temperatures was shown to have increased since the early
   1970s, but its magnitudes have been progressively decreasing since the
   first half of the 1980s. A stratospheric and lower mesospheric negative
   trend was shown to have been decreasing since the start of observations
   and reached positive magnitudes in recent years. The results obtained
   were compared with trend estimates based on satellite, lidar,
   radiophysical, and optical observations, as well as with data from the
   US rocketsonde network.},
