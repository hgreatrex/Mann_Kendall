Title = {Detection and monitoring of stratigraphic markers and temperature trends
   at the Greenland Ice Sheet Project 2 using passive-microwave
   remote-sensing data},
Journal = {JOURNAL OF GEOPHYSICAL RESEARCH-OCEANS},
