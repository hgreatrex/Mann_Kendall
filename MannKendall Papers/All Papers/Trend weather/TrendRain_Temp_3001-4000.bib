@article{ WOS:000314427500011,
Author = {Gonzalez, Manuel and Fontan, Almudena and Esnaola, Ganix and Collins,
   Michael},
Title = {Abrupt changes, multidecadal variability and long-term trends in sea
   surface temperature and sea level datasets within the southeastern Bay
   of Biscay},
Journal = {JOURNAL OF MARINE SYSTEMS},
