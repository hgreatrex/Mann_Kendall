@inproceedings{ WOS:000177601900043,
Author = {Singer, SF},
Book-Group-Author = {AMS
   AMS},
Title = {Disparity of temperature trends (1979-99) of atmosphere and surface: Can
   we trust the surface data?},
Booktitle = {12TH SYMPOSIUM ON GLOBAL CHANGE AND CLIMATE VARIATIONS},
Year = {2001},
Pages = {136-139},
Note = {12th Symposium on Global Change and Climate Variations, ALBUQUERQUE, NM,
   JAN 14-18, 2001},
Organization = {Amer Meteorol Soc},
Abstract = {A National Research Council report {[}NRC 2000] has highlighted the
   disparity between surface temperature trends (from thermometer readings)
   versus those from MSU instruments in weather satellites and radiosondes
   carried in weather balloons. The global surface record shows a
   substantial warming trend in the last twenty years while the atmospheric
   data from both MSU and radiosondes show little if any warming. As is
   well understood, climate models predict the opposite, namely a stronger
   warming trend for the troposphere than for the surface.
   In order to better understand the disparity between surface and
   atmospheric data, we investigated the latitude, altitude, and seasonal
   dependence of all the trends. We also calculated separate trend values
   for two periods, `79-'98 and `89-'99.
   We discuss several explanations for the observed disparity: ozone
   depletion, volcanism, ENSO, and increase in GH gases. After rejecting
   these, we conclude that PDO may be a candidate. But it is also possible
   that the global surface trends are not credible; they need further
   scrutiny. Well-controlled surface temperature data for the United States
   do not show an appreciable post-1940 warming, in contrast to the global
   surface data. In addition, proxy data from tree rings, ice cores, etc.
   show no post-1940 warming: Many even show a cooling trend after 1940.},
