@article{ WOS:000449299300019,
Author = {Grbec, Branka and Matic, Frano and Paklar, Gordana Beg and Morovic, Mira
   and Popovic, Ruzica and Vilibic, Ivica},
Title = {Long-Term Trends, Variability and Extremes of In Situ Sea Surface
   Temperature Measured Along the Eastern Adriatic Coast and its
   Relationship to Hemispheric Processes},
Journal = {PURE AND APPLIED GEOPHYSICS},
Year = {2018},
Volume = {175},
Number = {11},
Pages = {4031-4046},
Month = {NOV},
Abstract = {This paper examines long-term series of in situ sea surface temperature
   (SST) data measured at nine coastal and one open sea stations along the
   eastern Adriatic Sea for the period 1959-2015. Monthly and yearly
   averages were used to document SST trends and variability, while
   clustering and connections to hemispheric indices were achieved by
   applying the Principal Component Analysis (PCA) and Self-Organizing Maps
   (SOM) method. Both PCA and SOM revealed the dominance of temporal
   changes with respect to the effects of spatial differences in SST
   anomalies, indicating the prevalence of hemispheric processes over local
   dynamics, such as bora wind spatial inhomogeneity. SST extremes were
   connected with blocking atmospheric patterns. A substantial warming
   between 1979 and 2015, in total exceeding 1 degrees C, was preceded by a
   period with a negative SST trend, implying strong multidecadal
   variability in the Adriatic. The strongest connection was found between
   yearly SST and the East Atlantic (EA) pattern, while North Atlantic
   Oscillation (NAO) and East Atlantic/West Russia (EAWR) patterns were
   found to also affect February SST values. Quantification of the Adriatic
   SST and their connection to hemispheric indices allow for more precise
   projections of future SST, considered to be rather important for
   Adriatic thermohaline circulation, biogeochemistry and fisheries, and
   sensitive to ongoing climate change.},
