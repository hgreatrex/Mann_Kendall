@article{ WOS:000964212900003,
Author = {Sa'adi, Zulfaqar and Yusop, Zulkifli and Alias, Nor Eliza},
Title = {Long-term homogeneity and trend analysis of seasonality and extreme
   rainfall under the influence of climate change in Johor River basin,
