@article{ WOS:000853347200011,
Author = {Bagatinsky, V. A. and Diansky, N. A.},
Title = {Contributions of Climate Changes in Temperature and Salinity to the
   Formation of North Atlantic Thermohaline Circulation Trends in 1951-2017},
Journal = {MOSCOW UNIVERSITY PHYSICS BULLETIN},
Year = {2022},
Volume = {77},
Number = {3},
Pages = {564-580},
Month = {JUN},
Abstract = {To clarify the role of thermohaline factors in the formation of climatic
   trends in the North Atlantic (NA) thermohaline circulation, the
   contributions of changes in potential temperature and salinity to trends
   of the stream function of the Atlantic meridional overturning
   circulation (AMOC) were determined. To do this, using the INMOM
   (Institute of Numerical Mathematics Ocean Model) and data of the EN4
   electronic atlas (MetOffice, United Kingdom), the NA circulation was
   reconstructed by the diagnosis-adjustment method for the characteristic
   periods of 1951-2017, 1951-1990, and 1991-2017. In two selected periods
   of 1951-2017 and 1951-1990, all the main features of changes in the AMOC
   are explained mainly by temperature changes, which prevail over changes
   in salinity. At the same time, the observed trend towards weakening of
   the main core of the AMOC stream function from 1991 to 2017 is caused by
   changes in salinity, and the strengthening of the main core of the AMOC
   stream function from 1951 to 1990 is caused by changes in potential
   temperature. A relationship has been found between the trends of the
   AMOC stream function maximum at 38.5 degrees N and differences between
   zonally-averaged sea levels at 34.5 degrees N and 57.5 degrees N, which
   can possibly be used as the variability index of the AMOC stream
   function.},
