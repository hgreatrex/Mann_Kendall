@article{ WOS:000605309900001,
Author = {Bai, X. J. and Kang, H. X. and Wang, P. X. and Liu, Z.},
Title = {Effectiveness of upscaling the vegetation temperature condition index
   retrieved from Landsat data using an algorithm combining the trend
