@article{ WOS:000378773200021,
Author = {Garcia-Garizabal, Iker and Romero, Paola},
Title = {Homogeneous temperature series for thermal characterization and
   detection of trends in the Province of Santa Elena, Ecuador},
Journal = {INTERNATIONAL JOURNAL OF CLIMATOLOGY},
Year = {2016},
Volume = {36},
Number = {8},
Pages = {3129-3138},
Month = {JUN 30},
Abstract = {The generation of quality climatic series is the first step in the
   analysis of climate characteristics and behaviour in any region of the
   world. This article approaches the construction of homogeneous average
   temperature series for the period 1982-2011, for the study of spatial
   distribution and temporal evolution in the Province of Santa Elena, in
   the most western point of continental Ecuador in South America. Despite
   the low variability registered in the temperatures of the Province,
   these were higher and more homogeneous in the inland region than on the
   coast and almost 3 degrees C higher in the trimesters of the rainy
   season December-January-February (DJF) and March-April-May (MAM) than in
   the trimesters of the dry season June-July-August (JJA) and
   September-October-November (SON). Trend analysis detected positive
   increasing rates for the temperature of the province, with values three
   times higher during DJF and MAM in comparison with SON and annual
   average trend of 0.33 degrees C decade(-1) in the region. The
   construction of homogeneous average temperature series in the Province
   of Santa Elena has been an important step towards the spatial-temporal
   characterization of this peculiar climatic zone. These series can also
   be employed as base scenarios for the development of climate change
   studies in the region.},
