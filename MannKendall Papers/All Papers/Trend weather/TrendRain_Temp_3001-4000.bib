@article{ WOS:000225662100010,
Author = {Polonsky, AB and Lovenkova, EA},
Title = {Temperature and salinity trends in the active layer of the Black Sea in
   the second half of the 20th century and their possible causes},
Journal = {IZVESTIYA ATMOSPHERIC AND OCEANIC PHYSICS},
Year = {2004},
Volume = {40},
Number = {6},
Pages = {747-755},
Month = {NOV-DEC},
Abstract = {On the basis of analysis of archival hydrologic data, the cooling and
   freshening of the upper layer in the western part of the Black Sea was
   shown to occur in the cold period of the year during the 1960s-1990s. In
   the northern part of the Cape Chersonese-Bosporus Strait secular
   section, their values attained -1degreesC/10 yr and -0.1\%/10 yr,
   respectively. An increase in the winter discharge of the main rivers
   flowing into the Black Sea and additional turbulent entrainment of
   thermoclinic waters into the upper mixed layer are among the main
   factors responsible for this phenomenon. It is not inconceivable that a
   long-term increase in precipitation over some parts of the Black Sea
   water area also plays an important role. The linear trends of
   temperature and salinity in the upper layer of the Black Sea separated
