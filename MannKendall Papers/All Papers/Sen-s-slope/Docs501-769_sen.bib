@article{ WOS:000434815300004,
Author = {Das, Jayanta and Bhattacharya, Sudip Kumar},
Title = {Trend analysis of long-term climatic parameters in Dinhata of Koch Bihar
   district, West Bengal},
Journal = {SPATIAL INFORMATION RESEARCH},
