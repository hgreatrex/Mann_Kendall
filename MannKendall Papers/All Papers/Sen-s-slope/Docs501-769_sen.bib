@article{ WOS:000938780800001,
Author = {Ankrah, Johnson and Monteiro, Ana and Madureira, Helena},
Title = {Extreme Temperature and Rainfall Events and Future Climate Change
   Projections in the Coastal Savannah Agroecological Zone of Ghana},
Journal = {ATMOSPHERE},
Year = {2023},
