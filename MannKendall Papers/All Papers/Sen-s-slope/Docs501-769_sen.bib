@article{ WOS:000644122400012,
Author = {Xu, Jinqin and Zeng, Yan and Qiu, Xinfa and He, Yongjian and Shi,
   Guoping and Zhu, Xiaochen},
Title = {Aridity Changes and Related Climatic Drivers in the Drylands of China
   during 1960-2019},
