Language = {English},
Affiliation = {Mishra, AK (Corresponding Author), Clemson Univ, Glenn Dept Civil Engn, Clemson, SC 29634 USA.
   Alhathloul, Saleh H.; Khan, Abdul A.; Mishra, Ashok K., Clemson Univ, Glenn Dept Civil Engn, Clemson, SC 29634 USA.},
DOI = {10.1007/s00704-021-03533-z},
EarlyAccessDate = {JAN 2021},
