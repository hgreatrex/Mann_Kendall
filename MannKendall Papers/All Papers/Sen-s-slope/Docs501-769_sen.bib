@article{ WOS:000455314300105,
Author = {Masud, Mohammad Badrul and Ferdous, Jannatul and Faramarzi, Monireh},
Title = {Projected Changes in Hydrological Variables in the Agricultural Region
   of Alberta, Canada},
Journal = {WATER},
