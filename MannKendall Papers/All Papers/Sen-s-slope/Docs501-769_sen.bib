@article{ WOS:000749519200001,
Author = {Som, Kalyan Sundar and Dey, Mousumi},
Title = {Analysis and forecasting of rainfall trends in semi-arid Bundelkhand
   region of Madhya Pradesh, India: using statistical methods},
Journal = {ENVIRONMENTAL EARTH SCIENCES},
