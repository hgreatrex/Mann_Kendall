@article{ WOS:000351770100005,
Author = {Sabzevari, Azadeh Arbabi and Zarenistanak, Mohammad and Tabari, Hossein
   and Moghimi, Shokat},
Title = {Evaluation of precipitation and river discharge variations over
   southwestern Iran during recent decades},
Journal = {JOURNAL OF EARTH SYSTEM SCIENCE},
