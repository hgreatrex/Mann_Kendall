@article{ WOS:000376936100014,
Author = {He Yun-ling and Zhang Yi-ping and Wu Zhi-jie},
Title = {ANALYSIS OF CLIMATE VARIABILITY IN THE JINSHA RIVER VALLEY},
Journal = {JOURNAL OF TROPICAL METEOROLOGY},
