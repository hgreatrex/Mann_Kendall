@article{ WOS:000417721100018,
Author = {Ongoma, Victor and Chen, Haishan and Gao, Chujie and Nyongesa, Aston
   Matwai and Polong, Francis},
Title = {Future changes in climate extremes over Equatorial East Africa based on
   CMIP5 multimodel ensemble},
