@article{ WOS:001089099300001,
Author = {Thach, Tran Tuan},
Title = {Variability and trend detection of water discharge over the past 60
   years in the Vietnamese Hieu River Basin},
Journal = {WATER PRACTICE AND TECHNOLOGY},
