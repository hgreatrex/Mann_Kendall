Volume = {73},
Number = {2},
Pages = {413-422},
Month = {APR},
Abstract = {The present study aims to analyse the long period trend of precipitation
   and temperature in West district of Sikkim, India. For this purpose
   daily precipitation and temperature data of more than 100 years were
