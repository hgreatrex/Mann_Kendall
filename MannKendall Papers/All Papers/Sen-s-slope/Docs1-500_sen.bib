@article{ WOS:000569248300022,
Author = {Bissenbayeva, Sanim and Abuduwaili, Jilili and Issanova, Gulnura and
   Samarkhanov, Kanat},
Title = {Characteristics and Causes of Changes in Water Quality in the Syr Darya
   River, Kazakhstan},
