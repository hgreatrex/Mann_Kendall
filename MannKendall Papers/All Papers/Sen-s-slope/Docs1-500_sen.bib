@article{ WOS:000711325900002,
Author = {Beyene, Tegegn Kassa and Jain, Manoj Kumar and Yadav, Brijesh K. and
   Agarwal, Ankit},
Title = {Multiscale investigation of precipitation extremes over Ethiopia and
   teleconnections to large-scale climate anomalies},
