@article{ WOS:000817940900001,
Author = {Yin, Jina and Medellin-Azuara, Josue and Escriva-Bou, Alvar},
Title = {Hierarchical clustering and regional drought assessment of groundwater
   levels in heavily drafted aquifers},
Journal = {HYDROLOGY RESEARCH},
Year = {2022},
