@article{ WOS:000686080500001,
Author = {Irsyad, Fadli and Oue, Hiroki},
Title = {Predicting future dry season periods for irrigation management in West
   Sumatra, Indonesia},
Journal = {PADDY AND WATER ENVIRONMENT},
