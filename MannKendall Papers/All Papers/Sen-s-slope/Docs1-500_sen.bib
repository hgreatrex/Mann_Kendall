@article{ WOS:001260426200001,
Author = {Roy, Pankaj Kumar and Sarkar, Ahana and Saha, Shilpa and Ray, Poulami
   and Roy, Malabika Biswas},
Title = {Spatio-temporal analysis of precipitation in eastern India for last
   three decades},
