@article{ WOS:000396165100011,
Author = {Anjum, Raheel and He, Xiubin and Tanoli, Javed Iqbal and Raza, Syed
   Turab},
Title = {Contemporary Temperature Fluctuation in Urban Areas of Pakistan},
