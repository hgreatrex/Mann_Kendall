@article{ WOS:001232223500001,
Author = {Manea, Ancuta and Birsan, Marius-Victor and Dima, Viorica and Havris,
   Loredana-Elena},
Title = {Comparative Analysis of Land and Air Temperature in Romania since A.D.
   1961},
Journal = {LAND},
Year = {2024},
Volume = {13},
Number = {5},
Month = {MAY},
Abstract = {Daily time series with continuous records of mean air and soil
   temperature from 127 meteorological stations-fairly distributed over the
   country-were used to compute monthly temperature trends, as well as
   changes in the timing of the first and the last frost days over Romania
   since 1961. Results show that the frequency of the number of days with
   daily temperature averages below 0 degrees C in case of air and soil
   surface temperature is stable for most months, except for January, when
   (for both soil and air temperature), the number of days with a
   temperature below 0 degrees C is decreasing in the majority of the
   stations. The occurrence of the first day with (mean air and soil
   surface) temperatures below 0 degrees C, presents a delay in the south,
   south-east, and west, and an earlier occurrence in eastern and central
   regions. The occurrence of the last day with a mean air and soil surface
   temperature below 0 degrees C shows a stable trend for most stations
   (except for some small areas in the north, south-east and south-west of
