@article{ WOS:000472004600022,
Author = {Desai, Sujeet and Singh, D. K. and Islam, Adlul and Sarangi, A. and
   Khanna, Manoj},
Title = {Trend analysis of climatic variables in Betwa river basin},
Journal = {INDIAN JOURNAL OF AGRICULTURAL SCIENCES},
