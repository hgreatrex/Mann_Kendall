@article{ WOS:000398936200020,
Author = {Akinsanola, A. A. and Ogunjobi, K. O.},
Title = {Recent homogeneity analysis and long-term spatio-temporal rainfall
   trends in Nigeria},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
