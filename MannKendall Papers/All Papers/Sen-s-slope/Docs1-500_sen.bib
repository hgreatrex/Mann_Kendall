@article{ WOS:000910980600005,
Author = {Goodarzi, Mohammad Reza and Sabaghzadeh, Maryam and Mokhtari, Mohammad
   Hossein},
Title = {Impacts of aspect on snow characteristics using remote sensing from 2000
   to 2020 in Ajichai-Iran},
Journal = {COLD REGIONS SCIENCE AND TECHNOLOGY},
Year = {2022},
Volume = {204},
Month = {DEC},
Abstract = {Snow is one of the most important sources of fresh water; however, its
   availability in high altitudes where there are few weather stations,
   makes it difficult to investigate its characteristics. On the other
   hand, although satellite imaging has opened new windows for
   investigating snow, studying it in the long term has not been feasible
   since retrieving and processing satellite images are extremely
   time-consuming. Emergence of Google Earth Engine accelerated retrieving
   and processing of satellite images so that processing 5000 images can be
   completed in a quite short time. The present study aims to use Google
   Earth Engine to investigate the effect of elevation and aspect on snow
   depth, snow water equivalent, snow cover area, snow density, snow
   permittivity and precipitation. Furthermore, the correlation coefficient
   of the above snow-related parameters was calculated using Pearson
   Correlation Coefficient formula. Findings revealed a correlation of
   above 0.9 between snow depth and snow water equivalent. Later, using The
   Mann Kendall Trend Test and the Sen's Slope Estimator, all four
   parameters were analyzed from 2000 to 2020. According to the results,
   with increasing the altitude, the rate of Sen Slope decreased in
   northern slopes and showed an increasing trend on the southern
   slopes.Also, the snow depth and snow water equivalent in the southern
   slopes at an altitude below 2000 m were 1.5 times and at an altitude of
   above 2000 m were almost 6 times that of the northern slopes.},
