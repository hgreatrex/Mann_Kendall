@article{ WOS:000481444200005,
Author = {Subba, Sunil and Ma, Yaoming and Ma, Weiqiang},
Title = {Spatial and Temporal Analysis of Precipitation Extremities of Eastern
   Nepal in the Last Two Decades (1997-2016)},
Journal = {JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES},
Year = {2019},
Volume = {124},
