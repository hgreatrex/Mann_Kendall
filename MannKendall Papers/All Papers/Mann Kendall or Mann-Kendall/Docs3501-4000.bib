@article{ WOS:000509476600028,
Author = {Yang, Kangkang and Wang, Wei and Shi, Jun and Sun, Guorui and Zheng,
   Kaikai and Zhang, Mingyue},
Title = {Investigation on Single Tube CFST Arch Models by Modeling Structural
   Stressing State Based on NSF Method},
Journal = {APPLIED SCIENCES-BASEL},
