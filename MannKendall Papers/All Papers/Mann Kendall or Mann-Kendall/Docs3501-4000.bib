@article{ WOS:000353220700010,
Author = {Yao, Junqiang and Chen, Yaning},
Title = {Trend analysis of temperature and precipitation in the Syr Darya Basin
   in Central Asia},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2015},
