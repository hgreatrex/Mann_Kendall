@article{ WOS:000277383700009,
Author = {Liu, Dedi and Chen, Xiaohong and Lian, Yanqing and Lou, Zhanghua},
Title = {Impacts of climate change and human activities on surface runoff in the
   Dongjiang River basin of China},
