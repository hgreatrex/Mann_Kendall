@article{ WOS:000909514000013,
Author = {Chen, Xieling and Cheng, Gary and Zou, Di and Zhong, Baichang and Xie,
   Haoran},
Title = {Artificial Intelligent Robots for Precision Education: A Topic Modeling-
   Based Bibliometric Analysis},
