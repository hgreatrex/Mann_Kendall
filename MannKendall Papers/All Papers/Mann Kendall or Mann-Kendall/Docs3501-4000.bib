@article{ WOS:000251894600003,
Author = {Jiang, Tong and Su, Buda and Hartmann, Heike},
Title = {Temporal and spatial trends of precipitation and river flow in the
   Yangtze River Basin, 1961-2000},
Journal = {GEOMORPHOLOGY},
