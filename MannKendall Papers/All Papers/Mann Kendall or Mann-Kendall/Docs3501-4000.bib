@article{ WOS:000298769800002,
Author = {Kazys, Justas and Stankunavicius, Gintautas and Rimkus, Egidijus and
   Bukantis, Arunas and Valiukas, Donatas},
Title = {Long-range alternation of extreme high day and night temperatures in
   Lithuania},
