@article{ WOS:000241779100011,
Author = {Newell, Charles J. and Cowie, Iain and McGuire, Travis M. and McNab,
   Jr., Wait W.},
Title = {Multiyear temporal changes in chlorinated solvent concentrations at 23
   monitored natural attenuation sites},
Journal = {JOURNAL OF ENVIRONMENTAL ENGINEERING},
