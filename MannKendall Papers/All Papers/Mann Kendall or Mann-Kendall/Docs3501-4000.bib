@article{ WOS:001265024100014,
Author = {Marceta, Milica and Keca, Ljiljana and Jelic, Sreten},
Title = {ECONOMIC ASPECTS OF THE USE OF FORESTRY PRODUCTS FOR COMMERCIAL PURPOSES},
