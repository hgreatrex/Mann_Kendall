@article{ WOS:000941014300001,
Author = {Hailesilassie, Wondimu Tadiwos and Ayenew, Tenalem and Tekleab, Sirak},
Title = {A comparative study of drought characteristics using meteorological
   drought indices over the central main Ethiopian Rift},
Journal = {HYDROLOGY RESEARCH},
