@article{ WOS:000695811400005,
Author = {Gnjato, Slobodan and Popov, Tatjana and Adzic, Dragutin and Ivanisevic,
   Marko and Trbic, Goran and Bajic, Davorin},
Title = {Influence of climate change on river discharges over the Sava River
   watershed in Bosnia and Herzegovina},
Journal = {IDOJARAS},
