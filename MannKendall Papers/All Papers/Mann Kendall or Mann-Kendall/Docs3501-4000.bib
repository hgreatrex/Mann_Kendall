@article{ WOS:A1995TG07700002,
Author = {KARACA, M and TAYANC, M and TOROS, H},
Title = {EFFECTS OF URBANIZATION ON CLIMATE OF ISTANBUL AND ANKARA},
Journal = {ATMOSPHERIC ENVIRONMENT},
