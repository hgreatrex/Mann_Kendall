@inproceedings{ WOS:000313845902323,
Author = {Ding, Xiangyi and Jia, Yangwen},
Editor = {Iranpour, R and Zhao, J and Wang, A and Yang, FL and Li, X},
Title = {Fingerprint-based attribution study of climatic changes in the Hai River
   Basin of China},
Booktitle = {ADVANCES IN ENVIRONMENTAL SCIENCE AND ENGINEERING, PTS 1-6},
