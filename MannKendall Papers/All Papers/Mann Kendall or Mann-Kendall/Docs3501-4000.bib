@article{ WOS:000314037100023,
Author = {Tammets, Tiina and Jaagus, Jaak},
Title = {Climatology of precipitation extremes in Estonia using the method of
   moving precipitation totals},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
