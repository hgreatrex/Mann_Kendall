@article{ WOS:000651241600001,
Author = {Hessebo, Misrak Tamire and Woldeamanuel, Teshale and Tadesse, Menfese},
Title = {Spatial and temporal climate variability and change in the bilate
   catchment, central Rift Valley lakes region, Ethiopia},
Journal = {PHYSICAL GEOGRAPHY},
