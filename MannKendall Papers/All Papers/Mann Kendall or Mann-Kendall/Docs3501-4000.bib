@article{ WOS:000487900800015,
Author = {Huang, Jin and Zhang, Fangmin and Hu, Zhenghua and Chen, Shutao},
Title = {Climatology of rainfall erosivity during 1961-2012 in Jiangsu Province,
   southeast China},
Journal = {NATURAL HAZARDS},
Year = {2019},
