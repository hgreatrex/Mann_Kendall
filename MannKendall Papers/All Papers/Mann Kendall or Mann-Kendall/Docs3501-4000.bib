@article{ WOS:001163269000002,
Author = {Wei, Ting and Zhao, Xuehua},
Title = {Assessment of spatial-temporal variation of precipitation and
   meteorological drought in Shanxi province, China},
Journal = {NATURAL HAZARDS},
