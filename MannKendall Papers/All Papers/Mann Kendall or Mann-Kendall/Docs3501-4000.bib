@article{ WOS:000304718900004,
Author = {Back, Alvaro Jose and Della Bruna, Emilio and Vieira, Hamilton Justino},
Title = {Climate trends and production of grapes in the Region of Goethe Grape
   Valleys, Brazil},
Journal = {PESQUISA AGROPECUARIA BRASILEIRA},
