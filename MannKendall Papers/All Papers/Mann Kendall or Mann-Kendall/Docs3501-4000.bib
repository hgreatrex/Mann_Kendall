@article{ WOS:000617139900012,
Author = {Du Xinqiang and Chang Kaiyang and Lu Xiangqin},
Title = {Characteristics and causes of groundwater dynamic changes in Naoli River
   Plain, Northeast China},
Journal = {WATER SUPPLY},
