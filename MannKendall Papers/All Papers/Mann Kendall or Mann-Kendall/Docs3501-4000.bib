@article{ WOS:001149724500001,
Author = {Davtalab, Mehri and Bycenkiene, Steigvile and Bimbaite, Vilma},
Title = {Long-term spatial and temporal evaluation of the PM2.5 and PM10 mass
   concentrations in Lithuania},
Journal = {ATMOSPHERIC POLLUTION RESEARCH},
Year = {2023},
Volume = {14},
Number = {12},
Month = {DEC},
Abstract = {Evaluating the spatio-temporal trends of particulate mass concentration
   is beneficial for appraising the risk to human health and environmental
   quality. In this study, the long-term observational data of particulate
   matter mass concentration (PM2.5, PM10), and PM2.5/PM10 ratios'
   variability of sixteen EPA air pollution monitoring stations in
   Lithuania over 17 years (2006-2022), were analyzed. A seasonal-trend
   decomposition procedure using Loess and Mann-Kendall regression with
   Sen's slope was used to qualify long-term trends and seasonality. The
   Theil-Sen analysis estimated significant reductions of-2.66,-4.42,
   and-1.89\% per year for PM10, PM2.5, and PM2.5/PM10 ratios after 2013.
   The possible reasons for these changes are the reduction in liquid fuel
   con-sumption, the mitigation of energy sector emissions, as well as
   Lithuania's positive gross domestic product (GDP) in recent years. The
   results also revealed a significant increasing trend (+4.48\%/year) for
   PM10 in rural areas, due to increased agricultural emissions. Trend
   analysis was also applied to different quartiles. The upper and middle
   values of PM2.5 and the upper values of PM10, responsible for high PM
   mass concentration, underwent a significant decrease.},
