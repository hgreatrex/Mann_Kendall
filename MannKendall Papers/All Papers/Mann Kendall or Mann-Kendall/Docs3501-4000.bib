@article{ WOS:000556141400019,
Author = {Strohmenger, L. and Fovet, O. and Akkal-Corfini, N. and Dupas, R. and
   Durand, P. and Faucheux, M. and Gruau, G. and Hamon, Y. and Jaffrezic,
   A. and Minaudo, C. and Petitjean, P. and Gascuel-Odoux, C.},
Title = {Multitemporal Relationships Between the Hydroclimate and Exports of
   Carbon, Nitrogen, and Phosphorus in a Small Agricultural Watershed},
Journal = {WATER RESOURCES RESEARCH},
Year = {2020},
Volume = {56},
Number = {7},
Month = {JUL},
Abstract = {Agriculture affects the biogeochemical cycles of carbon, nitrogen, and
   phosphorus, leading to a deterioration of surface water quality. The
   increasing magnitude of climate change raises questions about potential
   additional or mitigating effects of climate change on this
   deterioration. One way to understand these potential effects is to
   cross-analyze the dynamics of nutrient concentrations and hydroclimatic
   variables at multiple time scales. Here, we used a 16-year data set,
   from a 5 km(2)agricultural watershed in France with a temperate oceanic
   climate, that contains a daily record of nutrient concentrations and
   hydroclimatic variables from 2002-2017. We calculated Mann-Kendall and
   Theil-Sen tests, Fourier transforms, and daily hydroclimatic
   distributions associated with extreme stream concentrations to
   investigate long-term trends, seasonal dynamics and their interannual
   variations, and the daily time scale, respectively. Dynamics of
   dissolved organic carbon (DOC) and nitrate (NO3) concentrations
   displayed opposite patterns at the three temporal scales, while soluble
   reactive phosphorus concentrations showed decoupled dynamics, related
   more to extreme hydrological events. Climate and past agricultural
   changes seem to have a synergetic effect that leads to long-term
   NO(3)decrease and DOC increase. Precipitation and, to a greater extent,
   watershed wetness controlled seasonal and event-driven dynamics.},
