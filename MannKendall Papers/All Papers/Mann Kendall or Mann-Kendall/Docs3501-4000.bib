@article{ WOS:000651427700003,
Author = {de Oliveira, Karla Lorrane and Ramos, Ramatisa Ladeia and Oliveira,
   Silvia Correa and Christofaro, Cristiano},
Title = {Water quality index and spatio-temporal perspective of a large Brazilian
   water reservoir},
