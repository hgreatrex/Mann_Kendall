@article{ WOS:000364629500007,
Author = {Roth, M. and Buishand, T. A. and Jongbloed, G.},
Title = {Trends in Moderate Rainfall Extremes: A Regional Monotone Regression
   Approach},
Journal = {JOURNAL OF CLIMATE},
Year = {2015},
