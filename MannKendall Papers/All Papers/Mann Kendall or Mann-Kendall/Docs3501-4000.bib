@article{ WOS:000589719000016,
Author = {Cui, Yongsheng and Pan, Chengzhong and Liu, Chunlei and Luo, Mingjie and
   Guo, Yahui},
Title = {Spatiotemporal variation and tendency analysis on rainfall erosivity in
   the Loess Plateau of China},
Journal = {HYDROLOGY RESEARCH},
