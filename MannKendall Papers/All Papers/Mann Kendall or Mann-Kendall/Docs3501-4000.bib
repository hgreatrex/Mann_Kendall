@article{ WOS:001049995400001,
Author = {Cao, Yanmin and Qian, Dongyue and Wang, Chongyu},
Title = {Quantitative traceability study on the water quality driving forces
   across cities in the Xiang River Basin},
Journal = {WATER SUPPLY},
