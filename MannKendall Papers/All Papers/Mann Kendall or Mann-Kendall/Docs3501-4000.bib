@article{ WOS:000437768700001,
Author = {Ionac, Nicoleta and Matei, Monica},
Title = {THE INFLUENCE OF EUROPEAN CLIMATE VARIABILITY MECHANISM ON AIR
   TEMPERATURES IN ROMANIA},
