@article{ WOS:000396103300020,
Author = {dos Santos, Gilsonley Lopes and Pereira, Marcos Gervasio and Delgado,
   Rafael Coll and Rodrigues Torres, Jose Luiz},
Title = {NATURAL REGENERATION IN ANTHROPOGENIC ENVIRONMENTS DUE TO AGRICULTURAL
   USE IN THE CERRADO, UBERABA, MG, BRAZIL},
Journal = {BIOSCIENCE JOURNAL},
