@article{ WOS:000473159300017,
Author = {Shank, Matthew K.},
Title = {Physicochemical Controls on Spatiotemporal Distribution and Benthic Mat
   Severity of <i>Didymosphenia geminata</i> in Pine Creek, an Unregulated
   Watershed in Northern Pennsylvania},
Journal = {NORTHEASTERN NATURALIST},
