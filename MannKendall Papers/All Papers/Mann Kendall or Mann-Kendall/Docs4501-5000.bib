@article{ WOS:000218691100008,
Author = {Cheo, Ambe Emmanuel and Amankwah, Emmanuel and Techoro, Prosper Somah},
Title = {Water Harvesting: A Potential Means for Water Security in the Far North
   Region of Cameroon},
