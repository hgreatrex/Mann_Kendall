@article{ WOS:000306748500010,
Author = {Sang, Yan-Fang and Wang, Zhong-Gen and Li, Zong-Li},
Title = {Discrete Wavelet Entropy Aided Detection of Abrupt Change: A Case Study
   in the Haihe River Basin, China},
Journal = {ENTROPY},
