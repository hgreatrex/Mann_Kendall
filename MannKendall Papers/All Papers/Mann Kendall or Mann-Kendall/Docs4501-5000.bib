@article{ WOS:001020498800001,
Author = {Faghih, Homayoun and Behmanesh, Javad},
Title = {Spatiotemporal simulation of annual precipitation in the Urmia Lake
   basin},
Journal = {STOCHASTIC ENVIRONMENTAL RESEARCH AND RISK ASSESSMENT},
