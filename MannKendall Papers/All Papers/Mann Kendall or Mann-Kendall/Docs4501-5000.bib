@article{ WOS:000345096800033,
Author = {Birsan, Marius-Victor and Dumitrescu, Alexandru and Micu, Dana Magdalena
   and Cheval, Sorin},
Title = {Changes in annual temperature extremes in the Carpathians since AD 1961},
