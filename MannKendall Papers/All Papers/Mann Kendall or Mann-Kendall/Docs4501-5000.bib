@article{ WOS:000565479900001,
Author = {Nunez-Gonzalez, Gerardo},
Title = {Analysis of the trends in precipitation and precipitation concentration
   in some climatological stations of Mexico from 1960 to 2010},
Journal = {NATURAL HAZARDS},
