@article{ WOS:000373598200009,
Author = {Singh, Dharmaveer and Jain, Sanjay Kumar and Gupta, Rajan Dev and Kumar,
   Sudhir and Rai, Shive Prakash and Jain, Neha},
Title = {Analyses of Observed and Anticipated Changes in Extreme Climate Events
   in the Northwest Himalaya},
