@article{ WOS:000963072400002,
Author = {Bayer-Altin, Tuerkan},
Title = {Long-term trends and variability in air temperatures of central Anatolia
   region, Turkey, for the period 1960-2021},
Journal = {ACTA GEOPHYSICA},
