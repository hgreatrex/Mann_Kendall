@article{ WOS:000573792000001,
Author = {Endale, B. Wubishaw and Simphiwe, E. Mini and Yimer, A. Abegaz},
Title = {Trends in climate extremes at local farming calendar timescale: evidence
   from Merti District, Ethiopia},
Journal = {MODELING EARTH SYSTEMS AND ENVIRONMENT},
Year = {2021},
