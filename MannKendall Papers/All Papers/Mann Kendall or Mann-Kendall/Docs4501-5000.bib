@article{ WOS:000709188300004,
Author = {Mohammed, Safwan and Gill, Abid Rashid and Alsafadi, Karam and Hijazi,
   Omar and Yadav, Krishna Kumar and Hasan, Mohd Abul and Khan, Afzal
   Husain and Islam, Saiful and Cabral-Pinto, Marina M. S. and Harsanyi,
