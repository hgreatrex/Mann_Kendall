@article{ WOS:000467661600010,
Author = {Yusoff, Mohd Famey and Latif, Mohd Talib and Juneng, Liew and Khan, Md
   Firoz and Ahamad, Fatimah and Chung, Jing Xiang and Mohtar, Anis Asma
   Ahmad},
Title = {Spatio-temporal assessment of nocturnal surface ozone in Malaysia},
Journal = {ATMOSPHERIC ENVIRONMENT},
