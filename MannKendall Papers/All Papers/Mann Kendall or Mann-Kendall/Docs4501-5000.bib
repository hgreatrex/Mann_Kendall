@article{ WOS:001070331300001,
Author = {Pawar, Uttam and Try, Sophal and Muttil, Nitin and Rathnayake, Upaka and
   Suppawimut, Worawit},
Title = {Frequency and trend analyses of annual peak discharges in the Lower
   Mekong Basin},
