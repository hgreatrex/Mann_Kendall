@article{ WOS:000345389600008,
Author = {Lin, Kairong and Lian, Yanqing and Chen, Xiaohong and Lu, Fan},
Title = {Changes in runoff and eco-flow in the Dongjiang River of the Pearl River
   Basin, China},
Journal = {FRONTIERS OF EARTH SCIENCE},
