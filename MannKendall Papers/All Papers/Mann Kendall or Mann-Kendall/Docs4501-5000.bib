@article{ WOS:000965873300004,
Author = {Yang, Yong and Sun, Huaiwei and Zhu, Modi and Wang, Jingfeng and Zhang,
   Wenxin},
Title = {An R package of maximum entropy production model to estimate 41 years of
   global evapotranspiration},
Journal = {JOURNAL OF HYDROLOGY},
