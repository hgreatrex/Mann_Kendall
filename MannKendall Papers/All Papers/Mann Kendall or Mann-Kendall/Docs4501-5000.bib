@article{ WOS:000404568000013,
Author = {Ni, Xiliang and Xie, Jianfeng and Zhou, Yuke and Gao, Xizhang and Ding,
   Lin},
Title = {Evaluating Vegetation Growing Season Changes in Northeastern China by
   Using GIMMS LAI3g Data},
Journal = {CLIMATE},
