@article{ WOS:000802610400001,
Author = {Ahmed, Syed Imran and Rudra, Ramesh and Goel, Pradeep and Amili, Arezoo
   and Dickinson, Trevor and Singh, Kamal and Khan, Alamgir},
Title = {Change in Winter Precipitation Regime across Ontario, Canada},
Journal = {HYDROLOGY},
