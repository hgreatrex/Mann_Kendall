@article{ WOS:000732549900004,
Author = {Gorai, Sanjoy and Ratha, Dwarikanath and Dhir, Amit},
Title = {Adapting Rainfall Variability to Flood Risk: A Case Study of the Ghaggar
   River Basin},
Journal = {JOURNAL OF THE GEOLOGICAL SOCIETY OF INDIA},
Year = {2021},
