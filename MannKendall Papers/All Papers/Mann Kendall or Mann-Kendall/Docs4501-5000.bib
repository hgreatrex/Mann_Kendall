@article{ WOS:001173090300001,
Author = {Besnier, Jessica and Getirana, Augusto and Beaudoing, Hiroko and
   Lakshmi, Venkataraman},
Title = {Characterizing the 2019-2021 drought in La Plata River Basin with GLDAS
   and SMAP},
