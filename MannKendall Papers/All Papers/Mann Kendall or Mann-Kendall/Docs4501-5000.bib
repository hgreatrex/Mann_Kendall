@article{ WOS:000367734800036,
Author = {Pescio, Andres E. and Martin, Paula B. and Dragani, Walter C.},
Title = {Wind speed trends over the southwestern Atlantic Ocean, between 33° and
   50°S},
Journal = {INTERNATIONAL JOURNAL OF CLIMATOLOGY},
