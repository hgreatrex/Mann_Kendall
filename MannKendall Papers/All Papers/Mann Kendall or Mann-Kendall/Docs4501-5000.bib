@article{ WOS:001125590300001,
Author = {Karamidehkordi, Esmail and Sadati, Seyed Abdolhamid Hashemi and Tajvar,
   Yahya and Mirmousavi, Seyed Hossein},
Title = {Climate change vulnerability and resilience strategies for citrus
   farmers},
