@article{ WOS:000416204800066,
Author = {Wang, Kui and Masiol, Mauro and Thimmaiah, Devraj and Zhang, Yuanyuan
   and Hopke, Philip K.},
Title = {Performance Evaluation of Two 25 kW Residential Wood Pellet Boiler
   Heating Systems},
