@article{ WOS:000379136400001,
Author = {Eugenia Scarpati, Olga and Daniel Capriolo, Alberto},
Title = {Droughts: recurrence, classification and distribution in the Argentina
   Pampas},
Journal = {CUADERNOS GEOGRAFICOS},
