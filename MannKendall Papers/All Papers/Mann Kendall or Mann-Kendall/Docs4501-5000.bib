@article{ WOS:000243419500002,
Author = {Gagnon, Alexandre S. and Gough, William A.},
Title = {East-west asymmetry in long-term trends of landfast ice thickness in the
   Hudson Bay region, Canada},
Journal = {CLIMATE RESEARCH},
