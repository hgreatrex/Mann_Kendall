@article{ WOS:000912370500002,
Author = {Silva, Josiel dos Santos and Cabral Jr, Jorio Bezerra and Rodrigues,
   Daniele Torres and Silva, Fabricio Daniel dos Santos},
Title = {Climatology and significant trends in air temperature in Alagoas,
   Northeast Brazil},
