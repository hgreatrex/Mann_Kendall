@article{ WOS:000373612400014,
Author = {Zhang, Qiang and Zheng, Yongjie and Singh, Vijay P. and Xiao, Mingzhong
   and Liu, Lin},
Title = {Entropy-based spatiotemporal patterns of precipitation regimes in the
   Huai River basin, China},
