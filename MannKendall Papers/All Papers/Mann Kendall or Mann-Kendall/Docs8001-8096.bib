@article{ WOS:000670936800001,
Author = {Kalu, Ikechukwu and Ndehedehe, Christopher E. and Okwuashi, Onuwa and
   Eyoh, Aniekan E.},
Title = {Assessing Freshwater Changes over Southern and Central Africa
   (2002-2017)},
