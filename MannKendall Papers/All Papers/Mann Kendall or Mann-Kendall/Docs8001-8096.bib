@article{ WOS:000581845700025,
Author = {El Kenawy, Ahmed M. and Al Buloshi, Ali and Al-Awadhi, Talal and Al
   Nasiri, Noura and Navarro-Serrano, Francisco and Alhatrushi, Salim and
