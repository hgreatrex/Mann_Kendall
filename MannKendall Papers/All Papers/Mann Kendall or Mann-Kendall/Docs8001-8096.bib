@article{ WOS:000393954900063,
Author = {Schemm, S. and Sprenger, M. and Martius, O. and Wernli, H. and Zimmer,
   M.},
Title = {Increase in the number of extremely strong fronts over Europe? A study
   based on ERA-Interim reanalysis (1979-2014)},
Journal = {GEOPHYSICAL RESEARCH LETTERS},
Year = {2017},
Volume = {44},
Number = {1},
Pages = {553-561},
Month = {JAN},
Abstract = {Evidence is presented that the frequency of extremely strong fronts,
   which occur mainly in summer, has increased over Europe in ERA-Interim
   reanalyses data (1979-2014). Fronts are defined using a common detection
   scheme based on gradients of equivalent potential temperature ((e)) at
   850hPa. The frequency increase is due to increasing atmospheric
   humidity, which in turn is reported as statistically significant over
   Europe in the Fifth Assessment Report of the Intergovernmental Panel on
   Climate Change (IPCC AR5). There is no trend in the frequency of
   extremely strong fronts in North America where humidity trends are,
   according to the IPCC AR5, close to zero. Because frontal precipitation
   increases with frontal strength, measured by the (e) gradient, the
   increase in the number of extremely strong fronts may help explain
   regional patterns of longer-term trends in strong precipitation events.},
