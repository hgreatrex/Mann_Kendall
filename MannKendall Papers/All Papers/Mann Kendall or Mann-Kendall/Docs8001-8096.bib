Author = {Wang, Qingming and Wang, Jianhua and Zhao, Yong and Li, Haihong and
   Zhai, Jiaqi and Yu, Zhongbo and Zhang, Shuyu},
Title = {Reference evapotranspiration trends from 1980 to 2012 and their
   attribution to meteorological drivers in the three-river source region,
   China},
