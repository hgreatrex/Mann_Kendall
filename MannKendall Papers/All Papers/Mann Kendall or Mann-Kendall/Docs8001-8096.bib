@article{ WOS:000363402800022,
Author = {Rice, Joshua S. and Emanuel, Ryan E. and Vose, James M. and Nelson,
   Stacy A. C.},
Title = {Continental US streamflow trends from 1940 to 2009 and their
   relationships with watershed spatial characteristics},
