@article{ WOS:001151413300001,
Author = {Iqbal, Usman and Nabi, Ghulam and Iqbal, Mudassar and Masood, Muhammad
   and Arshed, Abu Bakar and Saifullah, Muhammad and Shahid, Muhammad},
Title = {Groundwater Age and Origin and Its Relation with Anthropogenic and
   Climatic Factors},
