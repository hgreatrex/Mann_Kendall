@article{ WOS:000285780900014,
Author = {Krichak, S. O. and Breitgand, J. S. and Samuels, R. and Alpert, P.},
Title = {A double-resolution transient RCM climate change simulation experiment
   for near-coastal eastern zone of the Eastern Mediterranean region},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
