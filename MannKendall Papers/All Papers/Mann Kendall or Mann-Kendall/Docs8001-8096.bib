@article{ WOS:001022340600001,
Author = {Chen, Tso-Hsuan and Vlachos, Dionisios G. and Caratzoulas, Stavros},
Title = {Ab Initio Molecular Dynamics Study of Pd Nucleation on a
   γ-Al<sub>2</sub>O<sub>3</sub> Surface},
