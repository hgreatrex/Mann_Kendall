@inproceedings{ WOS:000400168900068,
Author = {Tamaddun, Kazi Ali and Kalra, Ajay and Ahmad, Sajjad},
Editor = {Pathak, CS and Reinhart, D},
Title = {Patterns and Periodicities of the Continental US Streamflow Change},
Booktitle = {WORLD ENVIRONMENTAL AND WATER RESOURCES CONGRESS 2016: HYDRAULICS AND
   WATERWAYS AND HYDRO-CLIMATE/CLIMATE CHANGE},
Year = {2016},
Pages = {658-667},
