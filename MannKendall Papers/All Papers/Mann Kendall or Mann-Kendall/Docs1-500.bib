@article{ WOS:000610488200001,
Author = {Alhathloul, Saleh H. and Khan, Abdul A. and Mishra, Ashok K.},
Title = {Trend analysis and change point detection of annual and seasonal
   horizontal visibility trends in Saudi Arabia},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
