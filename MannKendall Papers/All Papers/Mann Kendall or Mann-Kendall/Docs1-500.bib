@article{ WOS:001029660700049,
Author = {Pathak, Susanta Sarmah and Hazarika, Bipan and Goswami, Kuldeep},
Title = {TREND DETECTION IN TEA PRODUCTION OF INDIA},
Journal = {INTERNATIONAL JOURNAL OF AGRICULTURAL AND STATISTICAL SCIENCES},
Year = {2022},
Volume = {18},
Number = {1},
Pages = {1319-1324},
Abstract = {The climatic variability changes in the world impact the growth of cash
   crops and India is no exception. In this research work, Mann-Kendall and
