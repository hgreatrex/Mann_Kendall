@inproceedings{ WOS:000298021900020,
Author = {Toernros, Tobias},
Editor = {Servat, E and Demuth, S and Dezetter, A and Daniell, T},
Title = {Precipitation trends and suitable drought index in the arid/semi-arid
   southeastern Mediterranean region},
