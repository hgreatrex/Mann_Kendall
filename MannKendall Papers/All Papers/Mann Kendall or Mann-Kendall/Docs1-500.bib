@article{ WOS:000302943300005,
Author = {Samba, Gaston and Nganga, Dominique},
Title = {Rainfall variability in Congo-Brazzaville: 1932-2007},
Journal = {INTERNATIONAL JOURNAL OF CLIMATOLOGY},
