@article{ WOS:001263454800014,
Author = {Manivannan, S. and Thilagam, V. Kasthuri and Yaligar, Ravindra and
   Manoj, K. N.},
Title = {Crop planning using innovative trend analysis of 62-years rainfall data},
Journal = {INDIAN JOURNAL OF AGRICULTURAL SCIENCES},
