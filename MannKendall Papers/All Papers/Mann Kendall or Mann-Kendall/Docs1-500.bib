@article{ WOS:000487831800003,
Author = {Jung, Kang Young and Ahn, Jung Min and Cho, Sohyun and Lee, Yeong Jae
   and Han, Kun Yeun and Shin, Dongseok and Kim, Kyunghyun},
Title = {Evaluation of long-term water quality management policy effect using
   nonparametric statistical methods},
