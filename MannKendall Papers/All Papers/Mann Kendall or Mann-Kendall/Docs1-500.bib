@article{ WOS:000996301200001,
Author = {Nikolakopoulos, Stavros and Cator, Eric and Janssen, Mart P.},
Title = {Extending the Mann-Kendall test to allow for measurement uncertainty},
Journal = {STATISTICS},
Year = {2023},
Volume = {57},
