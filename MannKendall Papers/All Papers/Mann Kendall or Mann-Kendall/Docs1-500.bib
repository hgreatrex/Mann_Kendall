@article{ WOS:000448772700087,
Author = {Quan, L. and He, Y. and Zhang, X.},
Title = {TREND IN NATURAL ILLUMINANCE LEVELS BY MANN-KENDALL METHOD IN CHONGQING,
   CHINA},
