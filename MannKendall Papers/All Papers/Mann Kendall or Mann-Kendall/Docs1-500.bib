@article{ WOS:000311497300001,
Author = {Chattopadhyay, Goutami and Chakraborthy, Parthasarathi and
   Chattopadhyay, Surajit},
Title = {Mann-Kendall trend analysis of tropospheric ozone and its modeling using
   ARIMA},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2012},
Volume = {110},
Number = {3},
Pages = {321-328},
Month = {DEC},
Abstract = {The present work reports studies on the spatial distribution of
   tropospheric ozone extending over both southern and northern
   hemispheres. This study is based on a univariate approach to the spatial
   data series obtained at regular spatial intervals. Mann-Kendall's (MK)
   trend analysis has been carried out to discern the trend within the
   spatial distribution of the tropospheric ozone, and it has been observed
   that in all seasons, except monsoon (JJAS), there is a linear trend
   within the spatial distribution. Studying both monthly and seasonal
   behavior through autoregressive integrated moving average (ARIMA), it
   has been revealed that ARIMA (0,2,2) can be used as a representative of
   the spatially distributed tropospheric ozone over southern and northern
   hemispheres. The representative model has been confirmed through the
   study of Willmott's index and prediction yield.},
