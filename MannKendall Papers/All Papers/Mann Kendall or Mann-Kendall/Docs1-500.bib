@article{ WOS:000474012900001,
Author = {Javed, Aisha and Cheng, Vincent Y. S. and Arhonditsis, George B.},
Title = {Detection of spatial and temporal hydro-meteorological trends in Lake
   Michigan, Lake Huron and Georgian Bay},
Journal = {AQUATIC ECOSYSTEM HEALTH \& MANAGEMENT},
Year = {2019},
Volume = {22},
