@article{ WOS:000552402400003,
Author = {Kumar, Yogesh and Kumar, Anil},
Title = {Spatiotemporal analysis of trend using nonparametric tests for rainfall
   and rainy days in Jodhpur and Kota zones of Rajasthan (India)},
Journal = {ARABIAN JOURNAL OF GEOSCIENCES},
Year = {2020},
Volume = {13},
Number = {15},
Month = {JUL 18},
Abstract = {Climate change has serious impacts on meteorological processes that have
   significant control on water resource planning, development, and
   management. These impacts emerge in the form of trends. The present
   study was undertaken to assess the temporal trends of annual rainfall
   and rainy days in Jodhpur and Kota zones of Rajasthan, India, using
   nonparametric Mann-Kendall (MK), modified Mann-Kendall (MMK), and
   Theil-Sen slope estimator tests along with innovative trend analysis
   (ITA) method based on 60 years data (1957-2016). Spatial trend analysis
   was accomplished using the inverse distance weighted (IDW) method. Out
   of 34 stations in the Jodhpur zone, the annual rainfall at eight
   stations had a statistically significant increasing trend at a 5\% level
   of significance. In the case of annual rainy days, only two stations
   showed a significant increasing trend. The ITA method was found to be
   superior to the MK test as it was able to identify trends in different
   clusters which were not done by the MK test as it gives monotonous
   trends only. The significantly increasing trends in rainfall at eight
   stations in the Jodhpur zone revealed the impact of climate change in
   the Rajasthan state which is considered as an arid state. This could be
   attributed to various anthropogenic and climatic activities taking place
   in this region. The results of this study could serve as a preliminary
   information for water resources and agricultural planning and management
   and designing of soil and water conservation structures for improving
   the water availability status of the region which would aid in a
   sustainable development of the resources at a decentralized level.},
