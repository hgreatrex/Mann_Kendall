@article{ WOS:000462503900020,
Author = {Terzioglu, Zeynep Ozge and Kankal, Murat and Yuksek, Omer and Nemli,
   Murat Ozer and Akcay, Fatma},
Title = {ANALYSIS OF THE PRECIPITATION INTENSITY VALUES OF VARIOUS DURATIONS IN
   TRABZON PROVINCE OF TURKEY BY SEN'S INNOVATIVE TREND METHOD},
Journal = {SIGMA JOURNAL OF ENGINEERING AND NATURAL SCIENCES-SIGMA MUHENDISLIK VE
   FEN BILIMLERI DERGISI},
Year = {2019},
