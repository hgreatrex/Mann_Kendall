@article{ WOS:000462924500003,
Author = {Araghi, Alireza and Mousavi-Baygi, Mohammad and Adamowski, Jan and
   Martinez, Christopher J.},
Title = {Analyzing trends of days with low atmospheric visibility in Iran during
   1968-2013},
