@article{ WOS:000267298600003,
Author = {Zhang, Qiang and Xu, Chong-Yu and Zhang, Zengxin and Chen, Yongqin David},
Title = {Changes of temperature extremes for 1960-2004 in Far-West China},
Journal = {STOCHASTIC ENVIRONMENTAL RESEARCH AND RISK ASSESSMENT},
