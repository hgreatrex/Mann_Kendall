@article{ WOS:000412795900070,
Author = {Khyarayie, Fatemeh and Arbabie, Azadeh},
Title = {Evaluation of Precipitation Changes in the Provinces of Kermanshah and
   Kurdistan},
Journal = {HELIX},
Year = {2017},
Volume = {7},
Number = {2},
Pages = {1347-1351},
Abstract = {Detecting changes of temperature and precipitation is used as the
   primary evidence for climate changes in many parts of the world. So the
   purpose of this study is to assess changes of precipitation in the
   provinces of Kermanshah and Kurdistan in different time scales of years,
   seasons and months. For this purpose, the total monthly rainfall data of
   11 synoptic stations with good distribution and period length of twenty
   years or more by the end of 2014 were received from meteorological
   organization and processed in the statistical software. In order to
   calculate and verify the temperature parameters, Mann-Kendall
   nonparametric test was used. The results of the regional rainfall showed
   that the average rainfall of the area is 453.5 mm, which Marivan with
   909 mm and Qorveh with 336 mm had the highest and lowest rainfalls of
   the research stations and more than 90\% of stations have received
   precipitation between 350 and 500 mm. In terms of spatial distribution
   of precipitation, the values of this element in the study area are
   reduced from North to South and from West to East. In the seasonal
   scale, the highest intensity of precipitation has seen in winter so that
   the mean scores of Mann-Kendall test in the study area was -2.5 (90\% at
   the significance level) and this decreasing trend is significant in
   other research stations on levels 90, 95 and 99 percent except Bijar. In
   the annual scale, the general trend is also negative in the study area
   and is approaching to significant level (Mann-Kendall test mean score is
   1.7). This negative trend is inclusively seen in all research stations.},
