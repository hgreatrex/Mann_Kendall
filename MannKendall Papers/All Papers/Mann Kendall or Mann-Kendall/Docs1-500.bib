@article{ WOS:000538755300006,
Author = {Mustafa, Ahmed and Rahman, Ghani},
Title = {Assessing the Spatio-temporal Variability of Meteorological Drought in
   Jordan},
Journal = {EARTH SYSTEMS AND ENVIRONMENT},
