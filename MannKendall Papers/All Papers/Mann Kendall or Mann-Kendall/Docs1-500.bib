@article{ WOS:000592231400014,
Author = {Nalcioglu, Alisan and Unsal, Mehmet and Ercan, Burcu and Yagci, Ayse Ece},
Title = {Modeling of Hydrometeorological Factors With Discharge in Asi Basin},
Journal = {KSU TARIM VE DOGA DERGISI-KSU JOURNAL OF AGRICULTURE AND NATURE},
Year = {2020},
Volume = {23},
Number = {6},
Pages = {1510-1517},
Abstract = {Many weather events on the world, climatic changes and water use
   planning are affected by the hydrological cycle. The modeling of water
   cycle is very important for rainfall variability and flood. In this
   study, regression analysis was performed by using hydrometeorological
   parameters of Asi basin stations between 1962 and 2011. The relationship
   between the combinations of basin parameters with each other and
   relation with flood was analyzed by Datafit. As a result of the
   analysis, when the highest values of R2, MSE and RMSE (error rates) were
   examined, the rainfall - evapotranspiration - flow was determined to be
   the suggested model. In addition, trend analyses of these parameters
   were analyzed with the nonparametric Mann Kendall Test. The trends were
   examined in 85\% and 95\% confidence intervals for 6 parameters
   affecting the Asi Basin.},
