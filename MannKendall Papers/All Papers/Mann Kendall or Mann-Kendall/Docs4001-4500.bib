@article{ WOS:000851969100001,
Author = {Yang, Weichao and Hu, De and Jiang, Xuelian and Dun, Xuebo and Hou,
   Bingtao and Zheng, Chuanxing and Chen, Caixia and Zhuang, Rong},
Title = {Framework for Spatio-Temporal Distribution of Disasters and Influencing
   Factors: Exploratory Study of Tianjin, China},
Journal = {SUSTAINABILITY},
Year = {2022},
