@article{ WOS:000788944800002,
Author = {Ploszai, Robinson and Moro Mine, Miriam Rita and Marco Detzel, Daniel
   Henrique},
Title = {An Analysis of Non-stationary Drought Conditions in Parana State Based
   on Climate Change Scenarios},
