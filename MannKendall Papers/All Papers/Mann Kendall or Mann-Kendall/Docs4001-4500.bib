@article{ WOS:000339065900001,
Author = {Choi, Woonsup and Tareghian, Reza and Choi, Jinmu and Hwang, Chul-sue},
Title = {Geographically heterogeneous temporal trends of extreme precipitation in
   Wisconsin, USA during 1950-2006},
