@article{ WOS:000428241200029,
Author = {Bisht, Deepak Singh and Chatterjee, Chandranath and Raghuwanshi,
   Narendra Singh and Sridhar, Venkataramana},
Title = {Spatio-temporal trends of rainfall across Indian river basins},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
