@article{ WOS:000629232800001,
Author = {Shin, Ju-Young and Kim, Kyu Rang and Kim, Jinwon and Kim, Seungbum},
Title = {Long-term trend and variability of surface humidity from 1973 to 2018 in
   South Korea},
