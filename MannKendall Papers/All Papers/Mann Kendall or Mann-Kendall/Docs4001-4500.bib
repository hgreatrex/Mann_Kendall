@article{ WOS:000805915700001,
Author = {Wang, Qiaoli and Li, Zijun and Xu, Yu and Li, Rongrong and Zhang,
   Mengsheng},
Title = {Adaptive-weight water quality assessment and human health risk analysis
   for river water in Hong Kong},
Journal = {ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH},
