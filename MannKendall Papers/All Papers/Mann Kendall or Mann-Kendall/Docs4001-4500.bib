@article{ WOS:000382224800048,
Author = {Li, Qingting and Lu, Linlin and Weng, Qihao and Xie, Yanhua and Guo,
   Huadong},
Title = {Monitoring Urban Dynamics in the Southeast USA Using Time-Series
   DMSP/OLS Nightlight Imagery},
Journal = {REMOTE SENSING},
