@article{ WOS:000175012300007,
Author = {Wang, XLL and Swail, VR},
Title = {Trends of Atlantic wave extremes as simulated in a 40-yr wave hindcast
   using kinematically reanalyzed wind fields},
Journal = {JOURNAL OF CLIMATE},
