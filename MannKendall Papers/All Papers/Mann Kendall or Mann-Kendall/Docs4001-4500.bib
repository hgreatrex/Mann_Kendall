Title = {Analysis of hydroclimatic trends and variability and their impacts on
   hydropower generation in two river basins in Cote d'Ivoire (West Africa)
   during 1981-2017},
Journal = {ENVIRONMENTAL RESEARCH COMMUNICATIONS},
