@article{ WOS:001323147000001,
Author = {Liu, Huanyong and Chen, Yin and Du, Pengfei and Wang, Yangui and Zhao,
   Ying and Qu, Liqin},
Title = {Analysis of the Changes and Causes of Runoff and Sediment Load in the
   Middle Reaches of the Yellow River from 1950 to 2022},
Journal = {LAND},
