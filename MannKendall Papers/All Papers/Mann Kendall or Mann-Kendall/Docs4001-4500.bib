@article{ WOS:000349757200004,
Author = {Azim, Farooq and Shakir, Abdul Sattar and Rehman, Habib and Nabi, Ghulam},
Title = {Site-Specific Detections of Hydroclimatic Changes for Naran Watershed,
   Pakistan},
Journal = {ARABIAN JOURNAL FOR SCIENCE AND ENGINEERING},
Year = {2015},
Volume = {40},
Number = {3},
Pages = {693-704},
Month = {MAR},
Abstract = {In this paper, studies have been made to detect the changes in key
   hydroclimatic variables including precipitation, surface-air
   temperatures and stream flow with their trends, constructed projection
   scenarios, and simulated projected climate parameters for Naran
   watershed. Extreme indices have been determined by least squares and
   weighted average regression analysis. Temperature extremes have
   indicated signs of warming trends and increase in variability.
   Precipitation extremes have depicted decrease in occurrences and
   magnitudes. The analysis has provided evidence of high confidence about
   these changes. Trends of from hydroclimatic parameters and stream flows
   were also determined using Mann-Kendall test for the period 1962-2011.
   Inter-annual trends have been detected to determine the sensitivity with
   a base period (1962-1991) by using equal and unequal overlapped moving
   periods. It has been found that there is a clear trend reversibility
   during 1996-1997 for temperatures and precipitation pattern. However,
   the analysis has revealed that no long-term trends are persistent in
   inter-annual surface-air temperatures and precipitation. Intra-annual
   trends and variations on the monthly scale from data sets of 1962-2011
   have also been detected with their significant values. There seems
   evidence that variation of parameters in monthly scale is occurring that
   is affecting the ice melt schedule and evapotranspiration demand.},
