@article{ WOS:000558261800001,
Author = {Jiang, Rengui and Cao, Ruijuan and Lu, Xi Xi and Xie, Jiancang and Zhao,
   Yong and Li, Fawen},
Title = {Quantifying precipitation extremes and their relationships with
   large-scale climate oscillations in a tropical country, Singapore:
   1980-2018},
