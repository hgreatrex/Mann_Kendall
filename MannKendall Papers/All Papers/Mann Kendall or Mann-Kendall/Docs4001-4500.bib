@article{ WOS:000337146800009,
Author = {Manzanas, R. and Amekudzi, L. K. and Preko, K. and Herrera, S. and
   Gutierrez, J. M.},
Title = {Precipitation variability and trends in Ghana: An intercomparison of
   observational and reanalysis products},
Journal = {CLIMATIC CHANGE},
