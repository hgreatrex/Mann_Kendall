@article{ WOS:000291291500004,
Author = {Wasmund, Norbert and Tuimala, Jarno and Suikkanen, Sanna and Vandepitte,
   Leen and Kraberg, Alexandra},
Title = {Long-term trends in phytoplankton composition in the western and central
   Baltic Sea},
