@article{ WOS:000296414300002,
Author = {Chen, Yongqin David and Zhang, Qiang and Lu, Xixi and Zhang, Shurong and
   Zhang, Zengxin},
Title = {Precipitation variability (1956-2002) in the Dongjiang River (Zhujiang
   River basin, China) and associated large-scale circulation},
