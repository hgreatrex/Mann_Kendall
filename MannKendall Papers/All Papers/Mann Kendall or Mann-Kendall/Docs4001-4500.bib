@article{ WOS:000382411300004,
Author = {Chervenkov, Hristo and Tsonevsky, Ivan and Slavov, Kiril},
Title = {DROUGHT EVENTS ASSESSMENT AND TREND ESTIMATION - RESULTS FROM THE
   ANALYSIS OF LONG-TERM TIME SERIES OF THE STANDARDIZED PRECIPITATION
   INDEX},
Journal = {COMPTES RENDUS DE L ACADEMIE BULGARE DES SCIENCES},
Year = {2016},
Volume = {69},
Number = {8},
Pages = {983-994},
Abstract = {Drought is among the most complex climatic phenomena affecting the
   environment and society. Drought impacts are commonly quantified using
   drought indices (DIs), which are a function of several
   hydro-meteorological variables and are assumed to adequately quantify
   the degree of drought hazard for sensitive systems. The Standardized
   Precipitation Index (SPI) has significant advantages such as simplicity,
   suitability on variable time scale and robustness, based on solid
   theoretical development. Consequently, SPI has been selected by the
   World Meteorological Organization (WMO) as a key indicator for
   monitoring drought and used increasingly during the last two decades.
   This study uses the 161-year-long global gridded dataset of the SPI,
   computed from v2c version of the NOAA-CIRES 20CR precipitation database
   for 4 most frequently used time windows of 1, 3, 6 and 12 months denoted
   here as SPI-1, SPI-3, SPI-6 and SPI-12, respectively. The time series of
   these SPIs are analyzed for three gridcells, located over the Balkan
   Peninsula, showing the onset, ending, duration and the severity of the
   longest drought events. This study describes one primary event, which is
   common for all gridcells and other 4 secondary ones. The trend analyses
   performed additionally using standard methods (linear regression,
   Mann-Kendall test) do not detect any general pattern (i.e. for all SPIs
   and gridcells).},
