@article{ WOS:000573658100006,
Author = {Wang, TingTing and Li, JianGuo and Li, ZongXing},
Title = {Variation characteristics of evaporation in the Gulang River Basin
   during 1959-2013},
Journal = {SCIENCES IN COLD AND ARID REGIONS},
