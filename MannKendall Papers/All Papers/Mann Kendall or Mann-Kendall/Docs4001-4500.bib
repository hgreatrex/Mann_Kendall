@inproceedings{ WOS:000657330400068,
Author = {Vilfand, R. M. and Kulikova, I. A. and Makarova, M. E.},
Book-Group-Author = {IOP},
Title = {Weather and climate features of the Northern Hemisphere in 2019 in the
   context of long-period variability},
Booktitle = {CLIMATE CHANGE: CAUSES, RISKS, CONSEQUENCES, PROBLEMS OF ADAPTATION AND
   MANAGEMENT},
Series = {IOP Conference Series-Earth and Environmental Science},
