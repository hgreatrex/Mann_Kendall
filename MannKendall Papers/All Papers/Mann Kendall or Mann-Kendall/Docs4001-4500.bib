@article{ WOS:000390433900012,
Author = {Sonali, P. and Kumar, D. Nagesh},
Title = {Spatio-temporal variability of temperature and potential
   evapotranspiration over India},
Journal = {JOURNAL OF WATER AND CLIMATE CHANGE},
