@article{ WOS:000805056200002,
Author = {Akbarian, Mohammad and Khoorani, Asadollah},
Title = {The impacts of climate variability on the wind erosion potentials:
   western region of Makran coastal plain, South of Iran},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2022},
Volume = {149},
Number = {3-4},
Pages = {1209-1221},
Month = {AUG},
Abstract = {Coastal plains are prone to various wind erosion due to their
   characteristics. This study aimed to investigate the effect of climatic
   factors in the western region of Makran coastal plain in a southeastern
   region of Iran on the wind erosion potential during 1993-2018. First,
   the data related to wind velocities, relative humidity and
   precipitation, and the granulometric data of plain surface sediments
   were obtained. Then, the wind erosion threshold velocity in humid air
   conditions was determined. Finally, the Mann-Kendall test was applied to
   analyze the probability of wind erosion and its temporal variability.
   The results indicated that the wind erosion threshold, in terms of
   humidity changes, varied from 7.21 to 12.31 m/s during the study period.
   The highest probability of wind erosion was in February, March, and
   April, with 24.69\%, 21.51\%, and 20.41\%, respectively. The lowest
   probability of wind erosion was in October, November, and September with
   4.00\%, 4.12\%, and 6.00\%, respectively. Finally, the erosion trend
   analysis indicated that wind erosion was a temporal phenomenon that
   increased significantly in January (winter solstice) and July (summer
   solstice). These months were characterized by an increase in the wind
   blowing at speed above the threshold. However, the months were not
   different from other months of the year in terms of precipitation or the
   number of dry days. Therefore, wind erosion is expected to maximize in
   the early winter and the early summer.},
