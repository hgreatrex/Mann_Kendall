@article{ WOS:001321912100001,
Author = {Moreira, Rodrigo Martins and Martins, Ruan Almeida},
Title = {Influence of precipitation and temperature on burned areas in the
   agricultural and cattle ranching frontier of the Brazilian Amazon},
