@article{ WOS:001266626400001,
Author = {Liu, Jiayuan and Zheng, Shuwei and Lin, Jinkuo and Zhao, Mengjie and Ma,
   Yanan and Chen, Banghui and Wen, Fei and Lu, Zhijie and Li, Zijun},
Title = {Human Activities Have Altered Sediment Transport in the Yihe River, the
   Longest River Originating from Shandong Province, China},
Journal = {SUSTAINABILITY},
