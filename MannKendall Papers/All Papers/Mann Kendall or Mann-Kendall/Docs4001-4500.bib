@article{ WOS:001140677300001,
Author = {Hu, Pan and Feng, Lan},
Title = {The Risk of Water Quality Deterioration with Urban Flood Control-A Case
   in Wuxi},
Journal = {SUSTAINABILITY},
Year = {2024},
