@article{ WOS:001094630600001,
Author = {Lv, Cuimei and Wang, Xinrui and Ling, Minhua and Xu, Wenjing and Yan,
   Denghua},
Title = {Effects of Precipitation Concentration and Human Activities On City
   Runoff Changes},
Journal = {WATER RESOURCES MANAGEMENT},
