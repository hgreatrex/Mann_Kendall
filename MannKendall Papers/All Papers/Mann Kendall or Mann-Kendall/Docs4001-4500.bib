@article{ WOS:000463373300015,
Author = {Habib, Ammara and Chen, Bin and Khalid, Bushra and Tan, Saichun and Che,
   Huizheng and Mahmood, Tariq and Shi, Guangyu and Butt, Muhammad Tahir},
Title = {Estimation and inter-comparison of dust aerosols based on MODIS, MISR
   and AERONET retrievals over Asian desert regions},
Journal = {JOURNAL OF ENVIRONMENTAL SCIENCES},
Year = {2019},
Volume = {76},
Pages = {154-166},
Month = {FEB},
Abstract = {This study presents detailed analysis of spatiotemporal variations and
   trend of dust optical properties i.e., Aerosol Optical Depth (AOD) and
   Angstrom component over Asian desert regions using thirteen years of
   data (i.e., 2001-2013) retrieved from Aerosol Robotic Network (AERONET),
   Moderate Resolution Imaging Spectroradiometer (MODIS) and Multi-angle
   imaging Spectroradiometer (MISR). These regions include Solar Village,
   Dunhuang and Dalangzadgad and are considered as origin of desert
   aerosols in Asia. Mann-Kendall trend test was used to show the trend of
   AOD. The relationship of AOD with weather parameters and general AOD
   trend over different wavelengths has also been shown. AOD's trend has
   Angstrom exponent been observed significant throughout the year in Solar
   Village, while in Dunhuang and Dalanzadgad the significant trend has
   been found only in peak period (March-June). Analysis show high values
   of AOD and low values of angstrom in Solar Village during peak period.
   In Chinese desert regions, high values of AOD have been found during
   peak period Desert and low values in pre-peak period. Significant
   relationship has been observed between AOD and average temperature in
   Solar Village and Dalanzadgad whereas rainfall and wind speed showed no
   significant impact on AOD in all desert regions. (C) 2018 The Research
   Center for Eco-Environmental Sciences, Chinese Academy of Sciences.
