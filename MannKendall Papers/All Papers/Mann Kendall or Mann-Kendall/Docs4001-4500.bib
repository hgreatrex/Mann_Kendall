@article{ WOS:000267128000007,
Author = {Cotter, John},
Title = {A selection of nonparametric statistical methods for assessing trends in
   trawl survey indicators as part of an ecosystem approach to fisheries
   management (EAFM)},
Journal = {AQUATIC LIVING RESOURCES},
Year = {2009},
Volume = {22},
