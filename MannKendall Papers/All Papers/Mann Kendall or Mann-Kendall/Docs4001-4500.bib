@article{ WOS:000460899600136,
Author = {Yan, Zhihong and Wang, Shuqian and Ma, Ding and Liu, Bin and Lin, Hong
   and Li, Su},
Title = {Meteorological Factors Affecting Pan Evaporation in the Haihe River
   Basin, China},
Journal = {WATER},
Year = {2019},
Volume = {11},
Number = {2},
Month = {FEB},
Abstract = {Pan evaporation (E-pan) is an important indicator of regional
   evaporation intensity and degree of drought. However, although more
   evaporation is expected under rising temperatures, the reverse trend has
   been observed in many parts of the world, known as the pan evaporation
   paradox. In this paper, the Haihe River Basin (HRB) is divided into six
   sub-regions using the Canopy and k-means (The process for partitioning
   an N-dimensional population into k sets on the basis of a sample is
   called k-means) to cluster 44 meteorological stations in the area. The
   interannual and seasonal trends and the significance of eight
   meteorological indicators, including average temperature, maximum
   temperature, minimum temperature, precipitation, relative humidity,
   sunshine duration, wind speed, and E-pan, were analyzed for 1961 to 2010
   using the trend-free pre-whitening Mann-Kendall (TFPW-MK) test. Then,
   the correlation between meteorological elements and E-pan was analyzed
   using the Spearman correlation coefficient. Results show that the
   average temperature, maximum temperature, and minimum temperature of the
   HRB increased, while precipitation, relative humidity, sunshine
   duration, wind speed and E-pan exhibited a downward trend. The minimum
   temperature rose 2 and 1.5 times faster than the maximum temperature and
   average temperature, respectively. A significant reduction in sunshine
   duration was found to be the primary factor in the E-pan decrease, while
   declining wind speed was the secondary factor.},
