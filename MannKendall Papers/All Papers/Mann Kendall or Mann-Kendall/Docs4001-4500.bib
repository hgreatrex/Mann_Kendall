@article{ WOS:000493606200007,
Author = {Guo, Qiaoling and Yang, Yunsong and Su, Ning and Li, Jianlin and Wang,
   Xinyi},
Title = {Impact Assessment of Climate Change and Human Activities on Runoff
   Variation in Coal Mining Watershed, NW China},
