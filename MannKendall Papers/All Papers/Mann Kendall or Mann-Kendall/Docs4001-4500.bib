@article{ WOS:000466482900032,
Author = {Cheng, Nianliang and Cheng, Bingfen and Li, Shanshan and Ning, Tingzhou},
Title = {Effects of meteorology and emission reduction measures on air pollution
   in Beijing during heating seasons},
Journal = {ATMOSPHERIC POLLUTION RESEARCH},
Year = {2019},
