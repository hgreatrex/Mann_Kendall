@article{ WOS:000647821000004,
Author = {Zhao, Xiaohan and Zhang, Fangmin and Su, Rongmingzhu and Gao, Cao and
   Xing, Kaicheng},
Title = {Response of carbon and water fluxes to dryness/wetness in China},
Journal = {TERRESTRIAL ATMOSPHERIC AND OCEANIC SCIENCES},
