@article{ WOS:000457212900002,
Author = {Bilbao, Julia and Roman, Roberto and De Miguel, Argimiro},
Title = {Temporal and Spatial Variability in Surface Air Temperature and Diurnal
   Temperature Range in Spain over the Period 1950-2011},
