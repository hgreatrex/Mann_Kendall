@article{ WOS:000438428600044,
Author = {Li, Cheng and Wang, Ranghui and Ning, Husen and Luo, Qinghong},
Title = {Characteristics of meteorological drought pattern and risk analysis for
   maize production in Xinjiang, Northwest China},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2018},
