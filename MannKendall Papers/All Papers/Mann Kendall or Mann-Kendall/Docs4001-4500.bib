@article{ WOS:000320007200019,
Author = {Back, Alvaro Jose and Della Bruna, Emilio and Dalbo, Marco Antonio},
Title = {CLIMATE CHANGES AND GRAPE PRODUCTION IN VALE DO RIO DO PEIXE, IN THE
   STATE OF SANTA CATARINA},
