@article{ WOS:000510901000018,
Author = {Du, Sheng and Wu, Min and Chen, Luefeng and Zhou, Kailong and Hu, Jie
   and Cao, Weihua and Pedrycz, Witold},
Title = {A Fuzzy Control Strategy of Burn-Through Point Based on the Feature
   Extraction of Time-Series Trend for Iron Ore Sintering Process},
Journal = {IEEE TRANSACTIONS ON INDUSTRIAL INFORMATICS},
Year = {2020},
Volume = {16},
Number = {4},
Pages = {2357-2368},
Month = {APR},
Abstract = {Sinter ore is the main raw material for ironmaking, and burn-through
   point (BTP) is one of the significant factors to measure the stability
   of the sintering process. In this article, through the feature
   extraction of time-series trend, a fuzzy control strategy is presented
   for the BTP. First, the Hurst exponent of the time series for the BTP is
   calculated by resorting to the rescaled range analysis method, by which
   the trend feature is analyzed. Then, by using the Mann-Kendall test,
   both global and local trend feature variable of the time series for the
   BTP are extracted and regarded as the inputs of the fuzzy controller.
   Next, a fuzzy controller for the BTP is designed to produce the control
   quantity of the strand velocity. Finally, based on a semiphysical
   simulation system and the raw data collected from an iron and steel
   plant, an experiment is carried out to demonstrate the effectiveness of
   the proposed control strategy.},
