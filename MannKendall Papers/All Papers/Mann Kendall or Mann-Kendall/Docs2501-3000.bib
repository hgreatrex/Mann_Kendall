@article{ WOS:000526830500001,
Author = {Asamoah, Yaw and Ansah-Mensah, Kow},
Title = {Temporal Description of Annual Temperature and Rainfall in the Bawku
   Area of Ghana},
Journal = {ADVANCES IN METEOROLOGY},
