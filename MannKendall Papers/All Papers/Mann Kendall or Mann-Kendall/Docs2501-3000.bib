@article{ WOS:001326168900001,
Author = {Ayala, Jose J. Hernandez and Mendez-Tejeda, Rafael},
Title = {Analyzing Trends in Saharan Dust Concentration and Its Relation to
   <i>Sargassum</i> Blooms in the Eastern Caribbean},
