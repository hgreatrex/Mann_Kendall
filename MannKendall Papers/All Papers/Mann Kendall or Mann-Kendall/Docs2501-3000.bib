@inproceedings{ WOS:000394721800044,
Author = {Yao Yunlong and Yu Hongxian and Lu Xianguo and Wang Lei},
Editor = {Hu, J},
Title = {The impacts of wetland cultivation on the regional temperature based on
   Remote Sensing - A case study in Naoli watershed of Sanjiang Plain,
   Northeast China},
Booktitle = {2011 INTERNATIONAL CONFERENCE ON INTELLIGENT COMPUTATION AND INDUSTRIAL
   APPLICATION (ICIA2011), VOL I},
Year = {2011},
Pages = {175-178},
Note = {International Conference on Intelligent Computation and Industrial
   Application (ICIA2011), Hong Kong, PEOPLES R CHINA, JUN 18-19, 2011},
Organization = {Int Ind Elect Ctr},
Abstract = {The cold-humid ecological effect is one of important ecological
   functions of wetland, which was declined dramatically because of wetland
   cultivation. We aim to describe how the wetland cultivation affects the
   regional climate in this study. We derived four years (1976, 1986,1995
   and 2000) data of wetland distribution in Naoli watershed of Sanjiang
   plain, Northeast China by Landsat TM/ETM; The trend of annual
   maximum/minimum air temperature was analyzed by Mann-Kendall trend test,
   the relationship between wetland area and annual maximum/minimum air
   temperature was also represented by linear regression, and the results
   show that: the annual maximum/minimum air temperature had increased from
