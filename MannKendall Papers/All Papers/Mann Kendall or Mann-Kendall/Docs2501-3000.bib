@article{ WOS:000315074100010,
Author = {Bach, Martin and Frede, Hans-Georg},
Title = {Pesticide River Load Trend in German Rivers},
Journal = {WASSERWIRTSCHAFT},
Year = {2013},
Volume = {103},
Number = {1-2},
Pages = {62-65},
Abstract = {To judge the effectiveness of policies to reduce pesticide pollution of
   rivers solely the load of substances transported in river systems gives
   valid information. The long-term change of pesticide river loads was
   evaluated by the Mann-Kendall trend test for 57 river load time series
   of 14 herbicide substances from seven monitoring stations in Germany
   (longest time course 1990 to 2006). From 57 time series in total 27
   cases (47,4 \%) has a statistically significant downward trend. The
   intended effects of the pesticide reduction policy seem to have
   occurred, however, the data base to justify this statement is small. If
   the evaluation is Confined to the 12 approved substances, just 36 time
   series remain, thereof only 18 load series (38,3 \%) has a significantly
   decreasing trend.},
