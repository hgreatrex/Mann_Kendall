@inproceedings{ WOS:000442257000215,
Author = {Malygina, N. S. and Barlyaeva, V, T. and Papina, T. S. and Kurepina, N.
   Yu and Ryabchinskaya, N. A. and Eirikh, A. N.},
Editor = {Matvienko, GG and Romanovskii, OA},
Title = {Macrocirculaton processes responsible for precipitation in Altai},
Booktitle = {23RD INTERNATIONAL SYMPOSIUM ON ATMOSPHERIC AND OCEAN OPTICS:
   ATMOSPHERIC PHYSICS},
Series = {Proceedings of SPIE},
