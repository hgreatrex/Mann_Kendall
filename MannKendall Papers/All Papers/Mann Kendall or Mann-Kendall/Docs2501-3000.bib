@article{ WOS:000578322500001,
Author = {Senapati, Asis Kumar},
Title = {Weather effects and their long-term impact on agricultural yields in
   Odisha, East India: Agricultural policy implications using NARDL
   approach},
Journal = {JOURNAL OF PUBLIC AFFAIRS},
Year = {2022},
Volume = {22},
Number = {3},
Month = {AUG},
Abstract = {Farm production is uneven due to its high dependence on the weather
   conditions. A slight deviation of weather variables from the normal
   trend causes heavy losses to crops. Therefore it is imperative to study
   the spatial and temporal distribution of these weather variables and
   also important to find out the asymmetric effect of weather variables on
   agricultural yield which has serious policy implications. This study
   attempts to estimate the overall effect of weather variables such as
   rainfall, temperature, irrigation, ground frost frequency, and crop
   evapotranspiration on yield in Odisha from 1950 to 2017. The objective
   is to check the asymmetric relationship between rainfall and yield of
   major crops grown in Odisha along with other explanatory variables such
   as temperature, irrigation, ground frost frequency, and crop
   evapotranspiration using both nonparametric and econometric technique of
   a nonlinear autoregressive distributed lag (NARDL) model. Using
   nonparametric tests such as Sen's estimator and Mann-Kendall test, the
   study found that the rising trend of temperature during the post-monsoon
   period may alter the crop growth in the state. The highest positive and
   statistically significant values have been observed from the
   Mann-Kendall test in Central table land zones. Similarly, the maximum
   positive significant rainfall trend has been noticed in Cuttack and Puri
   districts. Using the NARDL model, the study concluded that positive
   components of both long-run and short-run elasticities of rainfall are
   negative indicating a complementarity between both long-run and
   short-run effects on the yield of major crops grown in Odisha. The
   magnitude of positive deviations of rainfall is higher than the
   corresponding elasticities of negative deviations. There is a strong
   asymmetry effect of rainfall over the yield of major crops grown in
   Odisha agriculture. Other variables like temperature, irrigation, ground
   frost frequency, and crop evapotranspiration have a minimal role to play
   in both the short run and long run.},
