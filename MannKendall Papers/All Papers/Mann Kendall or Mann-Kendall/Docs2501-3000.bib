@article{ WOS:000403101600008,
Author = {Mao, Dehua and Feng, Chang and Zhou, Hui and Hu, Guangwei and Li,
   Zhengzui and Guo, Ruizhi},
Title = {The runoff variation characteristics of Dongting Lake, China},
Journal = {TECNOLOGIA Y CIENCIAS DEL AGUA},
