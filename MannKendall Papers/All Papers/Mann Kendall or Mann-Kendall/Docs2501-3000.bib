@article{ WOS:000543684200007,
Author = {Zhang, Jing and Yu, Xiaolong},
Title = {Analysis of land use change and its influence on runoff in the Puhe
   River Basin},
Journal = {ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH},
Year = {2021},
Volume = {28},
Number = {30, SI},
Pages = {40116-40125},
Month = {AUG},
Abstract = {This article aims to evaluate the impact of urbanization on land use
   changes and the effects of land use changes on catchment's runoff. The
   maximum likelihood method was used to interpret the seven remote sensing
   images of the Puhe River Basin from 1985 to 2015. The Mann-Kendall test,
   slidingTtest, and double accumulation method are adopted to analyze the
   impact of land use changes on watershed runoff. The results show that
   with the development of urbanization, a large amount of cultivated land
   was replaced by urban construction land. By 2015, the construction land
   area accounted for 72.16\% of the total area. The year 1995 was a
   turning point in the urbanization development. The change of land use
   type has substantial influence on runoff and results in the increase of
   runoff coefficient. Human activities become the main factor affecting
   runoff change in the Puhe River Basin.},
