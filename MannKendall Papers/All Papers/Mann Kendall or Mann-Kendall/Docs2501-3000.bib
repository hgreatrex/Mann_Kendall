@article{ WOS:000236332400009,
Author = {Hodgkins, GA and Dudley, RW},
Title = {Changes in late-winter snowpack depth, water equivalent, and density in
   Maine, 1926-2004},
Journal = {HYDROLOGICAL PROCESSES},
