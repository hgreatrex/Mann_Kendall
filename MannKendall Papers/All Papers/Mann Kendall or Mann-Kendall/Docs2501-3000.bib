@inproceedings{ WOS:000309564800014,
Author = {Peng, Shizhang and Wang, Jie and Luo, Yufeng and Xu, Junzeng and Li, Dan},
Editor = {Ren, L and Wang, W and Yuan, F},
Title = {Effect of climate change on irrigation water requirements of rice in
   Tianmu Basin with consideration to shortened growth period},
Booktitle = {HYDROLOGICAL CYCLE AND WATER RESOURCES SUSTAINABILITY IN CHANGING
   ENVIRONMENTS},
Series = {IAHS Publication},
Year = {2011},
Volume = {350},
Pages = {104+},
Note = {5th International Symposium on Integrated Water Resources Management/3rd
   International Symposium on Methodology in Hydrology, Hohai Univ,
   Nanjing, PEOPLES R CHINA, NOV 19-21, 2010},
Organization = {IAHS, ICWRS; State Key Lab Hydrol Water Resources \& Hydraul Engn;
   Nanjing Hydraul Res Inst; IAHS, Chinese Natl Comm; Int Train \& Res Ctr
   Hydrol Water Resources \& Environm; UNESCO-IHP; Minist Educ \& State Adm
   Foreign Experts Affairs; Jiangsu Higher Educ Inst, Prior Acad Program
   Dev},
Abstract = {Considering the shortened growth period of rice due to global warning,
   we calculated irrigation water requirements of rice in Tianmu Basin
   during 1954-2008 using the FAO-56 Penman-Moniteth method, and its
   long-term trend was detected through the Mann-Kendall test. After
   analysis of trends in meteorological parameters, the impacts of climate
   change on irrigation water requirements were investigated. The results
   showed that, due to highly remarkable decreased average wind speed and
   sunshine hours, there was a slightly decreasing trend in the reference
   crop evapotranspiration (ET0); while a decreasing trend was detected in
   precipitation, which brought about an increasing trend in the irrigation
   water requirements. Overall, besides the alarming rise in industrial and
   domestic water requirements, climate change contributed to the lack of
   sufficient water for agriculture in China during the past decades, which
   will have significant implications for adaptation strategies for climate
   change and policies for water management.},
Publisher = {INT ASSOC HYDROLOGICAL SCIENCES},
Address = {INST OF HYDROLOGY, WALLINGFORD OX10 8BB, ENGLAND},
