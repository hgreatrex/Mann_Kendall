@article{ WOS:000273399900004,
Author = {Rodrigues da Silva, Vicente de Paulo and de Azevedo, Pedro Vieira and
   Brito, Robson Souto and Baracuy da Cunha Campos, Joao Hugo},
Title = {Evaluating the urban climate of a typically tropical city of
   northeastern Brazil},
