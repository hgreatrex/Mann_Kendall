@article{ WOS:000803819500001,
Author = {Dehghani, Fatemeh and Khalili, Davar and Zand-Parsa, Shahrokh and
   Kamgar-Haghighi, Ali Akbar},
Title = {Influence of Climatic Variability on Detected Drought Spatio/Temporal
   Variability and Characteristics by SPI and RDI},
Journal = {IRANIAN JOURNAL OF SCIENCE AND TECHNOLOGY-TRANSACTIONS OF CIVIL
   ENGINEERING},
