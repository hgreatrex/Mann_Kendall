@article{ WOS:000366326600002,
Author = {Choi, Woonsup and Nauth, Kathryn and Choi, Jinmu and Becker, Stefan},
Title = {Urbanization and Rainfall-Runoff Relationships in the Milwaukee River
   Basin},
Journal = {PROFESSIONAL GEOGRAPHER},
Year = {2016},
Volume = {68},
Number = {1},
Pages = {14-25},
Month = {JAN 2},
Abstract = {To understand the changing rainfall-runoff relationship, this study
   examined climate and streamflow data in the Milwaukee River Basin in
   southeastern Wisconsin, of which four catchments with different degrees
   of urbanization were selected for analysis. This study analyzed
   temperature, precipitation, and streamflow data with a range of
   statistical methods, including the Mann-Kendall test, double-mass
   technique, and quantile regression. Runoff ratios and extreme flow
   indexes were higher in more urbanized catchments. Catchments with
   long-term data (>forty years) showed significantly increasing runoff
   ratios and slopes in double-mass curves. Overall, there are signs of
   changes in the rainfall-runoff relationship, but how much they can be
   attributed to land use changes is uncertain.},
