@article{ WOS:000257772400002,
Author = {Hao, Xingming and Chen, Yaning and Xu, Changchun and Li, Weihong},
Title = {Impacts of climate change and human activities on the surface runoff in
   the Tarim River basin over the last fifty years},
Journal = {WATER RESOURCES MANAGEMENT},
Year = {2008},
Volume = {22},
Number = {9},
Pages = {1159-1171},
Month = {SEP},
Abstract = {The impacts of climate change and human activities on the surface runoff
   were analyzed by the Mann-Kendall and trend analysis methods based on
   the hydrological, meteorological and socioeconomic data over the last 50
   years in the Tarim River basin. Results show that the runoff in the
   headstreams increased but that in the mainstreams decreased
   significantly during the past 50 years. The former is a response to
   climate change, and the later is due to human activities. The surface
   runoff in the mainstreams decreased by 41.59, 63.77 and 75.15\% in the
   1970s, 1980s and 1990s, respectively, as compared to that without being
   disturbed by human activities in the same period. The main human
   activities are the irrigated agriculture combined with population
   increase in the region. As a consequence, the ecosystem in the lower
   reaches of Tarim River has degenerated, largely owing to inappropriate
   allocation of water resources.},
