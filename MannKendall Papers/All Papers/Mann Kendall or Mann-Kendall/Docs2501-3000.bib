@article{ WOS:000447575100008,
Author = {Shrestha, Suraj and Yao, Tandong and Adhikari, Tirtha Raj},
Title = {Analysis of rainfall trends of two complex mountain river basins on the
   southern slopes of the Central Himalayas},
