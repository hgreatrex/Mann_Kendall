@article{ WOS:000809098500001,
Author = {Rodrigues Silva, Felipe Gimenes and dos Santos, Alexandre Rosa and
   Fiedler, Nilton Cesar and Paes, Juarez Benigno and Alexandre, Rodrigo
   Sobreira and Guerra Filho, Plinio Antonio and da Silva, Rosane Gomes and
