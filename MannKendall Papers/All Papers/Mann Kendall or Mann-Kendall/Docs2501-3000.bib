@article{ WOS:000344779500003,
Author = {Chattopadhyay, G. and Chattopadhyay, S.},
Title = {Study on statistical aspects of monthly sunspot number time series and
   its long-range correlation through detrended fluctuation analysis},
Journal = {INDIAN JOURNAL OF PHYSICS},
Year = {2014},
Volume = {88},
Number = {11},
Pages = {1135-1140},
Month = {NOV},
Abstract = {Present paper reports a study on monthly sunspot number time series for
   various aspects. The skewness, homogeneity and existence of trend have
   been investigated. A univariate clustering has been carried out to
   identify the centroids of the clusters. It has been examined for
   white-noise process and spectral densities have been computed. We have
   observed that the time series is not Gaussian and positively skewed.
   Mann-Kendall's test has indicated existence of linear trend within the
   time series. Moreover, Pettitt's test has confirmed that time series is
   not homogeneous. Finally, detrended fluctuation analysis with first and
   second degree polynomials has been carried out. Irrespective of first or
   second degree, long-range power-law correlation has been observed within
   monthly sunspot number time series.},
