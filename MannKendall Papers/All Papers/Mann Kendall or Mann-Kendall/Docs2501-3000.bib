@article{ WOS:000559125200003,
Author = {Yang, Baisong and Wang, Wei and Yang, Lingxian and Sun, Guorui and Liu,
   Sijin},
Title = {Study on Stressing State and Failure Criterion of Concrete-Filled
   Stainless Steel and Steel Tubular Column},
Journal = {ADVANCES IN CIVIL ENGINEERING},
Year = {2020},
Volume = {2020},
Month = {JUL 20},
Abstract = {In this paper, the mechanical characteristics of concrete-filled
   stainless steel and steel tubular (CFSSAST) columns under axial and
   eccentric loads are analyzed by using the theory of structural stressing
   state. Firstly, the sum of generalized strain energy density (GSED)
   values of the short column at every load value (F-j) is normalized
   asEj,normto describe the structural stressing state. Then, according to
   Mann-Kendall (M-K) criterion and the natural law from quantitative
   change to qualitative change, the transition of stressing state is
   distinguished, which leads to the update of failure load. Then, the
   corresponding finite element models are established, and the accuracy of
   the models is verified by the experimental data, and the stress contour
   maps are analyzed by simulation data. Finally, the simulation data are
   used to perform parameter analysis. This study explores a new method to
   reveal the invisible working characteristics of structures and provides
   a new reference for the study of similar structures.},
