@article{ WOS:000841041000001,
Author = {Bosneagu, Romeo and Lupu, Carmen Elena and Torica, Emil and Lupu, Sergiu
   and Vatu, Nicolae and Tanase, Viorel Mihai and Vasilache, Camelia and
   Daneci-Patrau, Daniel and Scurtu, Ionut Cristian},
Title = {Long-term analysis of air temperatures variability and trends on the
   Romanian Black Sea Coast},
Journal = {ACTA GEOPHYSICA},
Year = {2022},
Volume = {70},
Number = {5, SI},
Pages = {2179-2197},
Month = {OCT},
Abstract = {Air temperature is one of the most important parameters that contribute
   to weather variability over time, being influenced by the flow of solar
   radiation, the general circulation of currents in the atmosphere relief.
   The present paper analyzes the minimum, maximum, and mean temperatures
   in Dobrogea, on the Romanian Black Sea coastal area, aiming to
   illustrate their evolution, which is a precondition for sustainable
   development in this region, from the perspective of regional and global
   climate changes. The weather stations included in this study are
   Constanta, Mangalia, Gura Portitei, Sfantu Gheorghe, and Sulina. The
   Pettit Test and the Standard Normal Homogeneity Test were used to
   determine changes in the evolution of the air temperature. For the
   period 1990-2020, the analysis of the change points, with a 95\%
   confidence level, shows a particularly interesting situation supporting
   the general evolution of air temperature at global level. Nonparametric
   tests including linear regression, Mann-Kendall, and Sen's slope tests
   were used to analyze trends for monthly, seasonal, and annual series.
   Results showed an increasing trend in the annual minimum, maximum, and
   mean temperatures in all five weather stations.},
