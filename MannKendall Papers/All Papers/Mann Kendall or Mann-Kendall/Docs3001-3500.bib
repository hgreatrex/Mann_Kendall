@article{ WOS:000471564200001,
Author = {Ghasemifar, Elham and Farajzadeh, Manuchehr and Mohammadi, Chenour and
   Alipoor, Ebrahim},
Title = {Long-term change of surface temperature in water bodies around Iran -
   Caspian Sea, Gulf of Oman, and Persian Gulf - using 2001-2015 MODIS data},
