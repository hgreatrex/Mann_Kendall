@article{ WOS:000355932400010,
Author = {Huang, Jin and Liu, Fengling and Xue, Yan and Sun, Shanlei},
Title = {The spatial and temporal analysis of precipitation concentration and dry
   spell in Qinghai, northwest China},
Journal = {STOCHASTIC ENVIRONMENTAL RESEARCH AND RISK ASSESSMENT},
Year = {2015},
Volume = {29},
Number = {5},
Pages = {1403-1411},
Month = {JUL},
Abstract = {Understanding precipitation variations from various aspects is important
   for the assessment of drought risk and the utilization of water
   resources. The precipitation concentration index (PCI) and the
   concentration index (CI) were used to investigate/quantify the
   heterogeneity of the monthly and daily rainfall in Qinghai province that
   is part of northwestern China, respectively. The precipitation
   concentration in Qinghai shows a significant irregularity of the monthly
   rainfall distribution and highly homogeneous distribution of the daily
   rainfall. It is found that PCI and CI show negative trends at most
   stations. Meanwhile, the spatial and temporal variation of nine dry
   spell (DS) indices are analyzed. From the spatial perspective, drought
   in the northwestern area is much severer than that in other areas of
   Qinghai. According to the results of temporal analysis by using the
   Mann-Kendall test, the number of very long DSs, maximum length of DS,
   mean length of DSs, and the total dry days of extreme DS all decrease.
   All these results verify that the warm dry climatic pattern in Qinghai
   can be changed into the warm wet climatic pattern.},
