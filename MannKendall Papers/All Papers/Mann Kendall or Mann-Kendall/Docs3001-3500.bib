@article{ WOS:000818811300008,
Author = {Topcu, E.},
Title = {Drought Analysis Using the Entropy Weight-based TOPSIS Method: A Case
   Study of Kars, Turkey},
Journal = {RUSSIAN METEOROLOGY AND HYDROLOGY},
