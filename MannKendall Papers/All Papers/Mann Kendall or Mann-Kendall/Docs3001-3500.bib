@article{ WOS:001062164500002,
Author = {Sahu, Ramgopal T. and Verma, Mani Kant and Ahmad, Ishtiyaq},
Title = {Density-based spatial clustering of application with noise approach for
   regionalisation and its effect on hierarchical clustering},
Journal = {INTERNATIONAL JOURNAL OF HYDROLOGY SCIENCE AND TECHNOLOGY},
Year = {2023},
Volume = {16},
Number = {3},
Pages = {240-269},
Abstract = {The study assesses DBSCAN-based cluster analysis to obtain a quantile
   with a minimum or low error (AR bias and R-RMSE). The Mahanadi River
   Basin was examined for spatiotemporal characteristics of precipitation
   records prior to testing the DBSCAN technique for regionalising
   precipitation. An application of the modified Mann-Kendall trend test
   and BEAST, i.e., Bayesian estimators of abrupt change, seasonality, and
