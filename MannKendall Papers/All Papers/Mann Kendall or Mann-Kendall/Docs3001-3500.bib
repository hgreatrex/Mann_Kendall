@article{ WOS:000754599000009,
Author = {Wei Huai-Bin and Yuan Jian-Ping and Cao Yong-Xiao},
Title = {CHARACTERISING DROUGHT AND FLOOD VARIATIONS IN THE JIALU RIVER BASIN IN
   CENTRAL CHINA BY COMBINING MULTIPLE METHODS},
Journal = {JOURNAL OF ENVIRONMENTAL PROTECTION AND ECOLOGY},
Year = {2021},
Volume = {22},
Number = {5},
Pages = {1911-1919},
Abstract = {The transformation system between drought and flood is complex and the
   key to forecast and manage drought and flood. The transformation of
   drought and flood involves not only the degree of drought and flood, but
   also their evolution trends and the key factors affecting this
   transformation. Taking the Jialu River Basin (JRB) in Central China as
   an example, via the comprehensive analysis of rainfall data from 8
   meteorological stations, the temporal and spatial distribution, the
   transformation period, and of the factors influencing drought and flood
   were revealed. Our results show that: (1) for most of the year, there is
   a drought in the JRB, irrespective of the meteorological station; (2)
   the dry-wet transformation between adjacent seasons showed that the main
   characteristics of each season were continuous drought/wetting; (3)
   there is no difference between dry and wet transformation in the JRB,
   indicating that topography does not play a role, and (4) the sequence of
   drought and flood showed good agreement with solar activity, indicating
   that sunspots are the main cause of the periodic change of drought and
   flood in the JRB.},
