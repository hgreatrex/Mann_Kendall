@article{ WOS:000491945900041,
Author = {Li, Fengping and Ju, Xiaopei and Lu, Wenxi and Li, Hongyan},
Title = {A comprehensive analysis of spatial and temporal variability of extreme
   precipitation in the Nenjiang River Basin, Northeast China},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
