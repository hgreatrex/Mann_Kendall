@incollection{ WOS:000674456500024,
Author = {Kassahun, Teowdroes and Bender, Svane},
Editor = {Filho, WL and Jacob, D},
Title = {Food Security in the Face of Climate Change at Kafa Biosphere Reserve,
   Ethiopia},
Booktitle = {HANDBOOK OF CLIMATE SERVICES},
