@article{ WOS:000297350700015,
Author = {Liu, Xingcai and Xu, Zongxue and Yu, Ruihong},
Title = {Trend of climate variability in China during the past decades},
Journal = {CLIMATIC CHANGE},
Year = {2011},
Volume = {109},
Number = {3-4},
Pages = {503-516},
Month = {DEC},
Abstract = {Trends in precipitation and mean air temperature in China are estimated,
   and trend analysis on statistical moments of residuals is further used
   to investigate climate variability at different timescales. Trends of
   statistical moments for residuals (i.e. detrended series of climate
   records) are estimated by using least-square method and Mann-Kendall
   test. Results show that upward trend is detected in annual mean air
   temperature but no linear trend for annual precipitation in China. Weak
   trend is found for variability of precipitation while no trend is found
   for that of air temperature for China as a whole. But some regional
   features of climate variability are observed. It is found that the
   northwest of China shows a significant increasing for precipitation
   variability, which is consistent with previous work, especially for
   monthly precipitation. No change is detected in monthly mean air
   temperature for all stations, while small decreasing and increasing
   trends are detected for variability of annual mean air temperature in
   northeast of China and southwest of China, respectively.},
