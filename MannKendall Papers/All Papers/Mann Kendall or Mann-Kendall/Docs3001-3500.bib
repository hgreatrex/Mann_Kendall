@inproceedings{ WOS:000380489900130,
Author = {Zelenakova, Martina and Purcz, Pavol and Hlavata, Helena and Blist'an,
   Peter},
Editor = {Ulanicki, B and Kapelan, Z and Boxall, J},
Title = {Climate change in urban versus rural areas},
