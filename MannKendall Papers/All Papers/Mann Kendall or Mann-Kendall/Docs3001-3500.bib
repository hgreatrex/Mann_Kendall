@article{ WOS:000995632900001,
Author = {Rebi, Ansa and Hussain, Azfar and Hussain, Ishtiaq and Cao, Jianhua and
   Ullah, Waheed and Abbas, Haider and Ullah, Safi and Zhou, Jinxing},
Title = {Spatiotemporal Precipitation Trends and Associated Large-Scale
   Teleconnections in Northern Pakistan},
