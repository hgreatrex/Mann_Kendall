   Pandey, Jitendra; Singh, Rachna, Banaras Hindu Univ, Ctr Adv Study Bot, Ganga River Ecol Res Lab, Environm Sci Div, Varanasi 221005, Uttar Pradesh, India.},
DOI = {10.1007/s13201-015-0334-7},
