@article{ WOS:000561744100001,
Author = {Lescesen, Igor and Dolinaj, Dragan and Pantelic, Milana and Telbisz,
   Tamas and Varga, Gyorgy},
Title = {HYDROLOGICAL DROUGHT ASSESSMENT OF THE TISZA RIVER},
