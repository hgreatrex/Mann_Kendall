@article{ WOS:000566695100109,
Author = {Xiao, Liying},
Title = {Spatial-temporal Analysis of Precipitation and Drought in the Poyang
   Lake Basin during 1957-2012},
Journal = {JOURNAL OF COASTAL RESEARCH},
