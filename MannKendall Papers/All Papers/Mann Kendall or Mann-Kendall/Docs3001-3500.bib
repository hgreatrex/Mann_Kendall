@article{ WOS:000585263600014,
Author = {Tudorache, Andreea-Violeta and Mihalcea, Andreea and Tudorache,
   George-Stelian},
Title = {TRENDS IN VARIABILITY OF MAXIMUM WATER FLOW IN SMALL RIVERS BASINS IN
   ROMANIA},
