@article{ WOS:000335152500026,
Author = {Siddik, Md. Abu Zafer and Rahman, Mursheda},
Title = {Trend analysis of maximum, minimum, and average temperatures in
   Bangladesh: 1961-2008},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2014},
