Title = {Evaluation of monotonic trends for streamflow in austral Amazon, Brazil:
   a case study for the Xingu and Tapajos rivers},
Booktitle = {HYDROLOGIC NON-STATIONARITY AND EXTRAPOLATING MODELS TO PREDICT THE
   FUTURE},
