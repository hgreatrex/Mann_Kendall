@article{ WOS:000700522800011,
Author = {Yerdelen, Cehit and Abdelkader, Mohamed},
Title = {HYDROLOGICAL DATA TREND ANALYSIS WITH WAVELET TRANSFORM},
Journal = {COMPTES RENDUS DE L ACADEMIE BULGARE DES SCIENCES},
Year = {2021},
Volume = {74},
Number = {8},
Pages = {1194-1202},
Abstract = {The analysis of natural events such as floods, drought, and earthquakes
   cannot be limited to simple mathematical equations. Generally, the
   events related to climate indicators should be considered a random
   process studied by statistical and probability methods. In this study,
   the wavelet transform along with classical trend analysis tests are
   applied to investigate the possible trends in the annual mean
   streamflow. The paper presents new approach to determine turning point
   in time series trend. This data analysis approach is used to determine
   the trend pattern, periodicity and trend change point in sixteen flow
   measurement stations of the Turkish Mediterranean Sea region. It is
   identified that the region is experiencing negative trends in annual
   streamflow starting from the 7th decade of the last century. The results
   presented in this work show that a combination of signal analysis
   methods and statistical tests improves time series analysis in
   engineering studies.},
