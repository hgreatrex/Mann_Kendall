@article{ WOS:000216131100008,
Author = {Nogueira Vilanova, Mateus Ricardo},
Title = {Hydrological trends in the upper Mogi-Guacu River Region, South of Minas
   Gerais State},
Journal = {REVISTA AGROGEOAMBIENTAL},
Year = {2014},
Volume = {6},
Number = {3},
Pages = {63-70},
Month = {DEC},
Abstract = {Temporal variations of the hydrological behavior of a region may be due
   to changes of the climate or of land use and occupation in watersheds,
   and its understanding is essential for water resources planning and
   management. This paper assesses the existence of monotonic trends in
   streamflow and rainfall series in 68 the Upper Mogi-Guacu River region,
   located in the south of Minas Gerais State. For this, the nonparametric
   Mann-Kendall test was applied to representative series of mean annual
   streamflow (of two streamflow stations), and the Regional Mann-Kendall
   test was applied to series of total annual rainfall (of two rainfall
   stations), both of period between 1973 and 2006. The results
   demonstrated the existence of statistically significant trends of
   reduction of the rainfall and streamflow in the study region.},
