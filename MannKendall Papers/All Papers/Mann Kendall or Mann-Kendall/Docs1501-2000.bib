@article{ WOS:000438998600002,
Author = {Zhang, Shiliang and Chang, Tingcheng and Wang, Yuxia},
Title = {Spatial and Seasonal Precipitation Variability in Eastern Fujian Based
   on Automatic Station Observation Data},
Journal = {SENSORS AND MATERIALS},
