@article{ WOS:000395492600005,
Author = {Liu, Zhihua and Wimberly, Michael C. and Dwomoh, Francis K.},
Title = {Vegetation Dynamics in the Upper Guinean Forest Region of West Africa
   from 2001 to 2015},
