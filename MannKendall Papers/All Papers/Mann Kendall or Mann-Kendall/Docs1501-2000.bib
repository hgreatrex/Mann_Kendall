@article{ WOS:000675210000030,
Author = {Yang, Zhi-Xin and Yu, Guokuan and Zhao, Jing and Wong, Pak Kin and Wang,
   Xian-Bo},
Title = {Online Equivalent Degradation Indicator Calculation for Remaining
   Charging-Discharging Cycle Determination of Lithium-Ion Batteries},
