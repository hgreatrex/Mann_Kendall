@article{ WOS:000416104400002,
Author = {Valipour, Mohammad and Sefidkouhi, Mohammad Ali Gholami and Khoshravesh,
   Mojtaba},
Title = {Estimation and trend evaluation of reference evapotranspiration in a
   humid region},
