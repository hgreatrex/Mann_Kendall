@inproceedings{ WOS:000371420700034,
Author = {Hasan, Dk. Siti Nurul Ain Binti Pg. Ali and Ratnayake, Uditha and Shams,
   Shahriar},
Editor = {Sulaiman, HA and Othman, MA and Saat, MSM and Darsono, AM and Zoinol, M and AbdAziz, MZA and Misran, MH and Aminuddin, MMM},
Title = {Evaluation of Rainfall and Temperature Trends in Brunei Darussalam},
Booktitle = {PROGRESS IN APPLIED MATHEMATICS IN SCIENCE AND ENGINEERING PROCEEDINGS},
