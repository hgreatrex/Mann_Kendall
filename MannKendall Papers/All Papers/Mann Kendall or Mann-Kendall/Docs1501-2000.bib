@article{ WOS:000405591400001,
Author = {Chen, Huai and Zhu, Lijun and Wang, Jianzhong and Fan, Hongxia and Wang,
   Zhihuan},
Title = {Modal analysis of annual runoff volume and sediment load in the Yangtze
   river-lake system for the period 1956-2013},
Journal = {WATER SCIENCE AND TECHNOLOGY},
