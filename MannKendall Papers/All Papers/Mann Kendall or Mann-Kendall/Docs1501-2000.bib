@article{ WOS:001327898300009,
Author = {Andrade-Velazquez, Mercedes and Montero-Martinez, Martin Jose and
   Magana, M. F. Vargas},
Title = {Climatic temperature indices for the coastal resilience observatory in
   Tabasco (Dos Bocas)-LANRESC},
