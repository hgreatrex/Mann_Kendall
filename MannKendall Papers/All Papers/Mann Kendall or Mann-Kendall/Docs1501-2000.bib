@article{ WOS:000477054700060,
Author = {Ozgur, Evren and Kocak, Kasim},
Title = {Climatology of snowfall/total precipitation days over Turkey},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2019},
