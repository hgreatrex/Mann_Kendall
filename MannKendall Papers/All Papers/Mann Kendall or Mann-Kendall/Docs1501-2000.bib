@article{ WOS:000473164300026,
Author = {Tiwari, Harinarayan and Pandey, Brij Kishor},
Title = {Non-parametric characterization of long-term rainfall time series},
Journal = {METEOROLOGY AND ATMOSPHERIC PHYSICS},
