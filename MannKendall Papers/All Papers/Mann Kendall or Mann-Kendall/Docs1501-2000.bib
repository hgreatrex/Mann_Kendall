@article{ WOS:000281095500010,
Author = {Zhang, Wei and Ruan, Xiaohong and Zheng, Jinhai and Zhu, Yuliang and Wu,
   Hongxu},
Title = {Long-term change in tidal dynamics and its cause in the Pearl River
   Delta, China},
Journal = {GEOMORPHOLOGY},
