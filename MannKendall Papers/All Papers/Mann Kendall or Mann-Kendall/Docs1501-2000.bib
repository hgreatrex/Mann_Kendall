@article{ WOS:000390104200001,
Author = {Yuan, Xuefeng and Jian, Jinshi and Jiang, Gang},
Title = {Spatiotemporal Variation of Precipitation Regime in China from 1961 to
   2014 from the Standardized Precipitation Index},
Journal = {ISPRS INTERNATIONAL JOURNAL OF GEO-INFORMATION},
