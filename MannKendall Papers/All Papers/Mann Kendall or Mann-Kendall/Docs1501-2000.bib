@article{ WOS:000378840300060,
Author = {Qiao, Yinhuan and Feng, Jianfeng and Liu, Xia and Wang, Wei and Zhang,
   Pei and Zhu, Lin},
Title = {Surface water pH variations and trends in China from 2004 to 2014},
