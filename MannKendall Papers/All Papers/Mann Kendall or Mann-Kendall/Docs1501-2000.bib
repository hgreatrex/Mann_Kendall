@article{ WOS:000492319300001,
Author = {Viru, Birgit and Jaagus, Jaak},
Title = {Spatio-temporal variability and seasonal dynamics of snow cover regime
   in Estonia},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
