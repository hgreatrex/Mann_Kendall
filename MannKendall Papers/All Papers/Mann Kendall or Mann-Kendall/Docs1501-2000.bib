@article{ WOS:001189879100002,
Author = {Abdellaoui, Benyounes and Falcini, Federico and Baibai, Tarik and Hilmi,
   Karim and Ettahiri, Omar and Santoleri, Rosalia and Houssa, Rachida and
   Nhhala, Hassan and Er-Raioui, Hassan and Oukhattar, Laila},
Title = {Spatial pattern of sea surface temperature and chlorophyll-a trends in
   relation to hydrodynamic processes in the Alborán Sea},
Journal = {MEDITERRANEAN MARINE SCIENCE},
Year = {2024},
Volume = {25},
Number = {1},
Abstract = {Environmental conditions such as temperature, planktonic biomass, and
   ocean currents play an important role in the development and
   distribution of marine species. This work aims to estimate, in a high
   spatial resolution, the actual trends of sea surface temperature (SST)
   and chlorophyll -a concentration (Chl-a) and to assess the relationship
   with the local hydrodynamic conditions in the Alboran Sea. To
   investigate these objectives, time series of SST and Chl-a of satellite
   sensor data were analyzed during 20 years from January 2001 to December
   2020, using the Seasonal -Trend -Loess (STL) decomposition method and
   the Mann -Kendall seasonality test. The results, obtained with a 95\% of
   confidence, showed that the Alboran Sea basin is subject to sea surface
   warming evaluated at 0.027 +/- 0.008 degrees C per year, related to the
   warming of the Atlantic water mass, which contributes to a decrease of
   productivity evaluated at -0.0024 +/- 0.0003 mu g /l per year of Chl-a
   concentration. These trends are not homogeneous over the entire basin
   area but show a large regional variation between different parts of the
   Alboran Sea due to the hydrodynamic process of the Atlantic Jet -
   Western Alboran Gyre system (AJ-WAG), which is more active especially in
   summer/autumn seasons and contribute largely to these changes by mixing
   the waters of the Atlantic Ocean and the Mediterranean Sea.},
