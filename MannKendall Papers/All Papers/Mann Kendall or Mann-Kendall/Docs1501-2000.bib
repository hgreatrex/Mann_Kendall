@article{ WOS:000991978500002,
Author = {An, Hyowon and Ha, Kyoochul and Lee, Eunhee and Ko, Kyung-Seok},
Title = {Analysis of groundwater quality variation characteristics using
   hydrogeochemical characteristics, ANOVA and trend analysis in rural
   areas},
Journal = {JOURNAL OF THE GEOLOGICAL SOCIETY OF KOREA},
