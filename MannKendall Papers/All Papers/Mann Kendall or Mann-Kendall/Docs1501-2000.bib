@article{ WOS:000367856800007,
Author = {Mao, Tianxu and Wang, Genxu and Zhang, Tao},
Title = {Impacts of Climatic Change on Hydrological Regime in the Three-River
   Headwaters Region, China, 1960-2009},
Journal = {WATER RESOURCES MANAGEMENT},
