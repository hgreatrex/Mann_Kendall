@article{ WOS:001172548600001,
Author = {Malani, Urvashi and Yadav, Sanjaykumar M.},
Title = {Spatiotemporal assessment of rainfall variability and its
   characteristics over Rapti River Basin, India},
Journal = {WATER PRACTICE AND TECHNOLOGY},
