@article{ WOS:000239592000005,
Author = {Becker, Stefan and Gemmer, Marco and Jiang, Tong},
Title = {Spatiotemporal analysis of precipitation trends in the Yangtze River
   catchment},
Journal = {STOCHASTIC ENVIRONMENTAL RESEARCH AND RISK ASSESSMENT},
