@article{ WOS:001127177000002,
Author = {Serkendiz, Hidir and Tatli, Hasan and Kilic, Aysegul and Cetin, Mahmut
   and Sungur, Ali},
Title = {Analysis of drought intensity, frequency and trends using the spei in
   Turkey},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
