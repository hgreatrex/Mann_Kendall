@article{ WOS:000452412700017,
Author = {Ding, Yang and Li, Yi and Li, Linchao and Yao, Ning and Hu, Wei and
   Yang, Daqing and Chen, Chunyan},
Title = {Spatiotemporal variations of snow characteristics in Xinjiang, China
   over 1961-2013},
Journal = {HYDROLOGY RESEARCH},
