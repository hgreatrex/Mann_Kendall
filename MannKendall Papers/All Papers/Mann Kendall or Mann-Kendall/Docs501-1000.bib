@article{ WOS:000372169700042,
Author = {Kaldybayev, Azamat and Chen, Yaning and Issanova, Gulnura and Wang,
   Huaijun and Mahmudova, Lyazzat},
Title = {Runoff response to the glacier shrinkage in the Karatal river basin,
   Kazakhstan},
Journal = {ARABIAN JOURNAL OF GEOSCIENCES},
