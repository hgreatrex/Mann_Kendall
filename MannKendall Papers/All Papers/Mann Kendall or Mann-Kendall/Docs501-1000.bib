@article{ WOS:001052806600001,
Author = {Vousoughi, Farnaz Daneshvar},
Title = {Wavelet-based de-noising approach for trend analysis and coherency
   relation in hydrological time series (case study: Ardabil plain)},
Journal = {SUSTAINABLE WATER RESOURCES MANAGEMENT},
Year = {2023},
Volume = {9},
Number = {5},
Month = {OCT},
Abstract = {The objective of this study was to examine the impact of de-noising
   using the wavelet transform (WT) as a preprocessing method on the trend
   of rainfall and runoff time series. To achieve this objective, the study
   employed variants of the Mann-Kendall test (MK) that consider the
   effects of lag-1 autocorrelation (MK2) and long-term persistence (LTP)
   (MK3) to detect the trends of hydrological time series. Additionally,
   the study used the wavelet transform coherence (WTC) test to evaluate
   the frequency relationship between rainfall and runoff time series. In
   the second phase of the study, the signals of rainfall and runoff were
   de-noised using a threshold-based wavelet method, and the effects of
   de-noising were compared in trend analysis for three stations in the
   Ardabil plain of Iran. The MK trend tests showed a negative trend for
   all three runoff stations and one rainfall station. The MK3 test showed
   that trends in hydrologic data were underestimated. Moreover, the
   de-noised wavelet-based temporal preprocessing reduced the Z values of
   the MK tests in most cases. The WTC results showed that runoff had the
   highest coherence (0.9-1) with rainfall time series in the frequency
   bands of 4-8 and 8-16 months.},
