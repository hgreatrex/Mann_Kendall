@article{ WOS:000662242700004,
Author = {Bacevic, Nikola R. and Milentijevic, Nikola M. and Valjarevic,
   Aleksandar and Gicic, Ajsa and Kicovic, Dusan and Radakovic, Milica G.
   and Nikolic, Milena and Pantelic, Milana},
Title = {Spatiotemporal variability of air temperatures in Central Serbia from
   1949 to 2018},
Journal = {IDOJARAS},
Year = {2021},
Volume = {125},
Number = {2},
Pages = {229-253},
Month = {APR-JUN},
Abstract = {The paper presents trends for three categories of variables: average
   annual, average maximum and average minimum air temperatures. Data was
   provided by the meteorological yearbooks of the Republic
   Hydrometeorological Service of Serbia. The main goal of this paper is to
   detect possible temperature trends in Central Serbia. The trend
   equation, trend magnitude, and Mann-Kendall non-parametric test were
   used in the analysis of climate parameters. The used statistical methods
   were supplemented by GIS numerical analysis, which aimed to analyze the
   spatial distribution of isotherms from 1949 to 2018. The obtained
   results indicate that out of the 72 analyzed time series, an increase in
   air temperature is dominant in 61 time series, while 11 time series show
   no changes. The highest increase was recorded in the average maximum
   time series (4.2 degrees C), followed by an increase of 3.5 degrees C in
   average maximum air temperatures. The highest increase in the average
   annual time-series was 3.0 degrees C. The lowest increases in air
   temperature were recorded in the average minimum time series (0.1 and
   0.2 degrees C). In two average minimum time series a decrease in average
   air temperatures was identified (-0.6 and -0.4 degrees C. The
   application of GIS tools indicates the existence of interregional
   differences in the arrangement of isotherms, leaded by the orography of
   the terrain. In the spatial distribution of the analyzed variables,
   ``poles of heat{''} and ``poles of cold{''} stand out, and the influence
   of the urban heat island is evident (especially in the case of the urban
   agglomeration of Belgrade). The manifested spatial patterns of air
   temperature need to he further examined and the correlation with
   possible causes need to be determined. For these reasons, the paper
   provides a solid basis for studying the climate of this area in the
   future, as it provides insight into climate dynamics over the past
   decades.},
