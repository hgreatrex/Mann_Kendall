@article{ WOS:001090200600009,
Author = {Kubiszyn, Katarzyna},
Title = {ANALYSIS OF THE LOCAL PRECIPITATION TRENDS IN THE PODKARPACKIE AND
   LUBUSKIE VOIVODESHIPS},
