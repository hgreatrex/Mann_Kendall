Title = {Unidirectional trends in rainfall and temperature of Bangladesh},
Booktitle = {HYDROLOGY IN A CHANGING WORLD: ENVIRONMENTAL AND HUMAN DIMENSIONS},
Series = {IAHS Publication},
Year = {2014},
Volume = {363},
