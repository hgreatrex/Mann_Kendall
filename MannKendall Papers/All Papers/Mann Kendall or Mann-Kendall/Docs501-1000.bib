@article{ WOS:001133423700001,
Author = {He, Sunan and Hao, Chengyuan},
Title = {Analysis on spatial-temporal variation characteristics of climate in
   Qinling-Huaihe demarcation zone since 1961},
Journal = {ECOLOGICAL INDICATORS},
