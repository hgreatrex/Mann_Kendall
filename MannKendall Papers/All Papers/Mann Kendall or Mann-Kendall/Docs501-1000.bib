@article{ WOS:000266790400002,
Author = {Deni, Sayang Mohd and Suhaila, Jamaludin and Zin, Wan Zawiah Wan and
   Jemain, Abdul Aziz},
Title = {Trends of Wet Spells over Peninsular Malaysia during Monsoon Seasons},
Journal = {SAINS MALAYSIANA},
