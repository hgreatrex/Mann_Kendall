Author = {Milovanovic, Bosko and Schuster, Phillip and Radovanovic, Milan and
   Vakanjac, Vesna Ristic and Schneider, Christoph and Milivojevic, Milovan},
Title = {SPATIAL-TEMPORAL VARIABILITY OF AIR TEMPERATURES IN SERBIA IN THE PERIOD
   1961-2010},
