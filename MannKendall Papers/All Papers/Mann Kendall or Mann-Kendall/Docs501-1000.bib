@article{ WOS:001171879700001,
Author = {Iqbal, Jafar and Khan, Najeebullah and Shahid, Shamsuddin and Ullah,
   Safi},
Title = {Evaluation of gridded dataset in estimating extreme precipitations
   indices in Pakistan},
