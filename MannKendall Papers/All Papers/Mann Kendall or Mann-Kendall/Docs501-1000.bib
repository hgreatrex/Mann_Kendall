@article{ WOS:000451925300070,
Author = {Zelenakova, Martina and Purcz, Pavol and Pintilii, Radu Daniel and
   Blistan, Peter and Hlustik, Petr and Oravcova, Anna and Abu Hashim,
   Mohamed},
Title = {Spatio-temporal Variations in Water Quality Parameter Trends in River
   Waters},
