@inproceedings{ WOS:000392856300110,
Author = {Kalra, Ajay and Sagarika, Soumya and Ahmad, Sajjad},
Editor = {Karvazy, K and Webster, VL},
Title = {Spatial and temporal evaluation of hydroclimatic variables in the
   Colorado River Basin},
Booktitle = {WORLD ENVIRONMENTAL AND WATER RESOURCES CONGRESS 2015: FLOODS, DROUGHTS,
   AND ECOSYSTEMS},
Year = {2015},
Pages = {1118-1127},
Note = {World Environmental and Water Resources Congress, Austin, TX, MAY 17-21,
   2015},
Organization = {Amer Soc Civil Engineers, Environm and Water Resources Inst},
Abstract = {The current study provides a comprehensive analysis of long term changes
   (trend and step) for three hydroclimatic variables i.e. streamflow,
   temperature, and precipitation within the Colorado River Basin located
   in the western United States. Monthly temperature and precipitation data
   for 29 climate division and 29 naturalized streamflow gages were
   analyzed for a period of 105 years i.e., 1906-2010. The non parametric
   Mann-Kendall (MK) is used to evaluate the trends, whereas the non
   parametric Pettitt test is used to evaluate the step changes. The effect
   of short term persistence (STP) and long term persistence (LTP) on long
   term trends is also analyzed. Results from MK test showed increasing
   temperature trends for majority of the months with winter precipitation
   showing decreasing trends. Streamflow results coincided with the
   temperature and precipitation changes indicating increases in streamflow
   during autumn and winter months with decreases during the spring and
   summer months. The change point results are similar to the trend results
   and helped in distinguishing the gradual trend with abrupt shift, which
   is useful for climate change studies. STP and LTP are prominent in
   streamflow and temperature compared to precipitation. Overall, the
   results from the analysis provide a detailed outlook of the changing
   hydrology within the Colorado River Basin and can assist water managers
   in efficient planning and management of water resources.},
