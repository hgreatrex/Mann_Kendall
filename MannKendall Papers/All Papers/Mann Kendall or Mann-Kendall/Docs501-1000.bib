@article{ WOS:000414707500085,
Author = {Jung, Younghun and Shin, Ju-Young and Ahn, Hyunjun and Heo, Jun-Haeng},
Title = {The Spatial and Temporal Structure of Extreme Rainfall Trends in South
   Korea},
Journal = {WATER},
