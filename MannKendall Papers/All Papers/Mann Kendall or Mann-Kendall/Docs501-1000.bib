@inproceedings{ WOS:000277833600050,
Author = {Liu Cuishan and Zhang Jianyun and Wang Guoqing and He Ruimin},
Editor = {Shang, HQ and Luo, XX},
Title = {Study on the Runoff Trends in Huaihe River Basin during 1956 ∼ 2000},
Booktitle = {PROCEEDINGS OF THE 4TH INTERNATIONAL YELLOW RIVER FORUM ON ECOLOGICAL
   CIVILIZATION AND RIVER ETHICS, VOL I},
Year = {2010},
Pages = {377-383},
Note = {4th International Yellow River Forum, Yellow River Conservancy Commiss,
   Zhengzhou, PEOPLES R CHINA, OCT 20-23, 2009},
Organization = {EU, China River Basin Management Program; World Wide Fund Nat;
   Australian Agcy Int Dev; Int Network Basin Org; Global Water
   Partnership; Int Water Resources Assoc; Dongying Municipal Govt Shandong
   Prov; Zhengzhou Municipal Govt Henan Prov; Int Econ Tech Cooperat \&
   Exchange Ctr; Yellow River Res Assoc; Henan Multi; N China Univ Water
   Conversancy \& Elect power; China Inst Water Resources \& Hydropower
   Res; Nanjing Hydraul Res Inst; Ctr Hydro, Informat River Basin},
Abstract = {Based on the measured runoff data during 1956 similar to 2000 of 11 key
   stations on 9 rivers in Huaihe River Basin, the changing trends of
   annual runoff as well as flood season runoff and non - flood season
   runoff are analyzed with Mann - Kendall statistics testing method. The
   result indicates that the annual runoff is generally shown as a
   decreasing trend in Huaihe River Basin. There are no significant
   changing trends of annual runoff in Bantai station and Wangjiaba station
   in the upper reaches; Bengbu station and Zhongdu station on the main
   stream as well as Jiangjiaji station in tributary show the decreasing
   trends but insignificant; other stations show significantly decreasing
   trends ever since 1979. The changing trend of flood season runoff is
   similar to annual runoff; whereas the decreasing amplitude of changing
   trend of non - flood season runoff is larger than annual runoff. The
