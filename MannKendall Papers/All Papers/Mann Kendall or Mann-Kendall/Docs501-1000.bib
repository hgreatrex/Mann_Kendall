@article{ WOS:000491437600024,
Author = {Mohammad, Pir and Goswami, Ajanta},
Title = {Temperature and precipitation trend over 139 major Indian cities: An
   assessment over a century},
Journal = {MODELING EARTH SYSTEMS AND ENVIRONMENT},
