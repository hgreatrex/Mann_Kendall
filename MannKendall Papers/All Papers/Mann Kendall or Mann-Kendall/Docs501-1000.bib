@inproceedings{ WOS:000234565300030,
Author = {Wang, W and Van Gelder, PHAJM and Vrijling, JK},
Editor = {Tsagarakis, KP},
Title = {Detection of changes in streamflow series in western Europe over
   1901-2000},
Booktitle = {WATER ECONOMICS, STATISTICS AND FINANCE},
