@inproceedings{ WOS:000323014600051,
Author = {Wang, Shuai and Lei, Xiaohui and Zhang, Feng},
Editor = {Gan, B and Gan, Y and Yu, Y},
Title = {Analysis on Variation Characteristics of Precipitation Time Series in
   Weihe River Basin during the Past 55 Years},
Booktitle = {ADVANCES IN APPLIED MATERIALS AND ELECTRONICS ENGINEERING II},
Series = {Advanced Materials Research},
Year = {2013},
Volume = {684},
Pages = {246+},
Note = {2nd International Conference on Applied Materials and Electronics
   Engineering (AMEE 2013), Hong Kong, PEOPLES R CHINA, APR 19-20, 2013},
Organization = {Int Sci \& Engn Res Ctr; Int Assoc Sci \& High Technol},
Abstract = {By using linear regression, moving average, Mann-Kendall test and Morlet
   wavelet analysis method, the annual, flood seasonal and dry seasonal
   precipitation series during 1954 similar to 2008 in Weihe River basin
   are analyzed. The results indicate that: (1)Annual precipitation and
   precipitation in flood season and dry season present decreasing trends
   during the past 55 years, and the variation trends are not significant.
   (2)There is an irregular spatial distribution of mean annual
   precipitation, which shows a decreasing trend from southeast to
   northwest. (3)Annual precipitation exists principal periods of about 2,
   10, 17 and 50 years, and the phase changes of 2 and 50 years period are
   most obvious. Time scales of phase change of precipitation series in
   flood season and dry season are roughly consistent with those in annual.
   The research results will provide reference for climate analysis and
   future weather forecasting in Weihe River basin.},
