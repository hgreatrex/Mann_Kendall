@article{ WOS:001071449800001,
Author = {Di Nunno, Fabio and De Matteo, Marco and Izzo, Giovanni and Granata,
   Francesco},
Title = {A Combined Clustering and Trends Analysis Approach for Characterizing
   Reference Evapotranspiration in Veneto},
Journal = {SUSTAINABILITY},
