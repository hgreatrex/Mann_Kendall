@article{ WOS:000391236300003,
Author = {Muhammad, Nur Shazwani and Akashah, Amieroul Iefwat and Abdullah, Jazuri},
Title = {ANALYSIS OF EXTREME RAINFALL INDICES IN PENINSULAR MALAYSIA},
