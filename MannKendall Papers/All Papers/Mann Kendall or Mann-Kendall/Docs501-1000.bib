@article{ WOS:000267236800002,
Author = {Karabulut, Murat},
Title = {Precipitation Trends in Kahramanmara along with Gaziantep and Adlyaman
   during the period of 1963-2005},
Journal = {EKOLOJI},
Year = {2009},
Volume = {18},
Number = {71},
Pages = {15-24},
Abstract = {Mediterranean region is one of the sensitive regions to climate
   variation particularly to precipitation changes in Turkey. In this
   study, trends in precipitation at annual, seasonal and monthly time
   scales for the period of 1963-2005 were examined for the Kahramanmaras
   along with Gaziantep and Adlyaman stations which are located in the
   continental Mediterranean precipitation regime region of Turkey.
   Non-parametric tests (such as Mann-Kendall and Sen's Slope), linear
   regression, cumulative deviation curve techniques were used to determine
   precipitation trends. The results showed that there is no statistically
   significant any trend in the study area, despite of slight precipitation
   increase for Kahramamnaras, slight decrease for Gaziantep and Adiyaman,
   annually. The year 1996 is the wettest year for all three stations, on
   the other hand the years 1996 for Kahramanmaras, 1970 for Adiyaman and
   1973 for Gaziantep are the driest years during the study period. Winter,
   spring seasons represent slight precipitation decrease for all three
   stations during the study period. Autumn showed increasing trend for
   Kahramanmara and Gaziantep even though which is not statistically
   significant. Most of the trends for individual months depicted
   decreasing trend but all are not statistically significant.
   Precipitation patterns, however, seems to have changed during the last
   43 years, and results of seasonal trends analyses seem to indicate that
   there has been a shift in winter precipitation seasonality toward Autumn
   months except Adiyaman station.},
