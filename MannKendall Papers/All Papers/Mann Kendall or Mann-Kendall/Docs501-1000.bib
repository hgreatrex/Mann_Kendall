@article{ WOS:000528258500007,
Author = {Paul, Ranjit Kumar and Sarkar, Sandipan and Mitra, Dipankar and Panwar,
   Sanjeev and Paul, A. K. and Bhar, L. M.},
Title = {Wavelets based estimation of trend in sub-divisional rainfall in India},
Journal = {MAUSAM},
