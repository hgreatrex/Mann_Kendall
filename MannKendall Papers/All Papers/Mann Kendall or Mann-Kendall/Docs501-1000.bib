@article{ WOS:000982150800011,
Author = {Jain, Sharad K. and Xu, Chong-Yu and Zhou, Yanlai},
Title = {Change analysis of All India and regional rainfall data series at annual
   and monsoon scales},
Journal = {HYDROLOGY RESEARCH},
