@article{ WOS:001235695300001,
Author = {Shen, MoYu and Liao, Anran and Pan, Min and Chen, Zhuo and Lu, Yun and
   Tao, Yi},
Title = {Long-term water quality variations and influencing factors in Dianchi
   Lake, China - insights from 30-year data},
