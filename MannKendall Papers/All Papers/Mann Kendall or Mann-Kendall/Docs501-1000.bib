@article{ WOS:000217398100013,
Author = {Sun Xiaozhou and Feng Zhiming and Yang Yanzhao and Zhang Weike},
Title = {Climate-induced Changes in Spring Maize Water Requirement in Xiliaohe
   River Watershed},
Journal = {CHINESE JOURNAL OF POPULATION RESOURCES AND ENVIRONMENT},
