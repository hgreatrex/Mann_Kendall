@article{ WOS:000540590800012,
Author = {Du, Huihua and Wang, Yimin and Wang, Zongzhi and Liu, Kelin and Cheng,
   Liang},
Title = {Spatial and temporal characteristics of the daily precipitation
   concentration index over China from 1979 to 2015},
Journal = {HYDROLOGY RESEARCH},
