@article{ WOS:000404664400011,
Author = {Li Qingyun and Sun Yanwei and Yuan Wenlin and Lyu Subing and Wan Fang},
Title = {Streamflow responses to climate change and LUCC in a semi-arid watershed
   of Chinese Loess Plateau},
Journal = {JOURNAL OF ARID LAND},
