@article{ WOS:000475302900046,
Author = {Arfan, Muhammad and Lund, Jewell and Hassan, Daniyal and Saleem, Maaz
   and Ahmad, Aftab},
Title = {Assessment of Spatial and Temporal Flow Variability of the Indus River},
