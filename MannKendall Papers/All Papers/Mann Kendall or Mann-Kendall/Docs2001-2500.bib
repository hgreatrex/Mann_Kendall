@article{ WOS:000808548500001,
Author = {Tosunoglu, Fatih and Slater, Louise},
Title = {Decreasing flood hazard evaluated in Turkey using nonstationary models},
Journal = {RIVER RESEARCH AND APPLICATIONS},
