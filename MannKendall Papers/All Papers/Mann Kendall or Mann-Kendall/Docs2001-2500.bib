   Himesh, S. and Goroshi, Sheshakumar},
Title = {Annual and seasonal trends in actual evapotranspiration over different
   meteorological sub-divisions in India using satellite-based data},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
Year = {2023},
