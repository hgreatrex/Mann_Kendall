@article{ WOS:001262940900005,
Author = {Singh, Pardeep and Guleria, Amit and Guleria, Chandresh and Vaidya,
   Manoj K.},
Title = {Climatic variability and its impact on maize and wheat yield in Himachal
   Pradesh},
Journal = {MAUSAM},
Year = {2024},
Volume = {75},
Number = {3},
Pages = {669-678},
Month = {JUL},
Abstract = {The present study has analyzed variability in climate variables annually
   and seasonally ( kharif and rabi season), viz . rainfall and temperature
   from1985 to 2021 and found its their impact on productivity of maize and
   wheat crops at different seven locations of Himachal Pradesh. The study
   found that, except for the maximum temperature in the kharif season,
   practically all of the seven sites had significant increases in maximum
   and minimum temperatures annually and seasonally over the study period.
   Highest positive deviation was found in minimum temperature of Kangra
   district rabi season i.e ., 2.12 degrees C while lowest was found in
   kharif season i.e ., 0.50 degrees C. Pettit's homogeneity test showed
