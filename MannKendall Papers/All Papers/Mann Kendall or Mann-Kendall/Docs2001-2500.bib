@article{ WOS:000868457100001,
Author = {Thakur, Kirti and Kumar, Harish and Snehmani},
Title = {Framework for variability analysis of seasonal meteorological data},
Journal = {JOURNAL OF EARTH SYSTEM SCIENCE},
Year = {2022},
Volume = {131},
Number = {4},
Month = {OCT 16},
Abstract = {Meteorological data-trend analysis is widely used as statistical method
   to access long-term changes in time-series of meteorological variables
   like precipitation, temperature, and evaporation. Understanding
   meteorological pattern of natural hazards like landslides, avalanches,
   and tsunami, is tough. Statistical computing, modelling and forecasting
   meteorological data are key instruments for studying hidden patterns.
   Long-term time-series of meteorological observations in mountainous
   terrain are rare in Indian Himalayan. This study proposes a framework
   including data selection, pre-processing, and knowledge discovery for
   Manali region in Pir Panjal Range. Pre-processing ensures data quality
   along with descriptive and inferential statistical methods used to
   understand data. Both parametric and non-parametric models are developed
   to investigate variations in 18 parameters. Trend magnitude for these
   parameters are determined and compared for three field observatories:
   Bahang, Solang, and Dhundi. An increasing seasonal trend was found for
   temperature (maximum, dry and wet), total water equivalent from
   beginning of season, and snow penetration for station data. Across
   observatories, many variables show no significant trend due to less and
   fluctuating data. Study also investigate significant relationship
   between features and the most correlated meteorological variables. It is
   useful for understanding changing meteorological status in the region
   for data prediction and interpolation. Correlation results can help in
   building better statistical regional models.},
