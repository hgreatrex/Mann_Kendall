@article{ WOS:001131428500001,
Author = {Zhang, Jinghan and Ju, Xiaopei and Wang, Sheng and Li, Fengping and
   Zhao, Ziyue},
Title = {Variations in Precipitation at the Shimantan Reservoir, China},
Journal = {WATER},
