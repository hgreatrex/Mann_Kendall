@article{ WOS:000385256300010,
Author = {Liuzzo, Lorena and Bono, Enrico and Sammartano, Vincenzo and Freni,
   Gabriele},
Title = {Analysis of spatial and temporal rainfall trends in Sicily during the
   1921-2012 period},
Journal = {THEORETICAL AND APPLIED CLIMATOLOGY},
