@inproceedings{ WOS:000238978701090,
Author = {Sarma, Ashwin and Tufts, Donald W.},
Title = {Improving CFAR detection through adaptive determination of reference
   window extents},
Booktitle = {OCEANS 2005, VOLS 1-3},
Series = {OCEANS-IEEE},
Year = {2005},
Pages = {1501-1507},
Note = {Oceans 2005 Conference, Washington, DC, SEP 17-23, 2005},
Organization = {IEEE},
Abstract = {Adaptive Constant False Alarm Rate (CFAR) detectors operating on
   envelope-squared matched-filtered beam data generally employ two equal
   fixed-length windows, straddling the test cell, to collect reference
   samples. Our objective is to prevent the consideration of reference
   sample sets that do not behave even approximately i.i.d. A procedure to
   adaptively determine window sizes is presented. The nested hypothesis
   testing procedure is based on the principles of Quality Control and
   utilizes the Mann-Kendall rank test for randomness. Potential benefits
   include improved false alarm control, increased detection probability
   and closer tracking of extended clutter edges.},
