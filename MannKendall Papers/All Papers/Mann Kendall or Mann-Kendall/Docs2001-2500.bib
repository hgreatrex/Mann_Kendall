@article{ WOS:001266584400001,
Author = {Liu, Ziying and Shen, Zhenzhong and Chang, Wenqian and Zhao, Yingxiu},
Title = {Spatiotemporal Evolution of Green Finance and High-Quality Economic
   Development: Evidence from China},
Journal = {SUSTAINABILITY},
Year = {2024},
