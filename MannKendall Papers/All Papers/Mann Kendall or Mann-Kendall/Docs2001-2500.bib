@article{ WOS:000393415100003,
Author = {Maleski, Jerome J. and Martinez, Christopher J.},
Title = {Historical trends in precipitation, temperature and drought in the
   Alabama-Coosa-Tallapoosa and Apalachicola-Chattahoochee-Flint river
   basins},
Journal = {INTERNATIONAL JOURNAL OF CLIMATOLOGY},
Year = {2017},
Volume = {37},
