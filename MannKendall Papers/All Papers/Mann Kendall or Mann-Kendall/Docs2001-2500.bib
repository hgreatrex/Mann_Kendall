@article{ WOS:000436600400005,
Author = {Jones, James R. and Schwartz, John S. and Ellis, Kelsey N. and Hathaway,
   Jon M. and Jawdy, Curtis M.},
Title = {Temporal variability of precipitation in the Upper Tennessee Valley},
