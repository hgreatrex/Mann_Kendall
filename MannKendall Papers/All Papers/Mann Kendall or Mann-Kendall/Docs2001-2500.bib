@article{ WOS:000341170200010,
Author = {Lu Hong and Chen Si-rong and Guo Yuan and He Hui and Xu Sheng-xuan},
Title = {SPATIO-TEMPORAL VARIATION CHARACTERISTICS OF EXTREMELY HEAVY
   PRECIPITATION FREQUENCY OVER SOUTH CHINA IN THE LAST 50 YEARS},
Journal = {JOURNAL OF TROPICAL METEOROLOGY},
