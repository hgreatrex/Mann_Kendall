@article{ WOS:000548358800029,
Author = {Abdulkareem, Iqbal H.},
Title = {TEMPERATURE AND PRECIPITATIONS TRENDS FOR HILLA CITY USING NOAA/ESRL
   GRIDDED HIGH-RESOLUTION DATA},
Journal = {JOURNAL OF ENGINEERING SCIENCE AND TECHNOLOGY},
Year = {2020},
Volume = {15},
