@article{ WOS:000832212600001,
Author = {Aamir, Erum and Khan, Aamir and Tariq, Muhammad Abubakar},
Title = {The Influence of Teleconnections on the Precipitation in Baluchistan},
Journal = {ATMOSPHERE},
Year = {2022},
