@article{ WOS:000533454500009,
Author = {Wang, Dong-fei and Xu, Hao-di and Wang, Lei and Wu, Xian and Sun,
   Hong-yue},
Title = {Statistical analyses of the effect of a drainage tunnel on landslide
   hydrogeological characteristics},
Journal = {HYDROLOGICAL PROCESSES},
