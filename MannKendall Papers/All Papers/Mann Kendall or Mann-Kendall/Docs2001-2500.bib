Title = {ASSESSMENT OF LAND DEGRADATION USING TIME SERIES TRENDS ANALYSIS OF
   VEGETATION INDICTORS IN BEIJING-TIANJIN DUST AND SANDSTORM SOURCE REGION},
Booktitle = {2014 IEEE INTERNATIONAL GEOSCIENCE AND REMOTE SENSING SYMPOSIUM (IGARSS)},
Series = {IEEE International Symposium on Geoscience and Remote Sensing IGARSS},
Year = {2014},
