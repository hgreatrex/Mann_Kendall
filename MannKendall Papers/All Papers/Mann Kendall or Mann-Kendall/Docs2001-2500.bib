@article{ WOS:000386578200026,
Author = {Trabert, Andreas and Opp, Christian},
Title = {Long-term trends in flood discharges of the Ulster and Upper Fulda
   (Germany): a statistical review},
Journal = {ENVIRONMENTAL EARTH SCIENCES},
