@article{ WOS:001205143800001,
Author = {Modi, Prakhar and Majhi, Kriti Kiran and Gogineni, Abhilash},
Title = {Examining the spatial and temporal vulnerability to drought in Odisha,
   India},
Journal = {WATER PRACTICE AND TECHNOLOGY},
Year = {2024},
