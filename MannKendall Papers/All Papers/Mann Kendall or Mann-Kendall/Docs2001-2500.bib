@article{ WOS:000337996000004,
Author = {Sawyer, P. S. and Stephen, H.},
Title = {The Big Pine Creek watershed and climate change: A trend analysis of
   Landsat surface reflectance and PRISM datasets over the last 3 decades},
Journal = {ADVANCES IN SPACE RESEARCH},
