@article{ WOS:001137610200019,
Author = {Singh, Neeti and Tomar, C. S. and Kumar, Gajendra and Arun, S. H. and
   Sankhala, Dinesh},
Title = {Temporal variations of rainfall over Konkan \& Goa during 1901-2020},
