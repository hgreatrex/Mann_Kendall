@article{ WOS:000309433600019,
Author = {Scannapieco, D. and Naddeo, V. and Zarra, T. and Belgiorno, V.},
Title = {River water quality assessment: A comparison of binary- and fuzzy
   logic-based approaches},
Journal = {ECOLOGICAL ENGINEERING},
Year = {2012},
