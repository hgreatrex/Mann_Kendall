@article{ WOS:001176423800004,
Author = {Morovati, M. and Karami, P.},
Title = {Trend analysis of temperature and vegetation cover in the distribution
   range of <i>Apis florea</i> in Iran},
Journal = {INTERNATIONAL JOURNAL OF ENVIRONMENTAL SCIENCE AND TECHNOLOGY},
