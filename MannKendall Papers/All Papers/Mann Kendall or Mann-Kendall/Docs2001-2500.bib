@article{ WOS:000274438200005,
Author = {Zhao, Guangju and Hoermann, Georg and Fohrer, Nicola and Zhang, Zengxin
   and Zhai, Jianqing},
Title = {Streamflow Trends and Climate Variability Impacts in Poyang Lake Basin,
   China},
Journal = {WATER RESOURCES MANAGEMENT},
