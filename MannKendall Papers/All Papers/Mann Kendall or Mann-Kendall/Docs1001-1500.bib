@article{ WOS:000464296100037,
Author = {Vazifehkhah, Saeed and Kahya, Ercan},
Title = {Hydrological and agricultural droughts assessment in a semi-arid basin:
   Inspecting the teleconnections of climate indices on a catchment scale},
