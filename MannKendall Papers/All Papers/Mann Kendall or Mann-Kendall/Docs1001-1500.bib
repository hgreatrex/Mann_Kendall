@article{ WOS:000267202000011,
Author = {Zhang, Q. and Xu, C. -Y. and Becker, S. and Zhang, Z. X. and Chen, Y. D.
   and Coulibaly, M.},
Title = {Trends and abrupt changes of precipitation maxima in the Pearl River
   basin, China},
Journal = {ATMOSPHERIC SCIENCE LETTERS},
