@article{ WOS:000473511200018,
Author = {Huang, Dan and Liu, Zaibin},
Title = {SPATIOTEMPORAL DISTRIBUTION AND PERIODICITY OF RAINFALL IN A TYPICAL
   ALLUVIAL PLAIN OF CHINA},
Journal = {ENVIRONMENTAL ENGINEERING AND MANAGEMENT JOURNAL},
