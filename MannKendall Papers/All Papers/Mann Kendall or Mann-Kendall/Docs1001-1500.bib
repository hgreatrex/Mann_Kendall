@article{ WOS:000871557500005,
Author = {Bacevic, Nikola R. and Milentijevic, Nikola and Valjarevic, Aleksandar
   and Nikolic, Milena and Stevanovic, Vladica and Kikovic, Dusan and
   Radakovic, Milica G. and Papic, Dragan and Markovic, Slobodan B.},
Title = {The analysis of annual and seasonal surface air temperature trends of
   southern and southeastern Bosnia and Herzegovina from 1961 to 2017},
Journal = {IDOJARAS},
Year = {2022},
Volume = {126},
