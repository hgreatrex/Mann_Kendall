@article{ WOS:000675914500009,
Author = {Yan, Dongfei and Jiang, Rengui and Xie, Jiancang and Zhao, Yong and Zhu,
   Jiwei and Liang, Jichao},
Title = {Characteristics and prediction of extreme drought event using LSTM model
   in Wei River Basin},
Journal = {TERRESTRIAL ATMOSPHERIC AND OCEANIC SCIENCES},
Year = {2021},
Volume = {32},
