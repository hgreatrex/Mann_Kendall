@article{ WOS:001256157700001,
Author = {Agbiji, Ndifon M. and Agunwamba, Jonah C. and Eshiet, Kenneth Imo-Imo
   Israel},
Title = {Trend Analysis of Climatic Variables in the Cross River Basin, Nigeria},
Journal = {GEOSCIENCES},
