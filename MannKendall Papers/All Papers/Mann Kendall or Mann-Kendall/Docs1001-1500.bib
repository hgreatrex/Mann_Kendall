@article{ WOS:000447254700010,
Author = {Lu, Xiaorong and Zhuang, Yanhua and Wang, Xuelei and Yang, Qing},
Title = {Assessment of Streamflow Change in Middle-Lower Reaches of the Hanjiang
   River},
