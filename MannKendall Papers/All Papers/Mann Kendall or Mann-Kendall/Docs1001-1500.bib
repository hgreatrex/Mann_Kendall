@article{ WOS:000329287800010,
Author = {Birsan, Marius-Victor and Dumitrescu, Alexandru},
Title = {Snow variability in Romania in connection to large-scale atmospheric
   circulation},
Journal = {INTERNATIONAL JOURNAL OF CLIMATOLOGY},
