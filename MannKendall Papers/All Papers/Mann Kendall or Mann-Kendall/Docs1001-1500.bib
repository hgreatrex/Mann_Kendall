@article{ WOS:000550356000001,
Author = {Tobin, Kenneth J. and Torres, Roberto and Bennett, Marvin E. and Dong,
   Jianzhi and Crow, Wade T.},
Title = {Long-Term Trends in Root-Zone Soil Moisture across CONUS Connected to
   ENSO},
Journal = {REMOTE SENSING},
