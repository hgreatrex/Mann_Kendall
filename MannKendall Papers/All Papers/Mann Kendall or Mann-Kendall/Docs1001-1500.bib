@article{ WOS:000429573600002,
Author = {Khan, Muhammad Imran and Liu, Dong and Fu, Qiang and Faiz, Muhammad
   Abrar},
Title = {Detecting the persistence of drying trends under changing climate
   conditions using four meteorological drought indices},
Journal = {METEOROLOGICAL APPLICATIONS},
