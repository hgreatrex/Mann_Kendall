@article{ WOS:000493525500392,
Author = {Zeng, Zhaoqi and Wu, Wenxiang and Li, Zhaolei and Zhou, Yang and Huang,
   Han},
Title = {Quantitative Assessment of Agricultural Drought Risk in Southeast Gansu
   Province, Northwest China},
Journal = {SUSTAINABILITY},
Year = {2019},
Volume = {11},
Number = {19},
Month = {OCT 1},
Abstract = {The sector that suffers the most directly from drought is agriculture,
   and drought is expected to become more serious in the context of global
   warming. Effectively evaluating the agricultural drought risk is
   fundamental to establishing present water resource management and
   guaranteeing food security. Therefore, southeast Gansu province was
   selected for this study as a typical region for which to analyze drought
   trends and assess agricultural drought risk. Trends for the period from
   1967 to 2018 were analyzed at 21 locations by using the Mann-Kendall
   method, and drought was quantified by using the Standardized
   Precipitation Evapotranspiration Index (SPEI). Spatial characteristics
   of the agricultural drought risk that emphasized the combined role of
   hazards and vulnerability were investigated at a 1-km grid scale.
