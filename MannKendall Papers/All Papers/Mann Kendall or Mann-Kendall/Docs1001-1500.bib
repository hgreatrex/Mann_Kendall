@article{ WOS:000391558100003,
Author = {Yan, Guixia and Li, Denghua},
Title = {Changes of pan evaporation in the middle and lower yangtze river of
   China},
Journal = {AGRO FOOD INDUSTRY HI-TECH},
Year = {2016},
Volume = {27},
Number = {6},
Pages = {16-21},
Month = {NOV-DEC},
Abstract = {Change of pan evaporation will alter hydrological cycle in a region and
   it would be of great significance for water resources planning,
   irrigation control and agricultural production. To investigate temporal
   variations in pan evaporation and the associated changes in air
   temperatures (Tmean, Tmax and Tmin), precipitation, relative humidity,
   sunshine duration and wind speed, annual climate dataof 68 stations at
   the middle and lower Yangtze River of China for the period 1961-2006
   were identified using the Mann-Kendall test and the linear regression.
   Meanwhile, correlation analysis and the Set Pair Analysis Method were
   used to reveal the factors affecting the change of pan evaporation. The
   results showed that pan evaporation presented a decreasing trend with
   1980 being the turning year and significantly decreasing trend was
   observed in 44\% of the stations at the 95\% confidence levels.
   Likewise, decreasing trendswere found for other meteorological factors
   except temperature and precipitation for the past 46 years, but only
   sunshine duration and wind speed had the similar variation trend and the
   turning point to pan evaporation. The decrease in pan evaporation may be
   caused by the reduction of sunshine duration and wind speed, and
   evaporation was mainly controlled by energy condition especially before
   1980. After 1980, reducedrelative humidity could inhibit pan evaporation
   to decrease, so evaporation was also somewhat controlled by water
   condition.},
