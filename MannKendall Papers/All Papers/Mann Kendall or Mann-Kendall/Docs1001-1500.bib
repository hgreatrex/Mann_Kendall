@article{ WOS:000718790800005,
Author = {El Kasri, Jada and Lahmili, Abdelaziz and Soussi, Halima and Jaouda,
   Imane and Bentaher, Maha},
Title = {Trend Analysis of Meteorological Variables: Rainfall and Temperature},
Journal = {CIVIL ENGINEERING JOURNAL-TEHRAN},
