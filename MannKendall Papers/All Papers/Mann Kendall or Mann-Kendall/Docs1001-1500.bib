Title = {Spatio-temporal change characteristics of pan evaporation and possible
   climate factors in the Gongga Mountain under changing environment},
Booktitle = {HYDROLOGICAL CYCLE AND WATER RESOURCES SUSTAINABILITY IN CHANGING
   ENVIRONMENTS},
Series = {IAHS Publication},
Year = {2011},
Volume = {350},
Pages = {2+},
