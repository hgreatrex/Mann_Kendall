@article{ WOS:000407097600006,
Author = {Wdowikowski, Marcin and Kazmierczak, Bartosz and Ledvinka, Ondrej},
Title = {Maximum daily rainfall analysis at selected meteorological stations in
   the upper Lusatian Neisse River basin},
Journal = {METEOROLOGY HYDROLOGY AND WATER MANAGEMENT-RESEARCH AND OPERATIONAL
   APPLICATIONS},
