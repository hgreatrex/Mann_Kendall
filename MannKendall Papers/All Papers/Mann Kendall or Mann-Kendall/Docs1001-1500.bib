@article{ WOS:000451164000023,
Author = {Umar, Da'u Abba and Ramli, Mohammad Firuz and Aris, Ahmad Zaharin and
   Jamil, Nor Rohaizah and Abdulkareem, Jabir Haruna},
Title = {Runoff irregularities, trends, and variations in tropical semi-arid
   river catchment},
