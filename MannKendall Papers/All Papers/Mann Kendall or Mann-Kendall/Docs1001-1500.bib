@article{ WOS:000957412400001,
Author = {Kgaphola, Motsoko Juniet and Ramoelo, Abel and Odindi, John and Kahinda,
   Jean-Marc Mwenge and Seetal, Ashwin},
Title = {Apportioning Human-Induced and Climate-Induced Land Degradation: A Case
   of the Greater Sekhukhune District Municipality},
