@article{ WOS:001223114100001,
Author = {Khalil, Alamgir},
Title = {Innovative polygon trend analysis for the monthly rainfall in the Mae
   Klong River Basin, Thailand},
Journal = {WATER PRACTICE AND TECHNOLOGY},
