@article{ WOS:000328088200004,
Author = {Wang, Yanfang and Shen, Yanjun and Chen, Yaning and Guo, Ying},
Title = {Vegetation dynamics and their response to hydroclimatic factors in the
   Tarim River Basin, China},
Journal = {ECOHYDROLOGY},
