@article{ WOS:000814556300003,
Author = {Sindhu, Dagam and Singh, R. B. and Singh, Umesh and Sayyed, Mujahida and
   Mishra, Pradeep},
Title = {STATISTICAL ANALYSIS OF RAINFALL PATTERNS IN CHHATARPUR DISTRICT OF
   MADHYA PRADESH},
