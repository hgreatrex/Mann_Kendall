@article{ WOS:000834930100003,
Author = {Tang, Chaoli and Hao, Dewei and Wei, Yuanyuan and Zhao, Fengmei and Lin,
   Han and Wu, Xin},
Title = {Analysis of Influencing Factors of SST in Tropical West Indian Ocean
   Based on COBE Satellite Data},
