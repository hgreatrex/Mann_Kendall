@article{ WOS:000330318900036,
Author = {Yi, Kunpeng and Tani, Hiroshi and Zhang, Jiquan and Guo, Meng and Wang,
   Xiufeng and Zhong, Guosheng},
Title = {Long-Term Satellite Detection of Post-Fire Vegetation Trends in Boreal
   Forests of China},
Journal = {REMOTE SENSING},
