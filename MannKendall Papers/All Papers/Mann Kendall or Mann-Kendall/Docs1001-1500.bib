@article{ WOS:000347582300031,
Author = {Shan, Nan and Shi, Zhongjie and Yang, Xiaohui and Gao, Jixi and Cai,
   Dawei},
Title = {Spatiotemporal trends of reference evapotranspiration and its driving
   factors in the Beijing-Tianjin Sand Source Control Project Region, China},
