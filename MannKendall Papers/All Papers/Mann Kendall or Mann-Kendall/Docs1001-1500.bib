@article{ WOS:000928909400001,
Author = {Mamgain, Shailja and Roy, Arijit and Karnatak, Harish Chandra and
   Chauhan, Prakash},
Title = {Satellite-based long-term spatiotemporal trends of wildfire in the
   Himalayan vegetation},
