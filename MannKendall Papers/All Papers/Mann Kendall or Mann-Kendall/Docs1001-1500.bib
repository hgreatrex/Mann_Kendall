   Turkkan, Gokcen Eryilmaz, Balikesir Univ, Dept Civil Engn, TR-10600 Balikesir, Turkey.},
DOI = {10.1007/s40996-022-00869-y},
EarlyAccessDate = {JUN 2022},
ISSN = {2228-6160},
EISSN = {2364-1843},
