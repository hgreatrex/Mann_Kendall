@article{ WOS:000514574600003,
Author = {Galkate, Ravi Venkatrao and Yadav, Shalini and Jaiswal, Rahul Kumar and
   Yadava, Ram Narayan},
Title = {Trend analysis of river flow and groundwater level for Shipra river
   basin in India},
Journal = {ARABIAN JOURNAL OF GEOSCIENCES},
Year = {2020},
Volume = {13},
Number = {3},
Month = {JAN 21},
Note = {International Conference on Water, Environment, Energy and Society
   (ICWEES), TUNISIA, MAY 08-11, 2018},
Organization = {Inst Reg Arides; AISECT Univ; Texas A \& M Univ},
Abstract = {The declining baseflow and drying up of river during the lean period are
   important issues in major rivers systems in India. In recent times, more
   focus has been given to formulate an action plan for river rejuvenation.
   The present study aims at the assessment of declining flow regime in
   Shipra river in the Madhya Pradesh state of India. Long-term daily
   runoff data recorded at the Ujjain gauge discharge site on Shipra river
   was analyzed to identify trends in dependable flows using a
   non-parametric Mann-Kendall test. The declining trend for dependable
   river flow at various probability levels was observed at Ujjain during
   the beginning of monsoon (month of July) and at the end of monsoon
   (months of September and October). However, a rising trend was observed
   during the mid-monsoon season (month of August). The trend analysis of
   pre-monsoon and post-monsoon groundwater level data indicated falling
   groundwater level trend in the entire Shipra basin. Significant
   declining trends in groundwater levels were observed in the middle reach
   area of the basin where important cities like Indore and Ujjain are
   located. Wigley and Jones's index was used to judge the rising or
   declining flow regime of Shipra river. Average annual rainfall, as well
   as river runoff, has decreased substantially during the last two
   decades. The trend analysis of flow and groundwater level data of the
   Shipra river basin and information derived using Wigley and Jones index
   will be useful to formulate strategies for the river rejuvenation
   planning.},
