@article{ WOS:000727776300006,
Author = {Ahmad, Sarfaraz and Abdullah, Muhammad and Rehman, Ataur},
Title = {Temporal and spatial trends of groundwater depth level in Ganga -Yamuna
   Doab, central Ganga basin, India},
Journal = {JOURNAL OF INDIAN GEOPHYSICAL UNION},
Year = {2021},
Volume = {25},
Number = {3},
Pages = {79-89},
Month = {MAY},
Abstract = {Spatial distribution of the groundwater depth level trends were analysed
   in the Doab region of the Ganga-Yamuna River using the Mann Kendall and
   Sen's slope test for the period 2001-2019. Fluctuating groundwater level
   trends were observed all over the study region and most of the wells in
   the region decreased in groundwater depth level. It was found that
   declining trends were prevalent on the left bank of the Yamuna river.
   However, the rising trends were observed mostly near the canals in the
   central part of the Doab. The average rate of decline was 0.17 m/yr. in
   pre-monsoon and 0.19 m/yr in the post-monsoon. Higher rate of decline in
   post-monsoon than in pre-monsoon suggests overall decrease in recharge
   area. An increase in agricultural area show positive relationship with
   declining trends of groundwater depth level. It is inferred that
   continuous new addition of agriculture area is due to conversion of the
   groundwater recharge areas such as riparian zone of rivers, ponds,
   natural community grazing ground and forest. The deciphered patterns are
   cause of concern, and provide a scientific foundation for groundwater
   development and management in the region for long-term water
   consumption.},
