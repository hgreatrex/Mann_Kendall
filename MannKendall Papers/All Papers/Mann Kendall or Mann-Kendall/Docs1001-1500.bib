@article{ WOS:000946039000001,
Author = {Waikhom, Sahita I. and Yadav, Vipin Kumar and Chadee, Aaron Anil and
   Varma, Vipul},
Title = {Variability in trends of streamflow and precipitation in the Narmada
   River Basin over the last four decades},
