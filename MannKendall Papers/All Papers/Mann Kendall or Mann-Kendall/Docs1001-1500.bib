@article{ WOS:000313657100008,
Author = {Xu, Jianhua and Chen, Yaning and Li, Weihong and Nie, Qin and Hong,
   Yulian and Yang, Yang},
Title = {The nonlinear hydro-climatic process in the Yarkand River, northwestern
   China},
Journal = {STOCHASTIC ENVIRONMENTAL RESEARCH AND RISK ASSESSMENT},
