@article{ WOS:001323399400001,
Author = {Mloza Banda, Medrina Linda and Cornelis, Wim and Mloza Banda, Henry R.},
Title = {Seasonal, Decadal, and El Niño-Southern Oscillation-Related Trends and
   Anomalies in Rainfall and Dry Spells during the Agricultural Season in
   Central Malawi},
Journal = {GEOGRAPHIES},
